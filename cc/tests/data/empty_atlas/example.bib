@article{Howk2017,
abstract = {We present a deep search for HI 21-cm emission from the gaseous halo of Messier 31 as part of Project AMIGA, a large program Hubble Space Telescope program to study the circumgalactic medium of the Andromeda galaxy. Our observations with the Robert C. Byrd Green Bank Telesope target sight lines to 48 background AGNs, more than half of which have been observed in the ultraviolet with the Cosmic Origins Spectrograph, with impact parameters {\$}25 \backslashlesssim \backslashrho \backslashlesssim 330{\$} kpc ({\$}0.1 \backslashlesssim \backslashrho / R{\_}{\{}\backslashrm vir{\}} \backslashlesssim 1.1{\$}). We do not detect any 21-cm emission toward these AGNs to limits of {\$}N({\{}\backslashrm HI{\}}) \backslashapprox 4 \backslashtimes10{\^{}}{\{}17{\}}{\$} cm{\$}{\^{}}{\{}-2{\}}{\$} ({\$}5\backslashsigma{\$}, per 2 kpc diameter beam). This column density corresponds to an optical depth of {\$}\backslashsim2.5{\$} at the Lyman limit, thus our observations overlap with absorption line studies of Lyman limit systems at higher redshift. Our non-detections place a limit on the covering factor of such optically-thick gas around M31 to {\$}f{\_}c {\textless} 0.051{\$} (at 90$\backslash${\%} confidence) for {\$}\backslashrho \backslashleq R{\_}{\{}\backslashrm vir{\}}{\$}. While individual clouds have previously been found in the region between M31 and M33, the covering factor of strongly optically-thick gas is quite small. Our upper limits on the covering factor are consistent with expectations from recent cosmological "zoom" simulations. Recent COS-Halos ultraviolet measurements of $\backslash$HI$\backslash$ absorption about an ensemble of galaxies at {\$}z \backslashapprox 0.2{\$} show significantly higher covering factors within {\$}\backslashrho \backslashlesssim 0.5 R{\_}{\{}\backslashrm vir{\}}{\$} at the same {\$}N({\{}\backslashrm H I{\}}){\$}, although the metal ion-to-H I ratios appear to be consistent with those seen in M31.},
annote = {Present part of project [AMIGA], large [[HST] survey] of the [CGM] of [M31], probing at impact parameters 25-330 kpc.
Detect no [21-cm emission] down to [[H I] column]s of 4e17 cm{\^{}}-2.
The [covering fraction] of 4e17 cm{\^{}}-2 [[H I] column] [CGM] gas is {\textless}{\~{}}0.051 around [M31], consistent with [zoom-in] [cosmological simulations] and significantly lower than [COS-Halos] results.

