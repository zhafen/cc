@article{VandeVoort2016,
  title = {The Impact of Stellar Feedback on Hot Gas in Galaxy Haloes: The {{Sunyaev}}–{{Zel}}'dovich Effect and Soft {{X}}-Ray Emission},
  author = {van de Voort, Freeke and Quataert, Eliot and Hopkins, Philip F. and Faucher-Giguère, Claude-André and Feldmann, Robert and Kereš, Dušan and Chan, T. K. and Hafen, Zachary},
  date = {2016-12-21},
  journaltitle = {Monthly Notices of the Royal Astronomical Society},
  volume = {463},
  pages = {4533--4544},
  issn = {0035-8711},
  doi = {10.1093/mnras/stw2322},
  url = {http://arxiv.org/abs/1604.01397},
  urldate = {2016-04-07},
  abstract = {The thermal Sunyaev-Zel'dovich (SZ) effect and soft X-ray emission are routinely observed around massive galaxies and in galaxy groups and clusters. We study these observational diagnostics of galaxy haloes for a suite of cosmological `zoom-in' simulations from the `Feedback In Realistic Environments' project, which spans a large range in halo mass 10\^10-10\^13 Msun). We explore the effect of stellar feedback on the hot gas observables. The properties of our simulated groups, such as baryon fractions, SZ flux, and X-ray luminosities (L\_X), are broadly consistent with existing observations, even though feedback from active galactic nuclei is not included. We make predictions for future observations of lower-mass objects for both SZ and diffuse X-ray measurements, finding that they are not just scaled-down versions of massive galaxies, but more strongly affected by galactic winds driven by star formation. Low-mass haloes ({$<\sptilde$}10\^11 Msun) retain a low fraction of their baryons, which results in a strong suppression of the SZ signal. Our simulations therefore predict a scaling with halo mass that is steeper than self-similar for haloes less massive than 10\^13 Msun. For halo masses {$<\sptilde$}10\^12 Msun, L\_X is time-variable and correlated primarily with the star formation rate (SFR). For these objects, the diffuse X-ray emission is powered mostly by galactic winds and the gas dominating the X-ray emission is flowing out with radial velocities close to the halo's circular velocity. For halo masses {$>\sptilde$}10\^13 Msun, on the other hand, L\_X is much less variable and not correlated with the SFR, because the emission originates from the quasi-hydrostatic, virialized halo gas.},
  archiveprefix = {arXiv},
  eprint = {1604.01397},
  eprinttype = {arxiv},
  file = {/Users/zhafen/Zotero/storage/AZAEF8LT/van de Voort et al. - 2016 - The impact of stellar feedback on hot gas in galaxy haloes the Sunyaev–Zel'dovich effect and soft X-ray e.pdf},
  keywords = {FIRE paper,massive galaxies,soft X-ray,Sunyaev-Zel'dovich,theory: simulation},
  note = {Studies the [observable properties] of the [CGM] for the [thermal [Sunyaev-Zel'dovich] effect] and [soft X-ray emission] using the [FIRE] simulations, including the effect of [stellar [feedback]].

The properties of [FIRE] halos are broadly consistent with observations of [baryon fraction], [[SZ] flux], and [X-ray luminosity], despite the lack of [black hole [feedback]].

Produce predictions of [future observations] for both [SZ] and [X-ray] observations and find that [{$<\sptilde$}1e12 Msun halos] are strongly affected by [galactic [wind]].

[{$<\sptilde$}1e11 Msun halos] have low [baryon fraction]s, and therefore a weak [SZ] signal.

[{$<\sptilde$}1e12 Msun halos] have time-variable [X-ray luminosity] correlated with the [SFR] and powered by [galactic [wind]].

[{$>\sptilde$}1e13 Msun halos] have steady [X-ray luminosity] produced by the [quasi-[hydrostatic]], [virialized], [hot halo].

