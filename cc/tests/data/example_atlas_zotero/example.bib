@article{hafen2020,
  title = {The Fates of the Circumgalactic Medium in the {{FIRE}} Simulations},
  author = {Hafen, Zachary and Faucher-Giguère, Claude-André and Anglés-Alcázar, Daniel and Stern, Jonathan and Kereš, Dušan and Esmerian, Clarke and Wetzel, Andrew and El-Badry, Kareem and Chan, T. K. and Murray, Norman},
  date = {2020-05-21},
  journaltitle = {Monthly Notices of the Royal Astronomical Society},
  volume = {494},
  pages = {3581--3595},
  issn = {0035-8711},
  doi = {10.1093/mnras/staa902},
  url = {http://arxiv.org/abs/1910.01123},
  urldate = {2019-10-05},
  abstract = {We analyse the different fates of the circumgalactic medium (CGM) in FIRE-2 cosmological simulations, focusing on the redshifts z = 0.25 and 2 representative of recent surveys. Our analysis includes 21 zoom-in simulations covering the halo mass range \$M\_\{\textbackslash rm h\}(z=0) \textbackslash sim 10\^\{10\} \textbackslash!-\textbackslash! 10\^\{12\} \textbackslash rm \{\textbackslash,M\}\_\textbackslash odot\$. We analyse both where the gas ends up after first leaving the CGM (its ‘proximate’ fate) and its location at z = 0 (its ‘ultimate’ fate). Of the CGM at z = 2, about half is found in the ISM or stars of the central galaxy by z = 0 in Mh(z = 2) ∼ 5 × 1011 M⊙ haloes, but most of the CGM in lower mass haloes ends up in the intergalactic medium (IGM). This is so even though most of the CGM in Mh(z = 2) ∼ 5 × 1010 M⊙ haloes first accretes on to the central galaxy before being ejected into the IGM. On the other hand, most of the CGM mass at z = 0.25 remains in the CGM by z = 0 at all halo masses analysed. Of the CGM gas that subsequently accretes on to the central galaxy in the progenitors of Mh(z = 0) ∼ 1012 M⊙ haloes, most of it is cool (T ∼ 104 K) at z = 2 but hot (∼Tvir) at z ∼ 0.25, consistent with the expected transition from cold mode to hot mode accretion. Despite the transition in accretion mode, at both z = 0.25 and \$2 \textbackslash, \{\textbackslash gtrsim\} 80\{\{\textbackslash{} \textbackslash rm per\textbackslash{} cent\}\}\$ of the cool gas in \$M\_\{\textbackslash rm h\} \textbackslash gtrsim 10\^\{11\} \textbackslash rm \{M\}\_\textbackslash odot\$ haloes will accrete on to a galaxy. We find that the metallicity of CGM gas is typically a poor predictor of both its proximate and ultimate fates. This is because there is in general little correlation between the origin of CGM gas and its fate owing to substantial mixing while in the CGM.},
  archiveprefix = {arXiv},
  eprint = {1910.01123},
  eprinttype = {arxiv},
  file = {/Users/zhafen/Zotero/storage/6HSA64TN/Hafen et al. - 2020 - The Fates of the Circumgalactic Medium in the FIRE.pdf;/Users/zhafen/Zotero/storage/2IZ9JTDN/1910.html},
  keywords = {Astrophysics - Astrophysics of Galaxies},
  note = {Analyzed the [fate of the [CGM]] in [FIRE-2 simulations], focusing on [z=0.25] and [z=2], including both the [ultimate [fate of the [CGM]]] (z=0 location) and the[proximate [fate of the [CGM]]] (where it goes upon leaving the CGM).

Of the [CGM] at [z=2], about half is found in the [ISM] or [stars] of the [central [galaxy]] by [z=0] in [\textasciitilde 1e11 Msun halos], but most of the [CGM] in [{$<$}1e11 Msun halos] ends up in the [IGM] after accreting onto the [central [galaxy]] and being [ejected].

Most of the [CGM] mass at [z=0.25] remains in the [CGM] by [z=0] across all halo masses analyzed.

Of the [CGM] gas that directly accretes onto the [central [galaxy]] in [L* halos], most of it is [cool [CGM] gas] (T\textasciitilde 1e4 K) at [z=2] but [hot [CGM] gas] (\textasciitilde Tvir) at [z=0.25], consistent with the expected transtion from [cold mode [accretion]] to [hot mode [accretion]].

At both [z=0.25] and [z=2] {$>\sptilde$}80\% of the [cool [CGM] gas] in [{$>\sptilde$}1e11 Msun halos] will be [accretion onto a [galaxy]].

There is little correlation between the [origin of the [CGM]] and the [fate of the [CGM]], and therefore the [metallicity] of [CGM] gas is typically a poor predictor of the [fate of the [CGM]].

!read=author
\par
Comment: 16 pages, 10 figures},
  number = {3}
}

@article{Hopkins2017,
  title = {{{FIRE}}-2 Simulations: Physics versus Numerics in Galaxy Formation},
  author = {Hopkins, Philip F and Wetzel, Andrew and Kereš, Dušan and Faucher-Giguère, Claude-André and Quataert, Eliot and Boylan-Kolchin, Michael and Murray, Norman and Hayward, Christopher C. and Garrison-Kimmel, Shea and Hummels, Cameron and Feldmann, Robert and Torrey, Paul and Ma, Xiangcheng and Anglés-Alcázar, Daniel and Su, Kung-Yi and Orr, Matthew and Schmitz, Denise and Escala, Ivanna and Sanderson, Robyn and Grudić, Michael Y and Hafen, Zachary and Kim, Ji-Hoon and Fitts, Alex and Bullock, James S. and Wheeler, Coral and Chan, T. K. and Elbert, Oliver D. and Narayanan, Desika},
  date = {2018-10-11},
  journaltitle = {Monthly Notices of the Royal Astronomical Society},
  volume = {480},
  pages = {800--863},
  issn = {0035-8711},
  doi = {10.1093/mnras/sty1690},
  url = {https://arxiv.org/abs/1702.06148},
  urldate = {2017-04-13},
  abstract = {The Feedback In Realistic Environments (FIRE) project explores the role of feedback in cosmological simulations of galaxy formation. Previous FIRE simulations used an identical source code (FIRE-1) for consistency. Now, motivated by the development of more accurate numerics (hydrodynamic solvers, gravitational softening, supernova coupling) and the exploration of new physics (e.g. magnetic fields), we introduce FIRE-2, an updated numerical implementation of FIRE physics for the GIZMO code. We run a suite of simulations and show FIRE-2 improvements do not qualitatively change galaxy-scale properties relative to FIRE-1. We then pursue an extensive study of numerics versus physics in galaxy simulations. Details of the star-formation (SF) algorithm, cooling physics, and chemistry have weak effects, provided that we include metal-line cooling and SF occurs at higher-than-mean densities. We present several new resolution criteria for high-resolution galaxy simulations. Most galaxy-scale properties are remarkably robust to the numerics that we test, provided that: (1) Toomre masses (cold disk scale heights) are resolved; (2) feedback coupling ensures conservation and isotropy, and (3) individual supernovae are time-resolved. As resolution increases, stellar masses and profiles converge first, followed by metal abundances and visual morphologies, then properties of winds and the circumgalactic medium. The central (\textasciitilde kpc) mass concentration of massive (L*) galaxies is sensitive to numerics, particularly how winds ejected into hot halos are trapped, mixed, and recycled into the galaxy. Multiple feedback mechanisms are required to reproduce observations: SNe regulate stellar masses; OB/AGB mass loss fuels late-time SF; radiative feedback suppresses instantaneous SFRs and accretion onto dwarfs. We provide tables, initial conditions, and the numerical algorithms required to reproduce our simulations.},
  archiveprefix = {arXiv},
  eprint = {1702.06148},
  eprinttype = {arxiv},
  file = {/Users/zhafen/Zotero/storage/PSVWASR8/Hopkins et al. - 2018 - FIRE-2 simulations physics versus numerics in galaxy formation(2).pdf},
  note = {Produce a new suite of [zoom-in] [hydrodynamic [cosmological simulations]], the [FIRE-2 simulations], an update of the [FIRE simulations] using [GIZMO].

[FIRE-2 simulations] and [FIRE simulations] are qualitatively comparable.

[Hydrodynamic [cosmological simulations]] do not depend on [star-formation prescription], [cooling prescription], or [chemistry prescription], provided [metal-line cooling] is implemented and [star formation] occurs at higher-than-mean densities.

Most galaxy-scale properties are remarkably robust to numerics, provided 1) [Toomre masses] are resolved, 2) [feedback] coupling conserves conservation and isotropy, and 3) individual [SNe] are time-resolved.

With increasing [resolution], [stellar masses] and [stellar profiles] converge first, followed by [metal abundances] and [visual morphologies], followed by [winds] and the [CGM].

The [central mass concentration] of galaxies is most sensitive to numerics, especially how [winds] ejected into [hot halos] are trapped, mixed, and recycled into the galaxy.

Multiple [feedback] mechanisms are required to reproduce observations: [SNe] dominate on [cosmological timescales], regulating [stellar masses]; [stellar mass loss] fules late-time star formation; [radiative feedback] helps to suppress [instantaneous [star formation]] in [{$<$}1e12 Msun halos].

The [[supernovae [feedback]] prescription] deposits [momentum] and [energy] from [[Sedov-Taylor] blast waves] at rates according to tabulated [stellar population synthesis] rates; the results match [high resolution [[ISM] simulations]].

The [energy] and [momentum]for the [[supernovae [feedback]] prescription] is calibrated to [high-resolution [SNe] simulations] and accounts for conversion of [thermal energy] to [kinetic energy] through [mass loading]; the result is higher [kinetic energy] than might otherwise be assumed.

!read=author},
  number = {1}
}

@article{Howk2017,
