@article{Muratov2016,
  title = {Metal Flows of the Circumgalactic Medium, and the Metal Budget in Galactic Haloes},
  author = {Muratov, Alexander L. and Kereš, Dušan and Faucher-Giguère, Claude-André and Hopkins, Philip F. and Ma, Xiangcheng and Anglés-Alcázar, Daniel and Chan, T. K. and Torrey, Paul and Hafen, Zachary H. and Quataert, Eliot and Murray, Norman},
  date = {2017-07-29},
  journaltitle = {Monthly Notices of the Royal Astronomical Society},
  volume = {468},
  pages = {4170--4188},
  issn = {0035-8711},
  doi = {10.1093/mnras/stx667},
  url = {http://arxiv.org/abs/1606.09252},
  urldate = {2016-07-04},
  abstract = {We present an analysis of the flow of metals through the circumgalactic medium in the Feedback in Realistic Environments (FIRE) simulations of galaxy formation, ranging from isolated dwarfs to \$L*\$. We find that nearly all metals produced in high-redshift galaxies are carried out in winds that escape the galaxy and reach \$0.25 R\_\{\textbackslash rm vir\}\$. When measured at \$0.25 R\_\{\textbackslash rm vir\}\$ the metallicity of outflows is greater than, but similar to the ISM metallicity. Many metals thus reside in a reservoir in the CGM. We find that the outflowing flux through \$R\_\{\textbackslash rm vir\}\$ is lower than that at \$0.25 R\_\{\textbackslash rm vir\}\$ by a factor of \$\textbackslash sim2-5\$. Cooling and recycling from this reservoir determine the metal budget in the ISM. The inflow metallicity at \$R\_\{\textbackslash rm vir\}\$ is generally very low, but outflow and inflow metallicities are similar in the inner halo. At low redshift, massive galaxies no longer generate outflows that reach the CGM, causing a divergence in CGM and ISM metallicity. Dwarf galaxies continue to generate outflows, which become increasingly dominated by metal-poor gas, while the galaxies themselves preferentially retains metal ejecta. In all but the least massive galaxy considered, a majority of the metals are within the halo at \$z=0\$. We measure the fraction of metals in CGM, ISM, stars, and roughly quantify the thermal state of CGM metals in each halo. The total amount of metals in the low-redshift CGM of two simulated L* galaxies is consistent with estimates from the COS halos survey, while for the other two it appears to be lower.},
  archiveprefix = {arXiv},
  eprint = {1606.09252},
  eprinttype = {arxiv},
  file = {/Users/zhafen/Zotero/storage/W5U4B7LQ/Muratov et al. - 2017 - Metal flows of the circumgalactic medium, and the metal budget in galactic haloes(3).pdf},
  note = {Study [metal]s in the [CGM] of the [FIRE simulations], spanning [1e9 Msun halos] to [1e12 Msun halos].

Nearly all [metals] produced in [[high-redshift] [galaxies]] are carried out in [wind]s that reach at least 0.25 Rvir, producing a large reservoir of metals in the [CGM].

The [metallicity] of [outflows] in the [CGM] at 0.25 Rvir is comparable to the [[ISM] [metallicity]].

The [[outflow] rate] in the [outer [CGM]] is higher than in the [inner [CGM]] by a factor of \textasciitilde 2-5.

The [metallicity] of [inflows] is much lower than the [outflow] metallicity in the [outer [CGM]], but comparable in the [inner [CGM]].

[Cooling] and [recycling] from the [CGM] constrain the [ISM [metal budget]].

In [low-redshift] [1e12 Msun halos] [outflows] do not reach far into the [CGM], reducing the [CGM] [metallicity].

In [low-redshift] [{$<\sptilde$}1e11 Msun halos] [outflows] continue to reach the CGM, but their [metallicity] decreases.

By [z=0] the [metal fraction] is greater than half the [metal budget] for [{$>$}1e10 Msun halos].

Measures the [metal budget] among the [CGM], [ISM], and [stars] in the [FIRE simulations], finding partial consistency with [COS-halos], but usually a lower [metal budget].

!read=author},
  number = {4},
  type = {Galaxy Astrophysics}
}

@online{pandya2021,
  title = {Characterizing Mass, Momentum, Energy and Metal Outflow Rates of Multi-Phase Galactic Winds in the {{FIRE}}-2 Cosmological Simulations},
  author = {Pandya, Viraj and Fielding, Drummond and Anglés-Alcázar, Daniel and Somerville, Rachel S. and Bryan, Greg L. and Hayward, Christopher C. and Stern, Jonathan and Kim, Chang-Goo and Quataert, Eliot and Forbes, John C. and Faucher-Giguère, Claude-André and Feldmann, Robert and Hafen, Zachary and Hopkins, Philip F. and Kereš, Dušan and Murray, Norman and Wetzel, Andrew},
  date = {2021-03-11},
  url = {http://arxiv.org/abs/2103.06891},
  urldate = {2021-03-16},
  abstract = {We characterize mass, momentum, energy and metal outflow rates of multi-phase galactic winds in a suite of FIRE-2 cosmological "zoom-in" simulations from the Feedback in Realistic Environments (FIRE) project. We analyze simulations of low-mass dwarfs, intermediate-mass dwarfs, Milky Way-mass halos, and high-redshift massive halos. Consistent with previous work, we find that dwarfs eject about 100 times more gas from their interstellar medium (ISM) than they form in stars, while this mass "loading factor" drops below one in massive galaxies. Most of the mass is carried by the hot phase (\${$>$}10\^5\$ K) in massive halos and the warm phase (\$10\^3-10\^5\$ K) in dwarfs; cold outflows (\${$<$}10\^3\$ K) are negligible except in high-redshift dwarfs. Energy, momentum and metal loading factors from the ISM are of order unity in dwarfs and significantly lower in more massive halos. Hot outflows have \$2-5\textbackslash times\$ higher specific energy than needed to escape from the gravitational potential of dwarf halos; indeed, in dwarfs, the mass, momentum, and metal outflow rates increase with radius whereas energy is roughly conserved, indicating swept up halo gas. Instantaneous mass loading factors tend to be larger during more powerful starbursts and when the inner halo is not virialized, but we see effectively no trend with the dense ISM gas fraction. We discuss how our results can guide future controlled numerical experiments that aim to elucidate the key parameters governing galactic winds and the resulting preventative feedback.},
  archiveprefix = {arXiv},
  eprint = {2103.06891},
  eprinttype = {arxiv}
}

