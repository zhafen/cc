
@article{Muratov2016,
	title = {Metal flows of the circumgalactic medium, and the metal budget in galactic haloes},
	volume = {468},
	issn = {0035-8711},
	url = {http://arxiv.org/abs/1606.09252},
	doi = {10.1093/mnras/stx667},
	abstract = {We present an analysis of the flow of metals through the circumgalactic medium in the Feedback in Realistic Environments ({FIRE}) simulations of galaxy formation, ranging from isolated dwarfs to \$L*\$. We find that nearly all metals produced in high-redshift galaxies are carried out in winds that escape the galaxy and reach \$0.25 R\_\{{\textbackslash}rm vir\}\$. When measured at \$0.25 R\_\{{\textbackslash}rm vir\}\$ the metallicity of outflows is greater than, but similar to the {ISM} metallicity. Many metals thus reside in a reservoir in the {CGM}. We find that the outflowing flux through \$R\_\{{\textbackslash}rm vir\}\$ is lower than that at \$0.25 R\_\{{\textbackslash}rm vir\}\$ by a factor of \${\textbackslash}sim2-5\$. Cooling and recycling from this reservoir determine the metal budget in the {ISM}. The inflow metallicity at \$R\_\{{\textbackslash}rm vir\}\$ is generally very low, but outflow and inflow metallicities are similar in the inner halo. At low redshift, massive galaxies no longer generate outflows that reach the {CGM}, causing a divergence in {CGM} and {ISM} metallicity. Dwarf galaxies continue to generate outflows, which become increasingly dominated by metal-poor gas, while the galaxies themselves preferentially retains metal ejecta. In all but the least massive galaxy considered, a majority of the metals are within the halo at \$z=0\$. We measure the fraction of metals in {CGM}, {ISM}, stars, and roughly quantify the thermal state of {CGM} metals in each halo. The total amount of metals in the low-redshift {CGM} of two simulated L* galaxies is consistent with estimates from the {COS} halos survey, while for the other two it appears to be lower.},
	pages = {4170--4188},
	number = {4},
	journaltitle = {Monthly Notices of the Royal Astronomical Society},
	author = {Muratov, Alexander L. and Kereš, Dušan and Faucher-Giguère, Claude-André and Hopkins, Philip F. and Ma, Xiangcheng and Anglés-Alcázar, Daniel and Chan, T. K. and Torrey, Paul and Hafen, Zachary H. and Quataert, Eliot and Murray, Norman},
	urldate = {2016-07-04},
	date = {2017-07-29},
	eprinttype = {arxiv},
	eprint = {1606.09252},
	note = {Genre: Galaxy Astrophysics},
	annotation = {Study [metal]s in the [{CGM}] of the [{FIRE} simulations], spanning [1e9 Msun halos] to [1e12 Msun halos].
Nearly all [metals] produced in [[high-redshift] [galaxies]] are carried out in [wind]s that reach at least 0.25 Rvir, producing a large reservoir of metals in the [{CGM}].
The [metallicity] of [outflows] in the [{CGM}] at 0.25 Rvir is comparable to the [[{ISM}] [metallicity]].
The [[outflow] rate] in the [outer [{CGM}]] is higher than in the [inner [{CGM}]] by a factor of {\textasciitilde}2-5.
The [metallicity] of [inflows] is much lower than the [outflow] metallicity in the [outer [{CGM}]], but comparable in the [inner [{CGM}]].
[Cooling] and [recycling] from the [{CGM}] constrain the [{ISM} [metal budget]].
In [low-redshift] [1e12 Msun halos] [outflows] do not reach far into the [{CGM}], reducing the [{CGM}] [metallicity].
In [low-redshift] [{\textless}{\textasciitilde}1e11 Msun halos] [outflows] continue to reach the {CGM}, but their [metallicity] decreases.
By [z=0] the [metal fraction] is greater than half the [metal budget] for [{\textgreater}1e10 Msun halos].
Measures the [metal budget] among the [{CGM}], [{ISM}], and [stars] in the [{FIRE} simulations], finding partial consistency with [{COS}-halos], but usually a lower [metal budget].

!read=author},
	file = {PDF:/Users/zhafen/Zotero/storage/W5U4B7LQ/Muratov et al. - 2017 - Metal flows of the circumgalactic medium, and the metal budget in galactic haloes(3).pdf:application/pdf},
}

@article{VandeVoort2016,
	title = {The impact of stellar feedback on hot gas in galaxy haloes: the Sunyaev–Zel'dovich effect and soft X-ray emission},
	volume = {463},
	issn = {0035-8711},
	url = {http://arxiv.org/abs/1604.01397},
	doi = {10.1093/mnras/stw2322},
	abstract = {The thermal Sunyaev-Zel'dovich ({SZ}) effect and soft X-ray emission are routinely observed around massive galaxies and in galaxy groups and clusters. We study these observational diagnostics of galaxy haloes for a suite of cosmological `zoom-in' simulations from the `Feedback In Realistic Environments' project, which spans a large range in halo mass 10{\textasciicircum}10-10{\textasciicircum}13 Msun). We explore the effect of stellar feedback on the hot gas observables. The properties of our simulated groups, such as baryon fractions, {SZ} flux, and X-ray luminosities (L\_X), are broadly consistent with existing observations, even though feedback from active galactic nuclei is not included. We make predictions for future observations of lower-mass objects for both {SZ} and diffuse X-ray measurements, finding that they are not just scaled-down versions of massive galaxies, but more strongly affected by galactic winds driven by star formation. Low-mass haloes ({\textless}{\textasciitilde}10{\textasciicircum}11 Msun) retain a low fraction of their baryons, which results in a strong suppression of the {SZ} signal. Our simulations therefore predict a scaling with halo mass that is steeper than self-similar for haloes less massive than 10{\textasciicircum}13 Msun. For halo masses {\textless}{\textasciitilde}10{\textasciicircum}12 Msun, L\_X is time-variable and correlated primarily with the star formation rate ({SFR}). For these objects, the diffuse X-ray emission is powered mostly by galactic winds and the gas dominating the X-ray emission is flowing out with radial velocities close to the halo's circular velocity. For halo masses {\textgreater}{\textasciitilde}10{\textasciicircum}13 Msun, on the other hand, L\_X is much less variable and not correlated with the {SFR}, because the emission originates from the quasi-hydrostatic, virialized halo gas.},
	pages = {4533--4544},
	number = {4},
	journaltitle = {Monthly Notices of the Royal Astronomical Society},
	author = {van de Voort, Freeke and Quataert, Eliot and Hopkins, Philip F. and Faucher-Giguère, Claude-André and Feldmann, Robert and Kereš, Dušan and Chan, T. K. and Hafen, Zachary},
	urldate = {2016-04-07},
	date = {2016-12-21},
	eprinttype = {arxiv},
	eprint = {1604.01397},
	note = {Genre: Galaxy Astrophysics; Cosmology and Extragalactic Astrophysics},
	keywords = {{FIRE} paper, theory: simulation, massive galaxies, soft X-ray, Sunyaev-Zel'dovich},
	annotation = {Studies the [observable properties] of the [{CGM}] for the [thermal [Sunyaev-Zel'dovich] effect] and [soft X-ray emission] using the [{FIRE}] simulations, including the effect of [stellar [feedback]].
The properties of [{FIRE}] halos are broadly consistent with observations of [baryon fraction], [[{SZ}] flux], and [X-ray luminosity], despite the lack of [black hole [feedback]].
Produce predictions of [future observations] for both [{SZ}] and [X-ray] observations and find that [{\textless}{\textasciitilde}1e12 Msun halos] are strongly affected by [galactic [wind]].
[{\textless}{\textasciitilde}1e11 Msun halos] have low [baryon fraction]s, and therefore a weak [{SZ}] signal.
[{\textless}{\textasciitilde}1e12 Msun halos] have time-variable [X-ray luminosity] correlated with the [{SFR}] and powered by [galactic [wind]].
[{\textgreater}{\textasciitilde}1e13 Msun halos] have steady [X-ray luminosity] produced by the [quasi-[hydrostatic]], [virialized], [hot halo].

!read=author},
	file = {PDF:/Users/zhafen/Zotero/storage/AZAEF8LT/van de Voort et al. - 2016 - The impact of stellar feedback on hot gas in galaxy haloes the Sunyaev–Zel'dovich effect and soft X-ray e.pdf:application/pdf},
}

@article{Garrison-kimmel2017,
	title = {The origin of the diverse morphologies and kinematics of Milky Way-mass galaxies in the {FIRE}-2 simulations},
	volume = {481},
	issn = {0035-8711},
	url = {http://arxiv.org/abs/1712.03966},
	doi = {10.1093/mnras/sty2513},
	abstract = {We use hydrodynamic cosmological zoom-in simulations from the {FIRE} project to explore the morphologies and kinematics of fifteen Milky Way ({MW})-mass galaxies. Our sample ranges from compact, bulge-dominated systems with 90\% of their stellar mass within 2.5 kpc to well-ordered disks that reach \${\textbackslash}gtrsim15\$ kpc. The gas in our galaxies always forms a thin, rotation-supported disk at \$z=0\$, with sizes primarily determined by the gas mass. For stars, we quantify kinematics and morphology both via the fraction of stars on disk-like orbits and with the radial extent of the stellar disk. In this mass range, stellar morphology and kinematics are poorly correlated with the properties of the halo available from dark matter-only simulations (halo merger history, spin, or formation time). They more strongly correlate with the gaseous histories of the galaxies: those that maintain a high gas mass in the disk after \$z{\textbackslash}sim1\$ develop well-ordered stellar disks. The best predictor of morphology we identify is the spin of the gas in the halo at the time the galaxy formed 1/2 of its stars (i.e. the gas that builds the galaxy). High-\$z\$ mergers, before a hot halo emerges, produce some of the most massive bulges in the sample (from compact disks in gas-rich mergers), while later-forming bulges typically originate from internal processes, as satellites are stripped of gas before the galaxies merge. Moreover, most stars in \$z=0\$ {MW}-mass galaxies (even \$z=0\$ bulge stars) form in a disk: \${\textbackslash}gtrsim\$60-90\% of stars begin their lives rotationally supported.},
	pages = {4133--4157},
	number = {3},
	journaltitle = {Monthly Notices of the Royal Astronomical Society},
	author = {Garrison-Kimmel, Shea and Hopkins, Philip F and Wetzel, Andrew and El-Badry, Kareem and Sanderson, Robyn E. and Bullock, James S and Ma, Xiangcheng and van de Voort, Freeke and Hafen, Zachary and Faucher-Giguère, Claude-André and Hayward, Christopher C. and Quataert, Eliot and Kereš, Dušan and Boylan-Kolchin, Michael},
	date = {2018-12-11},
	eprinttype = {arxiv},
	eprint = {1712.03966},
	keywords = {galaxies, halos, cosmology, theory, dark matter, local group},
	annotation = {Used the [{FIRE}-2 simulations] to study the [morphology] and [kinematics] of [L* galaxies], with simulated galaxies ranginging from [compact galaxies] to [extended disks].
In the [{FIRE}-2 simulations] all [L* galaxies] have a thin, [rotation-supported] disk at [z=0], with size related to the [galaxy gas mass].
Quantified [[stellar] kinematics] and [[stellar] morphology] using the fraction of stars on disk-like orbits and the radial extent of the stellar disk, both of which correlate tightly with the [stellar surface density] for [L* galaxies].
Across halo masses [[stellar] kinematics] and [[stellar] morphology] are poorly correlated with [dark matter initial conditions], but are rather correlated with the [galaxy gas evolution].
One of the better predictors for [stellar [morphology]] is the [[{CGM}] gas rotation] at the time the [galaxy] formed half its stars.

!read=author},
	file = {PDF:/Users/zhafen/Zotero/storage/C4P8HANG/Garrison-Kimmel et al. - 2018 - The origin of the diverse morphologies and kinematics of Milky Way-mass galaxies in the FIRE-2 simula(2).pdf:application/pdf},
}

@article{Hopkins2017,
	title = {{FIRE}-2 simulations: physics versus numerics in galaxy formation},
	volume = {480},
	issn = {0035-8711},
	url = {https://arxiv.org/abs/1702.06148},
	doi = {10.1093/mnras/sty1690},
	abstract = {The Feedback In Realistic Environments ({FIRE}) project explores the role of feedback in cosmological simulations of galaxy formation. Previous {FIRE} simulations used an identical source code ({FIRE}-1) for consistency. Now, motivated by the development of more accurate numerics (hydrodynamic solvers, gravitational softening, supernova coupling) and the exploration of new physics (e.g. magnetic fields), we introduce {FIRE}-2, an updated numerical implementation of {FIRE} physics for the {GIZMO} code. We run a suite of simulations and show {FIRE}-2 improvements do not qualitatively change galaxy-scale properties relative to {FIRE}-1. We then pursue an extensive study of numerics versus physics in galaxy simulations. Details of the star-formation ({SF}) algorithm, cooling physics, and chemistry have weak effects, provided that we include metal-line cooling and {SF} occurs at higher-than-mean densities. We present several new resolution criteria for high-resolution galaxy simulations. Most galaxy-scale properties are remarkably robust to the numerics that we test, provided that: (1) Toomre masses (cold disk scale heights) are resolved; (2) feedback coupling ensures conservation and isotropy, and (3) individual supernovae are time-resolved. As resolution increases, stellar masses and profiles converge first, followed by metal abundances and visual morphologies, then properties of winds and the circumgalactic medium. The central ({\textasciitilde}kpc) mass concentration of massive (L*) galaxies is sensitive to numerics, particularly how winds ejected into hot halos are trapped, mixed, and recycled into the galaxy. Multiple feedback mechanisms are required to reproduce observations: {SNe} regulate stellar masses; {OB}/{AGB} mass loss fuels late-time {SF}; radiative feedback suppresses instantaneous {SFRs} and accretion onto dwarfs. We provide tables, initial conditions, and the numerical algorithms required to reproduce our simulations.},
	pages = {800--863},
	number = {1},
	journaltitle = {Monthly Notices of the Royal Astronomical Society},
	author = {Hopkins, Philip F and Wetzel, Andrew and Kereš, Dušan and Faucher-Giguère, Claude-André and Quataert, Eliot and Boylan-Kolchin, Michael and Murray, Norman and Hayward, Christopher C. and Garrison-Kimmel, Shea and Hummels, Cameron and Feldmann, Robert and Torrey, Paul and Ma, Xiangcheng and Anglés-Alcázar, Daniel and Su, Kung-Yi and Orr, Matthew and Schmitz, Denise and Escala, Ivanna and Sanderson, Robyn and Grudić, Michael Y and Hafen, Zachary and Kim, Ji-Hoon and Fitts, Alex and Bullock, James S. and Wheeler, Coral and Chan, T. K. and Elbert, Oliver D. and Narayanan, Desika},
	urldate = {2017-04-13},
	date = {2018-10-11},
	eprinttype = {arxiv},
	eprint = {1702.06148},
	annotation = {Produce a new suite of [zoom-in] [hydrodynamic [cosmological simulations]], the [{FIRE}-2 simulations], an update of the [{FIRE} simulations] using [{GIZMO}].
[{FIRE}-2 simulations] and [{FIRE} simulations] are qualitatively comparable.
[Hydrodynamic [cosmological simulations]] do not depend on [star-formation prescription], [cooling prescription], or [chemistry prescription], provided [metal-line cooling] is implemented and [star formation] occurs at higher-than-mean densities.
Most galaxy-scale properties are remarkably robust to numerics, provided 1) [Toomre masses] are resolved, 2) [feedback] coupling conserves conservation and isotropy, and 3) individual [{SNe}] are time-resolved.
With increasing [resolution], [stellar masses] and [stellar profiles] converge first, followed by [metal abundances] and [visual morphologies], followed by [winds] and the [{CGM}].
The [central mass concentration] of galaxies is most sensitive to numerics, especially how [winds] ejected into [hot halos] are trapped, mixed, and recycled into the galaxy.
Multiple [feedback] mechanisms are required to reproduce observations: [{SNe}] dominate on [cosmological timescales], regulating [stellar masses]; [stellar mass loss] fules late-time star formation; [radiative feedback] helps to suppress [instantaneous [star formation]] in [{\textless}1e12 Msun halos].
The [[supernovae [feedback]] prescription] deposits [momentum] and [energy] from [[Sedov-Taylor] blast waves] at rates according to tabulated [stellar population synthesis] rates; the results match [high resolution [[{ISM}] simulations]].
The [energy] and [momentum]for the [[supernovae [feedback]] prescription] is calibrated to [high-resolution [{SNe}] simulations] and accounts for conversion of [thermal energy] to [kinetic energy] through [mass loading]; the result is higher [kinetic energy] than might otherwise be assumed.

!read=author},
	file = {PDF:/Users/zhafen/Zotero/storage/PSVWASR8/Hopkins et al. - 2018 - FIRE-2 simulations physics versus numerics in galaxy formation(2).pdf:application/pdf},
}

@article{Hafen2016,
	title = {Low-redshift Lyman limit systems as diagnostics of cosmological inflows and outflows},
	volume = {469},
	issn = {0035-8711},
	url = {http://arxiv.org/abs/1608.05712},
	doi = {10.1093/mnras/stx952},
	abstract = {We use cosmological hydrodynamic simulations with stellar feedback from the {FIRE} project to study the physical nature of Lyman limit systems ({LLSs}) at z{\textless}1. At these low redshifts, {LLSs} are closely associated with dense gas structures surrounding galaxies, such as galactic winds, dwarf satellites, and cool inflows from the intergalactic medium. Our analysis is based on 14 zoom-in simulations covering the halo mass range M\_h{\textasciitilde}10{\textasciicircum}9-10{\textasciicircum}13 Msun at z=0, which we convolve with the dark matter halo mass function to produce cosmological statistics. We find that the majority of cosmologically-selected {LLSs} are associated with halos in the mass range 10{\textasciicircum}10 {\textless} M\_h {\textless} 10{\textasciicircum}12 Msun. The incidence and {HI} column density distribution of simulated absorbers with columns 10{\textasciicircum}16.2 {\textless} N\_HI {\textless} 2x10{\textasciicircum}20 cm{\textasciicircum}-2 are consistent with observations. High-velocity outflows (with radial velocity exceeding the halo circular velocity by a factor {\textgreater}{\textasciitilde}2) tend to have higher metallicities ([X/H] {\textasciitilde} -0.5) while very low metallicity ([X/H] {\textless} -2) {LLSs} are typically associated with gas infalling from the intergalactic medium. However, most {LLSs} occupy an intermediate region in metallicity-radial velocity space, for which there is no clear trend between metallicity and radial kinematics. Metal-enriched inflows arise in the {FIRE} simulations as a result of galactic winds that fall back onto galaxies at low redshift. The overall simulated {LLS} metallicity distribution has a mean (standard deviation) [X/H] = -0.9 (0.4) and does not show significant evidence for bimodality, in contrast to recent observational studies but consistent with {LLSs} arising from halos with a broad range of masses and metallicities.},
	pages = {2292--2304},
	number = {2},
	journaltitle = {Monthly Notices of the Royal Astronomical Society},
	author = {Hafen, Zachary and Faucher-Giguère, Claude-André and Anglés-Alcázar, Daniel and Kereš, Dušan and Feldmann, Robert and Chan, T. K. and Quataert, Eliot and Murray, Norman and Hopkins, Philip F.},
	urldate = {2016-10-03},
	date = {2017-08-19},
	eprinttype = {arxiv},
	eprint = {1608.05712},
	note = {Genre: Galaxy Astrophysics; Cosmology and Extragalactic Astrophysics},
	keywords = {Absorption, H I, {LLS}, {CGM}, Metallicity, Written},
	annotation = {In the [{FIRE} simulations], [zoom-in] [hydrodynamic [cosmological simulations]], [{LLS}]s at [low-redshift] are associated with [dense [{CGM}] gas] from [galactic [wind]], [satellite galaxies], and [inflows].
In the [{FIRE} simulations] most [{LLS}] are found in the halos of [1e10 Msun halos] to [1e12 Msun halos].
The [{FIRE} simulations] produce [low-redshift] [cosmological [[H I] columns]] consistent with observations.
In the [{FIRE} simulations] high-velocity [outflows] tend to have higher [metallicity], while very low-metallicity [inflows] appear to be associated with [inflows], but most [{LLS}]s occupy an intermediate region in [metallicity]-[radial velocity] space.
[Metal-enriched [inflows]] can occur due to [recycling] of [galactic [winds]].
The [cosmological [{LLS} metallicity distribution]] does not show evidence for a [bimodality] due to [{LLS}]s originating from a broad range of halo masses.

!read=author},
	file = {PDF:/Users/zhafen/Zotero/storage/54Q8RIYX/Hafen et al. - 2017 - Low-redshift Lyman limit systems as diagnostics of cosmological inflows and outflows(2).pdf:application/pdf},
}

@article{Howk2017,
	title = {Project {AMIGA}: A Minimal Covering Factor for Optically Thick Circumgalactic Gas around the Andromeda Galaxy},
	volume = {846},
	issn = {1538-4357},
	url = {http://arxiv.org/abs/1706.01893},
	doi = {10.3847/1538-4357/aa87b4},
	abstract = {We present a deep search for {HI} 21-cm emission from the gaseous halo of Messier 31 as part of Project {AMIGA}, a large program Hubble Space Telescope program to study the circumgalactic medium of the Andromeda galaxy. Our observations with the Robert C. Byrd Green Bank Telesope target sight lines to 48 background {AGNs}, more than half of which have been observed in the ultraviolet with the Cosmic Origins Spectrograph, with impact parameters \$25 {\textbackslash}lesssim {\textbackslash}rho {\textbackslash}lesssim 330\$ kpc (\$0.1 {\textbackslash}lesssim {\textbackslash}rho / R\_\{{\textbackslash}rm vir\} {\textbackslash}lesssim 1.1\$). We do not detect any 21-cm emission toward these {AGNs} to limits of \$N(\{{\textbackslash}rm {HI}\}) {\textbackslash}approx 4 {\textbackslash}times10{\textasciicircum}\{17\}\$ cm\${\textasciicircum}\{-2\}\$ (\$5{\textbackslash}sigma\$, per 2 kpc diameter beam). This column density corresponds to an optical depth of \${\textbackslash}sim2.5\$ at the Lyman limit, thus our observations overlap with absorption line studies of Lyman limit systems at higher redshift. Our non-detections place a limit on the covering factor of such optically-thick gas around M31 to \$f\_c {\textless} 0.051\$ (at 90{\textbackslash}\% confidence) for \${\textbackslash}rho {\textbackslash}leq R\_\{{\textbackslash}rm vir\}\$. While individual clouds have previously been found in the region between M31 and M33, the covering factor of strongly optically-thick gas is quite small. Our upper limits on the covering factor are consistent with expectations from recent cosmological "zoom" simulations. Recent {COS}-Halos ultraviolet measurements of {\textbackslash}{HI}{\textbackslash} absorption about an ensemble of galaxies at \$z {\textbackslash}approx 0.2\$ show significantly higher covering factors within \${\textbackslash}rho {\textbackslash}lesssim 0.5 R\_\{{\textbackslash}rm vir\}\$ at the same \$N(\{{\textbackslash}rm H I\})\$, although the metal ion-to-H I ratios appear to be consistent with those seen in M31.},
	pages = {141},
	number = {2},
	journaltitle = {The Astrophysical Journal},
	author = {Howk, J. Christopher and Wotta, Christopher B. and Berg, Michelle A. and Lehner, Nicolas and Lockman, Felix J. and Hafen, Zachary and Pisano, D. J. and Faucher-Giguère, Claude-André and Wakker, Bart P. and Prochaska, J. Xavier and Wolfe, Spencer A. and Ribaudo, Joseph and Barger, Kathleen A. and Corlies, Lauren and Fox, Andrew J. and Guhathakurta, Puragra and Jenkins, Edward B. and Kalirai, Jason and O’Meara, John M. and Peeples, Molly S. and Stewart, Kyle R. and Strader, Jay},
	urldate = {2018-03-29},
	date = {2017-09-12},
	eprinttype = {arxiv},
	eprint = {1706.01893},
	annotation = {Present part of project [{AMIGA}], large [[{HST}] survey] of the [{CGM}] of [M31], probing at impact parameters 25-330 kpc.
Detect no [21-cm emission] down to [[H I] column]s of 4e17 cm{\textasciicircum}-2.
The [covering fraction] of 4e17 cm{\textasciicircum}-2 [[H I] column] [{CGM}] gas is {\textless}{\textasciitilde}0.051 around [M31], consistent with [zoom-in] [cosmological simulations] and significantly lower than [{COS}-Halos] results.

!read=author},
}

@article{hafen2019,
	title = {The origins of the circumgalactic medium in the {FIRE} simulations},
	volume = {488},
	issn = {0035-8711},
	url = {http://arxiv.org/abs/1811.11753},
	doi = {10.1093/mnras/stz1773},
	abstract = {We use a particle tracking analysis to study the origins of the circumgalactic medium ({CGM}), separating it into (1) accretion from the intergalactic medium ({IGM}), (2) wind from the central galaxy, and (3) gas ejected from other galaxies. Our sample consists of 21 {FIRE}-2 simulations, spanning the halo mass range Mh ∼ 1010–1012 M⊙, and we focus on z = 0.25 and z = 2. Owing to strong stellar feedback, only ∼L⋆ haloes retain a baryon mass \${\textbackslash}gtrsim{\textbackslash}! 50{\textbackslash}hbox\{ per cent\}\$ of their cosmic budget. Metals are more efficiently retained by haloes, with a retention fraction \${\textbackslash}gtrsim{\textbackslash}! 50{\textbackslash}hbox\{ per cent\}\$. Across all masses and redshifts analysed \${\textbackslash}gtrsim {\textbackslash}!60{\textbackslash}hbox\{ per cent\}\$ of the {CGM} mass originates as {IGM} accretion (some of which is associated with infalling haloes). Overall, the second most important contribution is wind from the central galaxy, though gas ejected or stripped from satellites can contribute a comparable mass in ∼L⋆ haloes. Gas can persist in the {CGM} for billions of years, resulting in well mixed-halo gas. Sightlines through the {CGM} are therefore likely to intersect gas of multiple origins. For low-redshift ∼L⋆ haloes, cool gas (T {\textless} 104.7 K) is distributed on average preferentially along the galaxy plane, however with strong halo-to-halo variability. The metallicity of {IGM} accretion is systematically lower than the metallicity of winds (typically by ≳1 dex), although {CGM} and {IGM} metallicities depend significantly on the treatment of subgrid metal diffusion. Our results highlight the multiple physical mechanisms that contribute to the {CGM} and will inform observational efforts to develop a cohesive picture.},
	pages = {1248--1272},
	number = {1},
	journaltitle = {Monthly Notices of the Royal Astronomical Society},
	author = {Hafen, Zachary and Faucher-Giguère, Claude-André and Anglés-Alcázar, Daniel and Stern, Jonathan and Kereš, Dušan and Hummels, Cameron and Esmerian, Clarke and Garrison-Kimmel, Shea and El-Badry, Kareem and Wetzel, Andrew and Chan, T. K. and Hopkins, Philip F. and Murray, Norman},
	urldate = {2018-11-30},
	date = {2019-09-01},
	eprinttype = {arxiv},
	eprint = {1811.11753},
	keywords = {Astrophysics - Astrophysics of Galaxies, Astrophysics - Cosmology and Nongalactic Astrophysics},
	annotation = {Comment: 23 pages, 22 figures. Minor revisions from previous version. Online interactive visualizations available at zhafen.github.io/{CGM}-origins and zhafen.github.io/{CGM}-origins-pathlines},
	annotation = {Uses a [particle-tracking] analysis applied to the [{FIRE}-2 simulations] to study the [origins of the [{CGM}]], including [{IGM} accretion], [galactic wind], and [satellite wind].
Strong [stellar feedback] means only [L* halos] retain {\textgreater}{\textasciitilde}50\% of their [baryon budget].
