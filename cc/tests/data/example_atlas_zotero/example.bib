@article{Stern2018,
  title = {Does {{Circumgalactic O}} vi {{Trace Low}}-Pressure {{Gas Beyond}} the {{Accretion Shock}}? {{Clues}} from {{H}} i and {{Low}}-Ion {{Absorption}}, {{Line Kinematics}}, and {{Dust Extinction}}},
  author = {Stern, Jonathan and Faucher-Giguère, Claude-André and Hennawi, Joseph F. and Hafen, Zachary and Johnson, Sean D. and Fielding, Drummond},
  date = {2018-09-25},
  journaltitle = {The Astrophysical Journal},
  volume = {865},
  pages = {91},
  issn = {1538-4357},
  doi = {10.3847/1538-4357/aac884},
  url = {http://arxiv.org/abs/1803.05446},
  urldate = {2018-11-12},
  abstract = {Large OVI columns are observed around star-forming, low-redshift \textasciitilde L* galaxies, with a dependence on impact parameter indicating that most O\^5+ particles reside beyond half the halo virial radius ({$>\sptilde$}100 kpc). In order to constrain the nature of the gas traced by OVI, we analyze additional observables of the outer halo, namely HI to OVI column ratios of 1-10, an absence of low-ion absorption, a mean differential extinction of E(B-V)\textasciitilde 10\^-3, and a linear relation between OVI column and velocity width. We contrast these observations with two physical scenarios: (1) OVI traces high-pressure (\textasciitilde 30 cm\^-3 K) collisionally-ionized gas cooling from a virially-shocked phase, and (2) OVI traces low-pressure ({$<\sptilde$}1 cm\^-3 K) gas beyond the accretion shock, where the gas is in ionization and thermal equilibrium with the UV background. We demonstrate that the high-pressure scenario requires multiple gas phases to explain the observations, and a large deposition of energy at {$>\sptilde$}100 kpc to offset the energy radiated by the cooling gas. In contrast, the low-pressure scenario can explain all considered observations with a single gas phase in thermal equilibrium, provided that the baryon overdensity is comparable to the dark-matter overdensity, and that the gas is enriched to {$>\sptilde$}Z\_sun/3 with an ISM-like dust-to-metal ratio. The low-pressure scenario implies that OVI traces a cool flow with mass flow rate of \textasciitilde 5 M\_sun yr\^-1, comparable to the star formation rate of the central galaxies. The OVI line widths are consistent with the velocity shear expected within this flow. The low-pressure scenario predicts a bimodality in absorption line ratios at \textasciitilde 100 kpc, due to the pressure jump across the accretion shock.},
  archiveprefix = {arXiv},
  eprint = {1803.05446},
  eprinttype = {arxiv},
  note = {Assess the consequences of certain [observable properties] of the [outer [CGM]], including values of \textasciitilde 1-10 for [[H I] to [O VI] [column ratio]]s, the absence of [low-ion [absorption]], a mean [differential extinction] of E\_B-V\textasciitilde 1e-3, and a linear relation between [[O VI] column] and [velocity width].

Compare [observable properties] of the [outer [CGM]] to those predicted by A) a high-pressure [collisionally-ionized] scenario and B) a low-pressure [photo-ionized] scenario.

Demonstrate that [observable properties] of the [outer [CGM]] match a [collisionally-ionized] scenario if there is [multiphase CGM gas] and large energy deposits.

Show that [photo-ionized] scenario is consistent with [observable properties] of the [outer [CGM]] with [single-phase gas] in [thermal equilibrium], given particular [baryon fractions] and gas [metallicity].

Provides evidence that in a [photo-ionized] scenario for the [outer [CGM]] the [origin of [O VI]] is a cool flow with inflow rate \textasciitilde 5 Msun/yr, and that [[O VI] [line widths]] are consistent with this scenario.

A prediction of the [photo-ionized] scenario for the [outer [CGM]] is a bimodality in [column ratio]s at the [accretion shock].

!read=author},
  number = {2}
}

