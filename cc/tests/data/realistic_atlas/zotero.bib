@article{mansfieldHowBiasedAre2020,
	title = {How Biased Are Halo Properties in Cosmological Simulations?},
	url = {http://arxiv.org/abs/2008.08591},
	abstract = {Cosmological N-body simulations have been a major tool of theorists for decades, yet many of the numerical issues that these simulations face are still unexplored. This paper measures numerical biases in these large, dark matter-only simulations that affect the properties of their dark matter haloes. We compare many simulation suites in order to provide several tools for simulators and analysts which help mitigate these biases. We summarise our comparisons with practical `convergence limits' that can be applied to a wide range of halo properties, including halo properties which are traditionally overlooked by the testing literature. We also find that the halo properties predicted by different simulations can diverge from one another at unexpectedly high resolutions. We demonstrate that many halo properties depend strongly on force softening scale and that this dependence leads to much of the measured divergence between simulations. We offer an empirical model to estimate the impact of such effects on the rotation curves of a halo population. This model can serve as a template for future empirical models of the biases in other halo properties.},
	author = {Mansfield, Philip and Avestruz, Camille},
	urldate = {2020-08-21},
	date = {2020-08-19},
	eprinttype = {arxiv},
	eprint = {2008.08591},
}

@article{velazquezTimescalesProbedStar2020,
	title = {The time-scales probed by star formation rate indicators for realistic, bursty star formation histories from the {FIRE} simulations},
	url = {http://arxiv.org/abs/2008.08582},
	abstract = {Understanding the rate at which stars form is central to studies of galaxy formation. Observationally, the star formation rates ({SFRs}) of galaxies are measured using the luminosity in different frequency bands, often under the assumption of a time-steady {SFR} in the recent past. We use star formation histories ({SFHs}) extracted from cosmological simulations of star-forming galaxies from the {FIRE} project to analyze the time-scales to which the H\$\{{\textbackslash}alpha\}\$ and far-ultraviolet ({FUV}) continuum {SFR} indicators are sensitive. In these simulations, the {SFRs} are highly time variable for all galaxies at high redshift, and continue to be bursty to z=0 in dwarf galaxies. When {FIRE} {SFHs} are partitioned into their bursty and time-steady phases, the best-fitting {FUV} time-scale fluctuates from its {\textasciitilde}10 Myr value when the {SFR} is time-steady to {\textgreater}{\textasciitilde}100 Myr immediately following particularly extreme bursts of star formation during the bursty phase. On the other hand, the best-fitting averaging time-scale for H\$\{{\textbackslash}alpha\}\$ is generally insensitive to the {SFR} variability in the {FIRE} simulations and remains {\textasciitilde}5 Myr at all times. These time-scales are shorter than the 100 Myr and 10 Myr time-scales sometimes assumed in the literature for {FUV} and H\$\{{\textbackslash}alpha\}\$, respectively, because while the {FUV} emission persists for stellar populations older than 100 Myr, the time-dependent luminosities are strongly dominated by younger stars. Our results confirm that the ratio of {SFRs} inferred using H\$\{{\textbackslash}alpha\}\$ vs. {FUV} can be used to probe the burstiness of star formation in galaxies.},
	author = {Velázquez, José A. Flores and Gurvich, Alexander B. and Faucher-Giguère, Claude-André and Bullock, James S. and Starkenburg, Tjitske K. and Moreno, Jorge and Lazar, Alexandres and Mercado, Francisco J. and Stern, Jonathan and Sparre, Martin and Hayward, Christopher C. and Wetzel, Andrew and El-Badry, Kareem},
	urldate = {2020-08-21},
	date = {2020-08-19},
	eprinttype = {arxiv},
	eprint = {2008.08582},
}

@article{wildeCGMExtentCircumgalactic2020,
	title = {{CGM}\${\textasciicircum}\{2\}\$ I: The Extent of the Circumgalactic Medium Traced by Neutral Hydrogen},
	url = {http://arxiv.org/abs/2008.08092},
	abstract = {We present initial results from the {COS} and Gemini Mapping the Circumgalactic Medium ({CGMCGM} \${\textbackslash}equiv\$ {CGM}\${\textasciicircum}\{2\}\$) survey. The {CGM}\${\textasciicircum}\{2\}\$ spectroscopic survey consists of 1689 galaxies, all with high-quality Gemini {GMOS} spectra suitable for precise redshift measurements, within 1 Mpc of twenty-two \$z {\textbackslash}lesssim 1\$ quasars, all with existing S/N\${\textbackslash}sim\$10 {HST}-{COS} G130M\$+\$G160M spectra. We show that the {HI} covering fraction above a threshold of \$N\_\{{\textbackslash}rm {HI}\}\$ \${\textgreater}10{\textasciicircum}\{14\} \$cm\${\textasciicircum}\{-2\}\$ is \${\textbackslash}gtrsim 0.6\$ within 1.5 virial radii (\$R\_\{{\textbackslash}rm vir\} {\textbackslash}sim R\_\{200m\}\$) of galaxies having stellar masses \$10{\textasciicircum}\{8\} M\_\{{\textbackslash}odot\} {\textless} M\_\{{\textbackslash}star\} {\textless} 10{\textasciicircum}\{11\} M\_\{{\textbackslash}odot\}\$. We examine the kinematics of the {HI} absorption relative to the galaxy systemic velocities and find that the majority of absorption lies within \${\textbackslash}pm\$ 250 km s\${\textasciicircum}\{-1\}\$, and that this concentration increases with increasing {HI} column density from \$N\_\{{\textbackslash}rm {HI}\}\$ \$=10{\textasciicircum}\{14\} - 10{\textasciicircum}\{17\}\$ cm\${\textasciicircum}\{-2\}\$. We examine {HI} covering fractions over a range of impact parameters to infer a characteristic size of the {CGM}, \$R{\textasciicircum}\{14\}\_\{{\textbackslash}rm {CGM}\}\$, as a function of galaxy mass. \$R{\textasciicircum}\{14\}\_\{{\textbackslash}rm {CGM}\}\$ is the impact parameter at which the probability of observing an absorber with \$N\_\{{\textbackslash}rm {HI}\}\$ \${\textgreater}\$ 10\${\textasciicircum}\{14\}\$ cm\${\textasciicircum}\{-2\}\$ is \${\textgreater}\$ 50\%. In this framework, the radial extent of the {CGM} of \$M\_\{{\textbackslash}star\} {\textgreater} 10{\textasciicircum}\{10\} M\_\{{\textbackslash}odot\}\$ galaxies is \$R{\textasciicircum}\{14\}\_\{{\textbackslash}rm {CGM}\} = 362\_\{-72\}{\textasciicircum}\{+83\}\$ kpc or \$R{\textasciicircum}\{14\}\_\{{\textbackslash}rm {CGM}\} = 1.4R\_\{{\textbackslash}rm vir\}\$. Lower-mass galaxies show a smaller physical scale \$R{\textasciicircum}\{14\}\_\{{\textbackslash}rm {CGM}\} = 281\_\{-36\}{\textasciicircum}\{+42\}\$ kpc but extend to \$R{\textasciicircum}\{14\}\_\{{\textbackslash}rm {CGM}\} = 2.3R\_\{{\textbackslash}rm vir\}\$. Our analysis suggests that using \$R\_\{{\textbackslash}rm vir\}\$ as a proxy for the characteristic radius of the {CGM} likely underestimates its extent, especially for galaxies with \$M\_\{{\textbackslash}star\} {\textless} 10{\textasciicircum}\{10\} M\_\{{\textbackslash}odot\}\$.},
	author = {Wilde, Matthew C. and Werk, Jessica K. and Burchett, Joseph N. and Prochaska, J. Xavier and Tchernyshyov, Kirill and Tripp, Todd M. and Tejos, Nicolas and Lehner, Nicolas and Bordoloi, Rongmon and O'Meara, John M. and Tumlinson, Jason},
	urldate = {2020-08-20},
	date = {2020-08-18},
	eprinttype = {arxiv},
	eprint = {2008.08092},
}

@article{vandevoortEffectMagneticFields2020,
	title = {The effect of magnetic fields on properties of the circumgalactic medium},
	url = {http://arxiv.org/abs/2008.07537},
	abstract = {We study the effect of magnetic fields on a simulated galaxy and its surrounding gaseous halo, or circumgalactic medium ({CGM}), within cosmological 'zoom-in' simulations of a Milky Way-mass galaxy as part of the 'Simulating the Universe with Refined Galaxy Environments' ({SURGE}) project. We use three different galaxy formation models, each with and without magnetic fields, and include additional spatial refinement in the {CGM} to improve its resolution. The central galaxy's star formation rate and stellar mass are not strongly affected by the presence of magnetic fields, but the galaxy is more disc-dominated and its central black hole is more massive when \$B{\textgreater}0\$. The physical properties of the {CGM} change significantly. With magnetic fields, the gas fraction and metal mass fraction in the halo are higher, the atomic hydrogen-dominated extended discs around the galaxy are more massive, the circumgalactic gas flows are slower, the densities in the inner {CGM} are higher, the temperatures in the outer {CGM} are higher, the pressure in the halo is higher, and the metals are less well-mixed throughout the halo. Additionally, we find that the {CGM} properties depend on azimuthal angle and that magnetic fields reduce the scatter in radial velocity, whilst enhancing the scatter in metallicity at fixed azimuthal angle. These results together show that magnetic fields in the {CGM} change the flow of gas in galaxy haloes, making it more difficult for metal-rich outflows to mix with the metal-poor {CGM} and to escape the halo, and therefore should be included in simulations of galaxy formation.},
	author = {van de Voort, Freeke and Bieri, Rebekka and Pakmor, Rüdiger and Gómez, Facundo A. and Grand, Robert J. J. and Marinacci, Federico},
	urldate = {2020-08-19},
	date = {2020-08-17},
	eprinttype = {arxiv},
	eprint = {2008.07537},
}

@article{ockerConstrainingGalaxyHaloes2021a,
	title = {Constraining Galaxy Haloes from the Dispersion and Scattering of Fast Radio Bursts and Pulsars},
	url = {http://arxiv.org/abs/2101.04784},
	abstract = {Fast radio bursts ({FRBs}) can be scattered by ionized gas in their local environments, host galaxies, intervening galaxies along their lines-of-sight, the intergalactic medium, and the Milky Way. The relative contributions of these different media depend on their geometric configuration and the internal properties of the gas. When these relative contributions are well understood, {FRB} scattering is a powerful probe of density fluctuations along the line-of-sight. Using {FRB} 121102 and {FRB} 180916 as case studies, we place an upper limit on the amount of scattering contributed by the Milky Way halo to {FRBs}. The scattering time \${\textbackslash}tau {\textbackslash}propto ({\textbackslash}tilde\{F\} {\textbackslash}times \{{\textbackslash}rm {DM}\}{\textasciicircum}2) A\_{\textbackslash}tau\$, where {DM} is the dispersion measure, \${\textbackslash}tilde\{F\}\$ quantifies electron density variations with \${\textbackslash}tilde\{F\}=0\$ for a smooth medium, and the dimensionless constant \$A\_{\textbackslash}tau\$ quantifies the difference between the mean scattering delay and the \$1/e\$ scattering time typically measured. Using a likelihood analysis we find \$({\textbackslash}tilde\{F\} {\textbackslash}times \{{\textbackslash}rm {DM}\}{\textasciicircum}2)\_\{{\textbackslash}rm {MW}, halo\} {\textless} 250/A\_{\textbackslash}tau\$ pc\${\textasciicircum}\{4/3\}\$ km\${\textasciicircum}\{-1/3\}\$ cm\${\textasciicircum}\{-1/3\}\$. For an assumed halo \${\textbackslash}widehat\{\{{\textbackslash}rm {DM}\}\}\_\{{\textbackslash}rm {MW}, halo\} = 60 {\textbackslash}pm 18\$ pc cm\${\textasciicircum}\{-3\}\$ we obtain \${\textbackslash}tilde\{F\}\_\{{\textbackslash}rm {MW},halo\}{\textless}0.03/A\_{\textbackslash}tau\$ pc\${\textasciicircum}\{-2/3\}\$ km\${\textasciicircum}\{-1/3\}\$. By contrast, pulsar scattering and {DMs} imply \${\textbackslash}tilde\{F\} {\textbackslash}sim0.1\$ pc\${\textasciicircum}\{-2/3\}\$ km\${\textasciicircum}\{-1/3\}\$ for the Galactic thin disk and \${\textbackslash}tilde\{F\}{\textbackslash}sim0.001\$ pc\${\textasciicircum}\{-2/3\}\$ km\${\textasciicircum}\{-1/3\}\$ for the thick disk. The maximum pulse broadening from the halo is \${\textbackslash}tau {\textbackslash}lesssim 12\$ \${\textbackslash}mu\$s at 1 {GHz}. We compare our analysis of the Milky Way halo with other galaxy haloes by placing limits on the scattering contributions from haloes intersecting the lines-of-sight to {FRB} 181112 and {FRB} 191108. Our results are consistent with haloes making negligible or very small contributions to the scattering times of {FRBs}.},
	author = {Ocker, Stella Koch and Cordes, James M. and Chatterjee, Shami},
	urldate = {2021-01-14},
	date = {2021-01-12},
	eprinttype = {arxiv},
	eprint = {2101.04784},
}

