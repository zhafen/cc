	file = {Full Text PDF:/Users/zhafen/Zotero/storage/UNKE9T5D/Fall and Efstathiou - 1980 - Formation and rotation of disc galaxies with haloe.pdf:application/pdf},
}

@article{pratt2021,
	title = {The Resolved Sunyaev--Zel'dovich Profiles of Nearby Galaxy Groups},
	url = {http://arxiv.org/abs/2105.01123},
	abstract = {Much of the baryons in galaxy groups are thought to have been driven out to large distances (\${\textbackslash}gtrsim\$\$R\_\{500\}\$) by feedback, but there are few constraining observations of this extended gas. This work presents the resolved Sunyaev--Zel'dovich ({SZ}) profiles for a stacked sample of 10 nearby galaxy groups within the mass range log\$\_\{10\}(M\_\{500\}[M\_\{{\textbackslash}odot\}]) = 13.6 -13.9\$. We measured the {SZ} profiles using the publicly available \$y\$-map from the Planck Collaboration as well as our own \$y\$-maps constructed from more recent versions of \$Planck\$ data. The \$y\$-map extracted from the latest data release yielded a significant {SZ} detection out to 3 \$R\_\{500\}\$. In addition, the stacked profile from these data were consistent with simulations that included {AGN} feedback. Our best-fit model using the latest \$Planck\$ data suggested a baryon fraction \${\textbackslash}approx 5.6{\textbackslash}\%\$ within \$R\_\{500\}\$. This is significantly lower than the cosmic value of \${\textbackslash}approx 16{\textbackslash}\%\$, supporting the idea that baryons have been driven to large radii by {AGN} feedback. Lastly, we discovered a significant (\${\textbackslash}sim 3{\textbackslash}sigma\$) "bump" feature near \${\textbackslash}sim 2\$ \$R\_\{500\}\$ that is most likely the signature of internal accretion shocks.},
	journaltitle = {{arXiv}:2105.01123 [astro-ph]},
	author = {Pratt, C. T. and Qu, Z. and Bregman, J. N.},
	urldate = {2021-05-05},
	date = {2021-05-03},
	eprinttype = {arxiv},
	eprint = {2105.01123},
	keywords = {Astrophysics - Astrophysics of Galaxies, Astrophysics - Cosmology and Nongalactic Astrophysics},
	file = {arXiv Fulltext PDF:/Users/zhafen/Zotero/storage/NK8A3S8Q/Pratt et al. - 2021 - The Resolved Sunyaev--Zel'dovich Profiles of Nearb.pdf:application/pdf;arXiv.org Snapshot:/Users/zhafen/Zotero/storage/LNNIG8C6/2105.html:text/html},
}

@article{theriault2021,
	title = {Global 21cm Absorption Signal from Superconducting Cosmic Strings},
	url = {http://arxiv.org/abs/2105.01166},
	abstract = {Superconducting cosmic strings emit electromagnetic waves between the times of recombination and reionization. Hence, they have an effect on the global 21cm signal. We compute the resulting absorption features, focusing on strings with critical current, study their dependence on the string tension \${\textbackslash}mu\$, and compare with observational results. For string tensions in the range of \$G {\textbackslash}mu = 10{\textasciicircum}\{-10\}\$, where \$G\$ is Newton's gravitational constant, there is an interesting amplification of the two characteristic absorption features, one during the cosmic dawn, \$z {\textbackslash}lesssim 30\$, and the other during the cosmic dark age, \$z {\textbackslash}sim 80\$, the former being comparable in amplitude to what was observed by the {EDGES} experiment.},
