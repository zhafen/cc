@article{Machado2017,
	title = {Properties of the circumgalactic medium in simulations compared to observations},
	url = {http://arxiv.org/abs/1710.08726},
	abstract = {Galaxies are surrounded by extended gaseous halos which store significant fractions of chemical elements. These are syntethized by the stellar populations and later ejected into the circumgalactic medium ({CGM}) by different mechanism, of which supernova feedback is considered one of the most relevant. We explore the properties of this metal reservoir surrounding star-forming galaxies in a cosmological context aiming to investigate the chemical loop between galaxies and their {CGM}, and the ability of the subgrid models to reproduce observational results. Using cosmological hydrodynamical simulations, we analyse the gas-phase chemical contents of galaxies with stellar masses in the range \$10{\textasciicircum}\{9\} - 10{\textasciicircum}\{11\}{\textbackslash},\{{\textbackslash}rm M\}\_\{{\textbackslash}odot\}\$. We estimate the fractions of metals stored in the different {CGM} phases, and the predicted {OVI} and {SiIII} column densities within the virial radius. We find roughly \$10{\textasciicircum}\{7\}{\textbackslash},\{{\textbackslash}rm M\}\_\{{\textbackslash}odot\}\$ of oxygen in the {CGM} of simulated galaxies having \$M\_\{{\textbackslash}star\}\{{\textbackslash}sim\}10{\textasciicircum}\{10\}{\textbackslash},\{{\textbackslash}rm M\}\_\{{\textbackslash}odot\}\$, in fair agreement with the lower limits imposed by observations. The \$M\_\{{\textbackslash}rm oxy\}\$ is found to correlate with \$M\_\{{\textbackslash}star\}\$, at odds with current observational trends but in agreement with other numerical results. The estimated profiles of {OVI} column density reveal a substantial shortage of that ion, whereas {SiIII}, which probes the cool phase, is overpredicted. The analysis of the relative contributions of both ions from the hot, warm and cool phases suggests that the warm gas (\$ 10{\textasciicircum}5{\textasciitilde}\{{\textbackslash}rm K\} {\textless} T {\textless} 10{\textasciicircum}6{\textasciitilde}\{{\textbackslash}rm K\}\$) should be more abundant in order to bridge the mismatch with the observations, or alternatively, that more metals should be stored in this gas-phase. Adittionally, we find that the X-ray coronae around the simulated galaxies have luminosities and temperatures in decent agreement with the available observational estimates. [abridged]},
	author = {Machado, Rubens E. G. and Tissera, Patricia B. and Neto, Gastão B. Lima and Sodré, Laerte},
	urldate = {2017-11-09},
	date = {2017-10-24},
	eprinttype = {arxiv},
	eprint = {1710.08726},
	file = {PDF:/Users/zhafen/Zotero/storage/29FXK6AR/Machado et al. - 2017 - Properties of the circumgalactic medium in simulations compared to observations.pdf:application/pdf},
}

@article{Liang2017b,
	title = {{BayesVP}: a Bayesian Voigt profile fitting package},
	url = {http://arxiv.org/abs/1710.09852},
	abstract = {We introduce a Bayesian approach for modeling Voigt profiles in absorption spectroscopy and its implementation in the python package, {BayesVP}, publicly available at https://github.com/cameronliang/{BayesVP}. The code fits the absorption line profiles within specified wavelength ranges and generates posterior distributions for the column density, Doppler parameter, and redshifts of the corresponding absorbers. The code uses publicly available efficient parallel sampling packages to sample posterior and thus can be run on parallel platforms. {BayesVP} supports simultaneous fitting for multiple absorption components in high-dimensional parameter space. We provide other useful utilities in the package, such as explicit specification of priors of model parameters, continuum model, Bayesian model comparison criteria, and posterior sampling convergence check.},
	author = {Liang, Cameron and Kravtsov, Andrey},
	urldate = {2017-11-09},
	date = {2017-10-26},
	eprinttype = {arxiv},
	eprint = {1710.09852},
}

@article{Kakiichi2017,
	title = {A new model framework for circumgalactic Ly\${\textbackslash}alpha\$ radiative transfer constrained by galaxy-Ly\${\textbackslash}alpha\$ forest clustering},
	url = {http://arxiv.org/abs/1710.10053},
	abstract = {We present a new perturbative approach to "constrained Ly\${\textbackslash}alpha\$ radiative transfer'" ({RT}) through the circum- and inter-galactic medium ({CGM} and {IGM}). We constrain the {HI} content and kinematics of the {CGM} and {IGM} in a physically motivated model, using the galaxy-Ly\${\textbackslash}alpha\$ forest clustering data from spectroscopic galaxy surveys in {QSO} fields at \$z{\textbackslash}sim2-3\$. This enables us to quantify the impact of the {CGM}/{IGM} on Ly\${\textbackslash}alpha\$ emission in an observationally constrained, realistic cosmological environment. Our model predicts that the {CGM} and {IGM} at these redshifts transmit \${\textbackslash}approx80{\textasciitilde}{\textbackslash}\%\$ of Ly\${\textbackslash}alpha\$ photons after having escaped from galaxies. This implies that while the inter-stellar medium primarily regulates Ly\${\textbackslash}alpha\$ escape, the {CGM} has a non-negligible impact on the observed Ly\${\textbackslash}alpha\$ line properties and the inferred Ly\${\textbackslash}alpha\$ escape fraction, even at \$z{\textbackslash}sim 2-3\$. Ly\${\textbackslash}alpha\$ scattering in the {CGM} and {IGM} further introduces an environmental dependence in the (apparent) Ly\${\textbackslash}alpha\$ escape fraction, and the observed population of Ly\${\textbackslash}alpha\$ emitting galaxies: the {CGM}/{IGM} more strongly suppresses direct Ly\${\textbackslash}alpha\$ emission from galaxies in overdense regions in the Universe, and redistributes this emission into brighter Ly\${\textbackslash}alpha\$ haloes. The resulting mean surface brightness profile of the Ly\${\textbackslash}alpha\$ haloes is generally found to be a power-law \${\textbackslash}propto r{\textasciicircum}\{-2.4\}\$. Our results demonstrate how (integral field) spectroscopic surveys of galaxies in {QSO} fields constrain circumgalactic Ly\${\textbackslash}alpha\$ {RT}, and we discuss the potential of these models for studying {CGM} physics, reionization and cosmology.},
	author = {Kakiichi, Koki and Dijkstra, Mark},
	urldate = {2017-11-09},
	date = {2017-10-27},
	eprinttype = {arxiv},
	eprint = {1710.10053},
	file = {PDF:/Users/zhafen/Zotero/storage/YDXRUTNG/Kakiichi, Dijkstra - 2017 - A new model framework for circumgalactic Ly\$alpha\$ radiative transfer constrained by galaxy-Ly\$alpha\$ forest.pdf:application/pdf},
}

@article{Lian2017,
	title = {The mass-metallicity relations for gas and stars in star-forming galaxies: strong outflow vs variable {IMF}},
	url = {http://arxiv.org/abs/1710.11135},
	abstract = {We investigate the mass-metallicity relations for the gaseous ({MZRgas}) and stellar components ({MZRstar}) of local star-forming galaxies based on a representative sample from {SDSS} {DR}12. The mass-weighted average stellar metallicities are systematically lower than the gas metallicities. This difference in metallicity increases toward galaxies with lower masses and reaches 0.4-0.8 dex at 10{\textasciicircum}9 Msun (depending on the gas metallicity calibration). As a result, the {MZRstar} is much steeper than the {MZRgas}. The much lower metallicities in stars compared to the gas in low mass galaxies implies dramatic metallicity evolution with suppressed metal enrichment at early times. The aim of this paper is to explain the observed large difference in gas and stellar metallicity and to infer the origin of the mass-metallicity relations. To this end we develop a galactic chemical evolution model accounting for star formation, gas inflow and outflow. By combining the observed mass-metallicity relation for both gas and stellar components to constrain the models, we find that only two scenarios are able to reproduce the observations. Either strong metal outflow or a steep {IMF} slope at early epochs of galaxy evolution is needed. Based on these two scenarios, for the first time we successfully reproduce the observed {MZRgas} and {MZRstar} simultaneously, together with other independent observational constraints in the local universe. Our model also naturally reproduces the flattening of the {MZRgas} at the high mass end leaving the {MZRstar} intact, as seen in observational data.},
	author = {Lian, Jianhui and Thomas, Daniel and Maraston, Claudia and Goddard, Daniel and Comparat, Johan and Gonzalez-Perez, Violeta and Ventura, Paolo},
	urldate = {2017-11-09},
	date = {2017-10-30},
	eprinttype = {arxiv},
	eprint = {1710.11135},
	file = {PDF:/Users/zhafen/Zotero/storage/LE7MBPCI/Lian et al. - 2017 - The mass-metallicity relations for gas and stars in star-forming galaxies strong outflow vs variable IMF.pdf:application/pdf},
}

@article{Concas2017,
	title = {Two-Face(s): ionized and neutral gas winds in the local Universe},
	url = {http://arxiv.org/abs/1710.08423},
	abstract = {We present a comprehensive study of the interstellar {NaI} \${\textbackslash}lambda\$5890, 5895 ({NaD}) resonant lines in a complete spectroscopic sample of \${\textbackslash}sim 600,000\$ passive, star-forming and starburst galaxies drawn from {SDSS} {DR}7 in order to look for cold-gas outflows in the local Universe. Individual galaxy spectra are stacked in bins of stellar mass and {SFR} and the dependence of galactic winds, with respect to the galaxies position in the {SFR}-\$M\_\{{\textbackslash}star\}\$ plane is investigated. While in most cases the interstellar medium ({ISM}) lines are fixed at the galaxy systemic velocity, at the higher {SFR} tail ({SFR}\${\textgreater}12.5 M\_\{{\textbackslash}odot\} yr{\textasciicircum}\{-1\}\$), we find evidence of blue-shifted {NaD} absorption profiles, which we interpret as evidence of neutral outflowing gas. We explore the properties of the {ISM} in these galaxies with high {SFR}, in particular relating the absorption {NaD} line shape with the galaxy geometry in galaxies with different ionization mechanisms: {AGN} and star-formation. We find that: a) the {ISM} {NaD} absorption lines show a clear transition from a strong disk-like component, perfectly centered to the systemic velocity, in the edge-on system (\$i {\textgreater} 50{\textasciicircum}{\textbackslash}circ\$ of the disk rotation axis), to an outflow, blue-shifted, component in face-on galaxies (\$i {\textless} 50{\textasciicircum}{\textbackslash}circ\$); b) these trends are observed in galaxies classified as "purely" {SF} and {AGN} dominated objects. We compare the kinematics of the neutral gas with the kinematics of the ionized gas as traced by the [{OIII}]\${\textbackslash}lambda\$5007 emission lines. We find that, in these high {SFR} galaxies, the perturbations of the [{OIII}] emission line are present only in {AGN} or composite systems. In conclusion, we find that, in the local Universe, galactic winds show two faces which are related to two different ejection mechanisms, namely the neutral outflowing gas phase related to the star formation rate along the galaxy disk and the ionized winds related to the {AGN} feedback.},
	author = {Concas, A. and Popesso, P. and Brusa, M. and Mainieri, V. and Thomas, D.},
	urldate = {2017-11-09},
	date = {2017-10-23},
	eprinttype = {arxiv},
	eprint = {1710.08423},
	file = {PDF:/Users/zhafen/Zotero/storage/29HZGC4M/Concas et al. - 2017 - Two-Face(s) ionized and neutral gas winds in the local Universe.pdf:application/pdf},
}

@article{Rahmani2017,
	title = {Observational signatures of a warped disk associated with cold-flow accretion},
	url = {http://arxiv.org/abs/1710.08398},
	abstract = {We present {MUSE} observations of the field of the quasar Q0152\$-\$020 whose spectrum shows a Lyman limit system ({LLS}) at redshift \$z\_\{{\textbackslash}rm abs\} = 0.38\$, with a metallicity Z \${\textbackslash}gtrsim 0.06\$ Z\$\_{\textbackslash}odot\$. The low ionization metal lines associated with the {LLS} present two narrow distinct absorption components with a velocity separation of 26 km \$\{{\textbackslash}rm s\}{\textasciicircum}\{-1\}\$. We detect six galaxies within 600 km \$\{{\textbackslash}rm s\}{\textasciicircum}\{-1\}\$ from the absorption redshift; their projected distances from the quasar sightline range from 60 to 200 kpc. The optical spectra of five of these galaxies exhibit prominent nebular emission lines, from which we deduce extinction-corrected star formation rates in the range {SFR} = 0.06-1.3 M\$\_{\textbackslash}odot\${\textasciitilde}yr\${\textasciicircum}\{-1\}\$, and metallicities between 0.2 Z\$\_{\textbackslash}odot\$ and Z\$\_{\textbackslash}odot\$. The sixth galaxy is only detected in the stellar continuum. By combining our data with archival Keck/{HIRES} spectroscopy of the quasar and {HST}/{WFPC}2 imaging of the field, we can relate absorption line and galaxy kinematics; we conclude that the {LLS} is most likely associated with the galaxy closest to the quasar sight-line (galaxy "a"). Our morphokinematic analysis of galaxy "a" combined with the absorption line kinematics supports the interpretation that one of the absorption components originates from an extension of the stellar disk of galaxy "a", while the other component may arise in accreting gas in a warped disk with specific angular momentum \${\textbackslash}sim 3\$ times larger than the specific angular momentum of the galaxy halo. Such warped disks are common features in hydrodynamical simulations of cold-flow accretion onto galaxies; the data presented here provide observational evidence in favour of this scenario.},
	author = {Rahmani, Hadi and Péroux, Céline and Augustin, Ramona and Husemann, Bernd and Kacprzak, Glenn G. and Kulkarni, Varsha and Milliard, Bruno and Møller, Palle and Pettini, Max and Straka, Lorrie and Vernet, Joël and York, Donald G.},
	urldate = {2017-11-09},
	date = {2017-10-23},
	eprinttype = {arxiv},
	eprint = {1710.08398},
	file = {PDF:/Users/zhafen/Zotero/storage/PK99DE85/Rahmani et al. - 2017 - Observational signatures of a warped disk associated with cold-flow accretion.pdf:application/pdf},
}

@article{Penny2017,
	title = {{SDSS}-{IV} {MaNGA}: Evidence of the importance of {AGN} feedback in low-mass galaxies},
	url = {http://arxiv.org/abs/1710.07568},
	abstract = {We present new evidence for {AGN} feedback in a subset of 69 quenched low-mass galaxies (\$M\_\{{\textbackslash}star\} {\textbackslash}lesssim 5{\textbackslash}times10{\textasciicircum}\{9\}\$ M\$\_\{{\textbackslash}odot\}\$, \$M\_\{{\textbackslash}rm\{r\}\} {\textgreater} -19\$) selected from the first two years of the {SDSS}-{IV} {MaNGA} survey. The majority (85 per cent) of these quenched galaxies appear to reside in a group environment. We find 6 galaxies in our sample that appear to have an active {AGN} that is preventing on-going star-formation; this is the first time such a feedback mechanism has been observed in this mass range. Interestingly, five of these six galaxies have an ionised gas component that is kinematically offset from their stellar component, suggesting the gas is either recently accreted or outflowing. We hypothesise these six galaxies are low-mass equivalents to the "red geysers" observed in more massive galaxies. Of the other 62 galaxies in the sample, we find 8 do appear for have some low-level, residual star formation, or emission from hot, evolved stars. The remaining galaxies in our sample have no detectable ionised gas emission throughout their structures, consistent with them being quenched. This work shows the potential for understanding the detailed physical properties of dwarf galaxies through spatially resolved spectroscopy.},
	author = {Penny, Samantha J. and Masters, Karen L. and Smethurst, Rebecca and Nichol, Robert C. and Krawczyk, Coleman M. and Bizyaev, Dmitry and Rembold, Sandro B. and Riffel, Rogemar A. and Ilha, Gabriele da Silva and Andrews, Brett H. and Bundy, Kevin and Drory, Niv and Oravetz, Daniel and Pan, Kaike},
	urldate = {2017-11-09},
	date = {2017-10-20},
	eprinttype = {arxiv},
	eprint = {1710.07568},
	file = {PDF:/Users/zhafen/Zotero/storage/KWVAGVUA/Penny et al. - 2017 - SDSS-IV MaNGA Evidence of the importance of AGN feedback in low-mass galaxies.pdf:application/pdf},
}

@article{Li2017,
	title = {The Circum-Galactic Medium of {MASsive} Spirals {II}: Probing the Nature of Hot Gaseous Halo around the Most Massive Isolated Spiral Galaxies},
	url = {http://arxiv.org/abs/1710.07355},
	abstract = {We present the analysis of the {XMM}-Newton data of the Circum-Galactic Medium of {MASsive} Spirals ({CGM}-{MASS}) sample of six extremely massive spiral galaxies in the local Universe. All the {CGM}-{MASS} galaxies have diffuse X-ray emission from hot gas detected above the background extending \${\textbackslash}sim(30-100){\textbackslash}rm{\textasciitilde}kpc\$ from the galactic center. This doubles the existing detection of such extended hot {CGM} around massive spiral galaxies. The radial soft X-ray intensity profile of hot gas can be fitted with a \${\textbackslash}beta\$-function with the slope typically in the range of \${\textbackslash}beta=0.35-0.55\$. This range, as well as those \${\textbackslash}beta\$ values measured for other massive spiral galaxies, including the Milky Way ({MW}), are in general consistent with X-ray luminous elliptical galaxies of similar hot gas luminosity and temperature, and with those predicted from a hydrostatic isothermal gaseous halo. Hot gas in such massive spiral galaxy tends to have temperature comparable to its virial value, indicating the importance of gravitational heating. This is in contrast to lower mass galaxies where hot gas temperature tends to be systematically higher than the virial one. The ratio of the radiative cooling to free fall timescales of hot gas is much larger than the critical value of \${\textbackslash}sim10\$ throughout the entire halos of all the {CGM}-{MASS} galaxies, indicating the inefficiency of gas cooling and precipitation in the {CGM}. The hot {CGM} in these massive spiral galaxies is thus most likely in a hydrostatic state, with the feedback material mixed with the {CGM}, instead of escaping out of the halo or falling back to the disk. We also homogenize and compare the halo X-ray luminosity measured for the {CGM}-{MASS} galaxies and other galaxy samples and discuss the "missing" galactic feedback detected in these massive spiral galaxies.},
	author = {Li, Jiang-Tao and Bregman, Joel N. and Wang, Q. Daniel and Crain, Robert A. and Anderson, Michael E. and Zhang, Shangjia},
	urldate = {2017-11-09},
	date = {2017-10-19},
	eprinttype = {arxiv},
	eprint = {1710.07355},
	file = {PDF:/Users/zhafen/Zotero/storage/8JZWXB3G/Li et al. - 2017 - The Circum-Galactic Medium of MASsive Spirals II Probing the Nature of Hot Gaseous Halo around the Most Massive Isola.pdf:application/pdf},
}

@article{Bogdan2017,
	title = {Probing the Hot X-ray Corona around the Massive Spiral Galaxy, {NGC} 6753, Using Deep {XMM}-Newton observations},
	url = {http://arxiv.org/abs/1710.07286},
	abstract = {X-ray emitting gaseous coronae around massive galaxies are a basic prediction of galaxy formation models. Although the coronae around spiral galaxies offer a fundamental test of these models, observational constraints on their characteristics are still scarce. While the presence of extended hot coronae has been established around a handful of massive spiral galaxies, the short X-ray observations only allowed to measure the basic characteristics of the coronae. In this work, we utilize deep {XMM}-Newton observations of {NGC} 6753 to explore its extended X-ray corona in unprecedented detail. Specifically, we establish the isotropic morphology of the hot gas, suggesting that it resides in hydrostatic equilibrium. The temperature profile of the gas shows a decrease with increasing radius: it drops from \${kT}{\textbackslash}approx0.7\$ {keV} in the innermost parts to \${kT}{\textbackslash}approx0.4\$ {keV} at 50 kpc radius. The temperature map reveals the complex temperature structure of the gas. We study the metallicity distribution of the gas, which is uniform at \$Z{\textbackslash}approx0.1\$ Solar. This value is about an order of magnitude lower than that obtained for elliptical galaxies with similar dark matter halo mass, hinting that the hot gas in spiral galaxies predominantly originates from external gas inflows rather than from internal sources. By extrapolating the density profile of the hot gas out to the virial radius, we estimate the total gas mass and derive the total baryon mass of {NGC} 6753. We conclude that the baryon mass fraction is \$f\_\{{\textbackslash}rm b\} {\textbackslash}approx 0.06\$, implying that about half of the baryons are missing.},
	author = {Bogdan, Akos and Bourdin, Herve and Forman, William R. and Kraft, Ralph P. and Vogelsberger, Mark and Hernquist, Lars and Springel, Volker},
	urldate = {2017-11-09},
	date = {2017-10-19},
	eprinttype = {arxiv},
	eprint = {1710.07286},
	file = {PDF:/Users/zhafen/Zotero/storage/V7JT9YKF/Bogdan et al. - 2017 - Probing the Hot X-ray Corona around the Massive Spiral Galaxy, NGC 6753, Using Deep XMM-Newton observations.pdf:application/pdf},
}

@article{Petkova2017,
	title = {Fast and accurate Voronoi density gridding from Lagrangian hydrodynamics data},
	url = {http://arxiv.org/abs/1710.07108},
	doi = {10.1016/j.jcp.2017.10.024},
	abstract = {Voronoi grids have been successfully used to represent density structures of gas in astronomical hydrodynamics simulations. While some codes are explicitly built around using a Voronoi grid, others, such as Smoothed Particle Hydrodynamics ({SPH}), use particle-based representations and can benefit from constructing a Voronoi grid for post-processing their output. So far, calculating the density of each Voronoi cell from {SPH} data has been done numerically, which is both slow and potentially inaccurate. This paper proposes an alternative analytic method, which is fast and accurate. We derive an expression for the integral of a cubic spline kernel over the volume of a Voronoi cell and link it to the density of the cell. Mass conservation is ensured rigorously by the procedure. The method can be applied more broadly to integrate a spherically symmetric polynomial function over the volume of a random polyhedron.},
	author = {Petkova, Maya A. and Laibe, Guillaume and Bonnell, Ian A.},
	urldate = {2017-11-09},
	date = {2017-10-19},
	eprinttype = {arxiv},
	eprint = {1710.07108},
}

@article{Chisholm2017,
	title = {Feeding the fire: Tracing the mass-loading of 10{\textasciicircum}7 K galactic outflows with O {VI} absorption},
	url = {http://arxiv.org/abs/1710.06857},
	abstract = {Galactic outflows regulate the amount of gas galaxies convert into stars. However, it is difficult to measure the mass outflows remove because they span a large range of temperatures and phases. Here, we study the rest-frame ultraviolet spectrum of a lensed galaxy at z{\textasciitilde}2.9 with prominent interstellar absorption lines from O I, tracing neutral gas, up to O {VI}, tracing transitional phase gas. The O {VI} profile mimics weak low-ionization profiles at low velocities, and strong saturated profiles at high velocities. These trends indicate that O {VI} gas is co-spatial with the low-ionization gas. Further, at velocities blueward of -200 km/s the column density of the low-ionization outflow rapidly drops while the O {VI} column density rises, suggesting that O {VI} is created as the low-ionization gas is destroyed. Photoionization models do not reproduce the observed O {VI}, but adequately match the low-ionization gas, indicating that the phases have different formation mechanisms. Photoionized outflows are more massive than O {VI} outflows for most of the observed velocities, although the O {VI} mass outflow rate exceeds the photoionized outflow at velocities above the galaxy's escape velocity. Therefore, most gas capable of escaping the galaxy is in a hot outflow phase. We suggest that the O {VI} absorption is a temporary by-product of conduction transferring mass from the photoionized phase to an unobserved hot wind, and discuss how this mass-loading impacts the observed circum-galactic medium.},
	author = {Chisholm, J. and Bordoloi, R. and Rigby, J. R. and Bayliss, M.},
	urldate = {2017-11-09},
	date = {2017-10-18},
	eprinttype = {arxiv},
	eprint = {1710.06857},
	file = {PDF:/Users/zhafen/Zotero/storage/LCCMG2NY/Chisholm et al. - 2017 - Feeding the fire Tracing the mass-loading of 107 K galactic outflows with O VI absorption.pdf:application/pdf},
}

@article{Johnson2017,
	title = {The Extent of Chemically Enriched Gas around Star-forming Dwarf Galaxies},
	volume = {850},
	issn = {2041-8213},
	url = {http://arxiv.org/abs/1710.06441},
	doi = {10.3847/2041-8213/aa9370},
	abstract = {Supernova driven winds are often invoked to remove chemically enriched gas from dwarf galaxies to match their low observed metallicities. In such shallow potential wells, outflows may produce massive amounts of enriched halo gas (circum-galactic medium or {CGM}) and pollute the intergalactic medium ({IGM}). Here, we present a survey of the {CGM} and {IGM} around \$18\$ star-forming field dwarfs with stellar masses of \${\textbackslash}log{\textbackslash},M\_*/M\_{\textbackslash}odot{\textbackslash}approx8-9\$ at \$z{\textbackslash}approx0.2\$. Eight of these have {CGM} probed by quasar absorption spectra at projected distances, \$d\$, less than the host virial radius, \$R\_\{{\textbackslash}rm h\}\$. Ten are probed in the surrounding {IGM} at \$d/R\_\{{\textbackslash}rm h\}=1-3\$. The absorption measurements include neutral hydrogen, the dominant silicon ions for diffuse cool gas (\$T{\textbackslash}sim10{\textasciicircum}4\$ K; Si {II}, Si {III}, and Si {IV}), moderately ionized carbon (C {IV}), and highly ionized oxygen (O {VI}). Metal absorption from the {CGM} of the dwarfs is less common and \${\textbackslash}approx4{\textbackslash}times\$ weaker compared to massive star-forming galaxies though O {VI} absorption is still common. None of the dwarfs probed at \$d/R\_\{{\textbackslash}rm h\}=1-3\$ have definitive metal-line detections. Combining the available silicon ions, we estimate that the cool {CGM} of the dwarfs accounts for only \$2-6{\textbackslash}\%\$ of the expected silicon budget from the yields of supernovae associated with past star-formation. The highly ionized O {VI} accounts for \${\textbackslash}approx8{\textbackslash}\%\$ of the oxygen budget. As O {VI} traces an ion with expected equilibrium ion fractions of \${\textbackslash}lesssim 0.2\$, the highly ionized {CGM} may represent a significant metal reservoir even for dwarfs not expected to maintain gravitationally shock heated hot halos.},
	pages = {L10},
	number = {1},
	journaltitle = {The Astrophysical Journal},
	author = {Johnson, Sean D. and Chen, Hsiao-Wen and Mulchaey, John S. and Schaye, Joop and Straka, Lorrie A.},
	urldate = {2017-11-09},
	date = {2017-11-15},
	eprinttype = {arxiv},
	eprint = {1710.06441},
	file = {PDF:/Users/zhafen/Zotero/storage/2JVGUISP/Johnson et al. - 2017 - The Extent of Chemically Enriched Gas around Star-forming Dwarf Galaxies.pdf:application/pdf},
}

@article{Frank2017,
	title = {Agnostic Stacking of Intergalactic Doublet Absorption: Measuring the {NeVIII} Population},
	url = {http://arxiv.org/abs/1710.05023},
	abstract = {We present a blind search for doublet intergalactic metal absorption with a method we dub `agnostic stacking'. Using forward-modelling we combine it with direct detections in the literature to measure the overall metal population. Here we apply this novel approach to the search for {NeVIII} in 26 high-quality {COS} spectra of {QSOs} at z{\textgreater}0.7. We probe an unprecedented low limit of log N{\textgreater}12.3 at 0.47{\textless}z{\textless}1.34 with a total pathlength \$\{{\textbackslash}Delta\}\$z = 7.36. The method selects absorption without requiring knowledge of its source, be it observing noise, artifacts, or any line transition. Stacking this mixed population with {NeVIII} absorption dilutes doublet features in composite spectra in a deterministic manner. We stack potential {NeVIII} absorption in two regimes: absorption too weak to be statistically significant in direct line studies (12.3 {\textless} log N{\textless} 13.7), and strong absorbers (log N{\textgreater} 13.7). We do not detect {NeVIII} in either regime, and place upper limits on the population using agnostic stacking alone. Combining our measurements with direct line detections, the {NeVIII} population is reproduced with a single power law column density distribution of slope {\textbackslash}b\{eta\} = -1.86 and normalisation log f\_\{13.7\} = -13.99, leading to an incidence rate of strong {NeVIII} absorbers of dn/dz =1.38. Comparing our results with a group of 3 systems in {PG}1148+549, these have a 0.024\% probability of arising by chance. We infer a cosmic mass density for {NeVIII} in the column density range 12.3 {\textless} log N {\textless} 15.0 of \$\{{\textbackslash}Omega\}\$({NeVIII}) = 2.2x10{\textasciicircum}\{-8\}. We translate this inferred density into an estimate of the baryon density of the {NeVIII}-bearing gas, and arrive at \$\{{\textbackslash}Omega\}b{\textasciitilde}1.8x10{\textasciicircum}\{-3\}\$, which constitutes only 4\% of the total baryonic mass. The measured {NeVIII} column density distribution function and cosmic density here are inconsistent with predictions of the {EAGLES} simulations at \$\{{\textbackslash}sigma\}{\textgreater}2.0\$ significance. (abridged)},
	author = {Frank, Stephan and Pieri, Matthew M. and Mathur, Smita and Danforth, Charles W. and Shull, J. Michael},
	urldate = {2017-11-09},
	date = {2017-10-13},
	eprinttype = {arxiv},
	eprint = {1710.05023},
	file = {PDF:/Users/zhafen/Zotero/storage/3YGPY5YG/Frank et al. - 2017 - Agnostic Stacking of Intergalactic Doublet Absorption Measuring the NeVIII Population.pdf:application/pdf},
}

@article{Shull2017,
	title = {An Ultraviolet Survey of Low-redshift Partial Lyman-limit Systems with the {HST} Cosmic Origins Spectrograph},
	volume = {849},
	issn = {1538-4357},
	url = {http://arxiv.org/abs/1710.03232},
	doi = {10.3847/1538-4357/aa9229},
	abstract = {We present an ultraviolet spectroscopic survey of strong H I absorbers in the intergalactic medium, probing their evolution over the last 6-7 Gyr at redshifts \$0.24 {\textbackslash}leq z {\textbackslash}leq 0.84\$. We measure column densities \$N\_\{{\textbackslash}rm {HI}\} {\textbackslash},( \{{\textbackslash}rm cm\}{\textasciicircum}\{-2\})\$ from the pattern of Lyman-series absorption lines and flux decrement at the Lyman limit ({LL}) when available. We analyzed 220 H I absorbers in ultraviolet spectra of 102 active galactic nuclei ({AGN}) taken by the Cosmic Origins Spectrograph aboard the Hubble Space Telescope with G130M/G160M gratings (1134-1795 {\textbackslash}{AA}). For 158 absorbers with \${\textbackslash}log N\_\{{\textbackslash}rm {HI}\} {\textbackslash}geq 15\$, the mean frequency is \$d \{{\textbackslash}cal N\}/dz = 4.95 {\textbackslash}pm 0.39\$ over pathlength \${\textbackslash}Delta z = 31.94\$ (\$0.24 {\textbackslash}leq z {\textbackslash}leq 0.84)\$. We identify 8 Lyman Limit Systems ({LLS}, \${\textbackslash}log N\_\{{\textbackslash}rm {HI}\} {\textbackslash}geq 17.2\$) and 54 partial systems ({pLLS}) with \$16.0 {\textbackslash}leq {\textbackslash}log N\_\{{\textbackslash}rm {HI}\} {\textless} 17.2\$. Toward 159 {AGN} between \$0.01 {\textless} z\_\{{\textbackslash}rm abs\} {\textless} 0.84\$ with \${\textbackslash}Delta z {\textbackslash}approx 48\$, we find four damped Ly\${\textbackslash}alpha\$ absorbers ({DLA}) with \$(d \{{\textbackslash}cal N\}/dz)\_\{{\textbackslash}rm {DLA}\} = 0.083{\textasciicircum}\{+0.066\}\_\{-0.040\}\$ at \${\textbackslash}langle z {\textbackslash}rangle = 0.18\$. The mean {LLS} frequency between \$z = 0.24-0.48\$ is \$(d \{{\textbackslash}cal N\}/dz)\_\{{\textbackslash}rm {LLS}\} = 0.36{\textasciicircum}\{+0.20\}\_\{-0.13\}\$ fitted to \$N(z) = (0.25{\textasciicircum}\{+0.13\}\_\{-0.09\})(1+z){\textasciicircum}\{1.14\}\$. For 54 {pLLS} we find \$(d \{{\textbackslash}cal N\}/dz)\_\{{\textbackslash}rm {pLLS}\} = 1.69{\textbackslash}pm0.23\$ at \${\textbackslash}langle z {\textbackslash}rangle = 0.39\$, a frequency consistent with gaseous halo sizes \$R {\textbackslash}approx 100 h{\textasciicircum}\{-1\}{\textasciitilde}\{{\textbackslash}rm kpc\}\$ for (\$0.3-3L{\textasciicircum}*\$) galaxies. A maximum-likelihood analysis yields a distribution \$f(N,z) = C\_0 N{\textasciicircum}\{-{\textbackslash}beta\} (1+z){\textasciicircum}\{{\textbackslash}gamma\}\$ with \${\textbackslash}beta = 1.48 {\textbackslash}pm 0.05\$ and \${\textbackslash}gamma = 1.14{\textasciicircum}\{+0.88\}\_\{-0.89\}\$ for \$15 {\textbackslash}leq {\textbackslash}log N\_\{{\textbackslash}rm {HI}\} {\textbackslash}leq 17.5\$. The far-{UV} opacity gradient is \$d {\textbackslash}tau\_\{{\textbackslash}rm eff\} / dz {\textbackslash}approx (0.444)(1+z){\textasciicircum}\{1.14\}\$ over the range \$15 {\textbackslash}leq {\textbackslash}log N\_\{{\textbackslash}rm {HI}\} {\textbackslash}leq 17\$, implying mean {LyC} optical depth \${\textbackslash}tau\_\{{\textbackslash}rm eff\} {\textbackslash}approx 0.3-0.5\$ toward sources at \$z = 1-2\$.},
	pages = {106},
	number = {2},
	journaltitle = {The Astrophysical Journal},
	author = {Shull, J. Michael and Danforth, Charles W. and Tilton, Evan M. and Moloney, Joshua and Stevans, Matthew L.},
	urldate = {2017-11-09},
	date = {2017-11-07},
	eprinttype = {arxiv},
	eprint = {1710.03232},
	file = {PDF:/Users/zhafen/Zotero/storage/UHXUVFQ4/Shull et al. - 2017 - An Ultraviolet Survey of Low-redshift Partial Lyman-limit Systems with the HST Cosmic Origins Spectrograph.pdf:application/pdf},
}

@article{Toba2017,
	title = {Ionized gas outflows in infrared-bright dust-obscured galaxies selected with {WISE} and {SDSS}},
	url = {http://arxiv.org/abs/1710.02525},
	abstract = {We present the ionized gas properties of infrared ({IR})-bright dust-obscured galaxies ({DOGs}) that show an extreme optical/{IR} color, \$(i - [22])\_\{{\textbackslash}rm {AB}\} {\textgreater} 7.0\$, selected with the Sloan Digital Sky Survey ({SDSS}) and Wide-field Infrared Survey Explorer ({WISE}). For 36 {IR}-bright {DOGs} that show [{OIII}]\${\textbackslash}lambda\$5007 emission in the {SDSS} spectra, we performed a detailed spectral analysis to investigate their ionized gas properties. In particular, we measured the velocity offset (the velocity with respect to the systemic velocity measured from the stellar absorption lines) and the velocity dispersion of the [{OIII}] line. We found that the derived velocity offset and dispersion of most {IR}-bright {DOGs} are larger than those of Seyfert 2 galaxies (Sy2s) at \$z {\textless} 0.3\$, meaning that the {IR}-bright {DOGs} show relatively strong outflows compared to Sy2s. This can be explained by the difference of {IR} luminosity contributed from active galactic nucleus, \$L\_\{{\textbackslash}rm {IR}\}\$ ({AGN}), because we found that (i) \$L\_\{{\textbackslash}rm {IR}\}\$ ({AGN}) correlates with the velocity offset and dispersion of [{OIII}] and (ii) our {IR}-bright {DOGs} sample has larger \$L\_\{{\textbackslash}rm {IR}\}\$ ({AGN}) than Sy2s. Nevertheless, the fact that about 75\% {IR}-bright {DOGs} have a large (\${\textgreater}\$ 300 km s\${\textasciicircum}\{-1\}\$) velocity dispersion, which is a larger fraction compared to other {AGN} populations, suggests that {IR}-bright {DOGs} are good laboratories to investigate {AGN} feedback. The velocity offset and dispersion of [{OIII}] and [{NeIII}]\${\textbackslash}lambda\$3869 are larger than those of [{OII}]\${\textbackslash}lambda\$3727, which indicates that the highly ionized gas tends to show more stronger outflows.},
	author = {Toba, Yoshiki and Bae, Hyun-Jin and Nagao, Tohru and Woo, Jong-Hak and Wang, Wei-Hao and Wagner, Alexander Y. and Sun, Ai-Lei and Chang, Yu-Yen},
	urldate = {2017-11-09},
	date = {2017-10-06},
	eprinttype = {arxiv},
	eprint = {1710.02525},
	file = {PDF:/Users/zhafen/Zotero/storage/4B3SRC6L/Toba et al. - 2017 - Ionized gas outflows in infrared-bright dust-obscured galaxies selected with WISE and SDSS.pdf:application/pdf},
}

@article{Gonzalez-Alfonso2017,
	title = {Feedback and feeding in the context of galaxy evolution with {SPICA}: direct characterization of molecular outflows and inflows},
	url = {http://arxiv.org/abs/1710.02356},
	abstract = {A far-infrared observatory such as the \{{\textbackslash}it {SPace} Infrared telescope for Cosmology and Astrophysics\} (\{{\textbackslash}it {SPICA}\}), with its unprecedented spectroscopic sensitivity, would unveil the role of feedback in galaxy evolution during the last \${\textbackslash}sim10\$ Gyr of the Universe (\$z=1.5-2\$), through the use of far- and mid-infrared molecular and ionic fine structure lines that trace outflowing and infalling gas. Outflowing gas is identified in the far-infrared through P-Cygni line shapes and absorption blueshifted wings in molecular lines with high dipolar moments, and through emission line wings of fine-structure lines of ionized gas. We quantify the detectability of galaxy-scale massive molecular and ionized outflows as a function of redshift in {AGN}-dominated, starburst-dominated, and main-sequence galaxies, explore the detectability of metal-rich inflows in the local Universe, and describe the most significant synergies with other current and future observatories that will measure feedback in galaxies via complementary tracers at other wavelengths.},
	author = {González-Alfonso, E. and Armus, L. and Carrera, F. J. and Charmandaris, V. and Efstathiou, A. and Egami, E. and Fernández-Ontiveros, J. A. and Fischer, J. and Granato, G. L. and Gruppioni, C. and Hatziminaoglou, E. and Imanishi, M. and Isobe, N. and Kaneda, H. and Koziel-Wierzbowska, D. and Malkan, M. A. and Martin-Pintado, J. and Mateos, S. and Matsuhara, H. and Miniutti, G. and Nakagawa, T. and Pozzi, F. and Rico-Villas, F. and Rodighiero, G. and Roelfsema, P. and Spinoglio, L. and Spoon, H. W. W. and Sturm, E. and van der Tak, F. and Vignali, C. and Wang, L.},
	urldate = {2017-11-09},
	date = {2017-10-06},
	eprinttype = {arxiv},
	eprint = {1710.02356},
	file = {PDF:/Users/zhafen/Zotero/storage/2ILW72UX/González-Alfonso et al. - 2017 - Feedback and feeding in the context of galaxy evolution with SPICA direct characterization of molecula.pdf:application/pdf},
}

@article{Liang2018,
	title = {Observing the circumgalactic medium of simulated galaxies through synthetic absorption spectra},
	volume = {479},
	issn = {0035-8711},
	url = {http://arxiv.org/abs/1710.00411},
	doi = {10.1093/mnras/sty1668},
	abstract = {We explore the multiphase structure of the circumgalactic medium ({CGM}) probed by synthetic spectra through a cosmological zoom-in galaxy formation simulation. We employ a Bayesian method for modelling a combination of absorption lines to derive physical properties of absorbers with a formal treatment of detections, including saturated systems, and non-detections in a uniform manner. We find that in the lines of sight passing through localized density structures, absorption lines of low, intermediate and high ions are present in the spectrum and overlap in velocity space. Low, intermediate and high ions can be combined to derive the mass-weighted properties of a density-varying peak, although the ions are not co-spatial within the structure. By contrast, lines of sight that go through the hot halo only exhibit detectable {HI} and high ions. In such lines of sight, the absorption lines are typically broad due to the complex velocity fields across the entire halo. We show that the derived gas density, temperature, and metallicity match closely the corresponding {HI} mass-weighted averages along the {LOS}. We also show that when the data quality allows, our Bayesian technique allows one to recover the underlying physical properties of {LOS} by incorporating both detections and non-detections. It is especially useful to include non-detections, of species such as {NV} or {NeVIII}, when the number of detections of strong absorbers, such as {HI} and {OVI}, is smaller than the number of model parameters (density, temperature, and metallicity).},
	pages = {1822--1835},
	number = {2},
	journaltitle = {Monthly Notices of the Royal Astronomical Society},
	author = {Liang, Cameron J and Kravtsov, Andrey V and Agertz, Oscar},
	urldate = {2017-11-09},
	date = {2018-09-11},
	eprinttype = {arxiv},
	eprint = {1710.00411},
	file = {PDF:/Users/zhafen/Zotero/storage/62XXTLPZ/Liang, Kravtsov, Agertz - 2018 - Observing the circumgalactic medium of simulated galaxies through synthetic absorption spectra.pdf:application/pdf},
}

@article{Cai2017,
	title = {Probing the Metal Enrichment of the Intergalactic Medium at \$z=5-6\$ Using the Hubble Space Telescope},
	url = {http://arxiv.org/abs/1709.10111},
	doi = {10.3847/2041-8213/aa8fc6},
	abstract = {We test the galactic outflow model by probing associated galaxies of four strong intergalactic {CIV} absorbers at \$z=5\$--6 using the Hubble Space Telescope ({HST}) {ACS} ramp narrowband filters. The four strong {CIV} absorbers reside at \$z=5.74\$, \$5.52\$, \$4.95\$, and \$4.87\$, with column densities ranging from \$N\_\{{\textbackslash}rm\{{CIV}\}\}=10{\textasciicircum}\{13.8\}\$ cm\${\textasciicircum}\{-2\}\$ to \$10{\textasciicircum}\{14.8\}\$ cm\${\textasciicircum}\{-2\}\$. At \$z=5.74\$, we detect an i-dropout Ly\${\textbackslash}alpha\$ emitter ({LAE}) candidate with a projected impact parameter of 42 physical kpc from the {CIV} absorber. This {LAE} candidate has a Ly\${\textbackslash}alpha\$-based star formation rate ({SFR}\$\_\{{\textbackslash}rm\{Ly{\textbackslash}alpha\}\}\$) of 2 \$M\_{\textbackslash}odot\$ yr\${\textasciicircum}\{-1\}\$ and a {UV}-based {SFR} of 4 \$M\_{\textbackslash}odot\$ yr\${\textasciicircum}\{-1\}\$. Although we cannot completely rule out that this \$i\$-dropout emitter may be an [{OII}] interloper, its measured properties are consistent with the {CIV} powering galaxy at \$z=5.74\$. For {CIV} absorbers at \$z=4.95\$ and \$z=4.87\$, although we detect two {LAE} candidates with impact parameters of 160 kpc and 200 kpc, such distances are larger than that predicted from the simulations. Therefore we treat them as non-detections. For the system at \$z=5.52\$, we do not detect {LAE} candidates, placing a 3-\${\textbackslash}sigma\$ upper limit of {SFR}\$\_\{{\textbackslash}rm\{Ly{\textbackslash}alpha\}\}{\textbackslash}approx 1.5{\textbackslash} M\_{\textbackslash}odot\$ yr\${\textasciicircum}\{-1\}\$. In summary, in these four cases, we only detect one plausible {CIV} source at \$z=5.74\$. Combining the modest {SFR} of the one detection and the three non-detections, our {HST} observations strongly support that smaller galaxies ({SFR}\$\_\{{\textbackslash}rm\{Ly{\textbackslash}alpha\}\} {\textbackslash}lesssim 2{\textbackslash} M\_{\textbackslash}odot\$ yr\${\textasciicircum}\{-1\}\$) are main sources of intergalactic {CIV} absorbers, and such small galaxies play a major role in the metal enrichment of the intergalactic medium at \$z{\textbackslash}gtrsim5\$.},
	author = {Cai, Zheng and Fan, Xiaohui and Dave, Romeel and Finlator, Kristian and Oppenheimer, Ben},
	urldate = {2017-11-09},
	date = {2017-09-28},
	eprinttype = {arxiv},
	eprint = {1709.10111},
	file = {PDF:/Users/zhafen/Zotero/storage/YSBVTSVR/Cai et al. - 2017 - Probing the Metal Enrichment of the Intergalactic Medium at \$z=5-6\$ Using the Hubble Space Telescope.pdf:application/pdf},
}

@article{Bland-Hawthorn2017,
	title = {In search of cool flow accretion onto galaxies \$-\$ where does the disk gas end?},
	url = {http://arxiv.org/abs/1709.08733},
	doi = {10.3847/1538-4357/aa8f45},
	abstract = {The processes taking place in the outermost reaches of spiral disks (the 'proto-disk') are intimately connected to the build-up of mass and angular momentum in galaxies. The thinness of spiral disks suggests that the activity is mostly quiescent and presumably this region is fed by cool flows coming into the halo from the intergalactic medium. While there is abundant evidence for the presence of a circumgalactic medium ({CGM}) around disk galaxies as traced by quasar absorption lines, it has been very difficult to connect this material to the outer gas disk. This has been a very difficult transition region to explore because baryon tracers are hard to observe. In particular, {HI} disks have been argued to truncate at a critical column density N(H) \${\textbackslash}approx 3{\textbackslash}times 10{\textasciicircum}\{19\}\$ cm\${\textasciicircum}\{-2\}\$ at 30 kpc for an L* galaxy where the gas is vulnerable to the external ionizing background. But new deep observations of nearby L* spirals (e.g. Milky Way, {NGC} 2997) suggest that {HI} disks may extend much further than recognised to date, up to 60 kpc at N(H) \${\textbackslash}approx 10{\textasciicircum}\{18\}\$ cm\${\textasciicircum}\{-2\}\$. Motivated by these observations, here we show that a clumpy outer disk of dense clouds or cloudlets is potentially detectable to much larger radii and lower {HI} column densities than previously discussed. This extended proto-disk component is likely to explain some of the {MgII} forest seen in quasar spectra as judged from absorption-line column densities and kinematics. We fully anticipate that the armada of new radio facilities and planned {HI} surveys coming online will detect this extreme outer disk (scree) material. We also propose a variant on the successful 'Dragonfly' technique to go after the very weak H\${\textbackslash}alpha\$ signals expected in the proto-disk region.},
	author = {Bland-Hawthorn, Joss and Maloney, Phil and Stephens, Alex and Zovaro, Anna and Popping, Attila},
	urldate = {2017-11-09},
	date = {2017-09-25},
	eprinttype = {arxiv},
	eprint = {1709.08733},
	file = {PDF:/Users/zhafen/Zotero/storage/SHKZXNTD/Bland-Hawthorn et al. - 2017 - In search of cool flow accretion onto galaxies \$-\$ where does the disk gas end.pdf:application/pdf},
}

@article{Hanawa2017,
	title = {Fragmentation of Filamentary Cloud Permeated by Perpendicular Magnetic Field},
	url = {http://arxiv.org/abs/1709.05149},
	doi = {10.3847/1538-4357/aa8b6d},
	abstract = {We examine the linear stability of an isothermal filamentary cloud permeated by a perpendicular magnetic field. Our model cloud is assumed to be supported by gas pressure against the self-gravity in the unperturbed state. For simplicity, the density distribution is assumed to be symmetric around the axis. Also for simplicity, the initial magnetic field is assumed to be uniform and turbulence is not taken into account. The perturbation equation is formulated to be an eigenvalue problem. The growth rate is obtained as a function of the wavenumber for fragmentation along the axis and the magnetic field strength. The growth rate depends critically on the outer boundary. If the displacement vanishes in the region very far from the cloud axis (fixed boundary), cloud fragmentation is suppressed by a moderate magnetic field, which means the plasma beta is below 1.67 on the cloud axis. If the displacement is constant along the magnetic field in the region very far from the cloud, the cloud is unstable even when the magnetic field is infinitely strong. The cloud is deformed by circulation in the plane perpendicular to the magnetic field. The unstable mode is not likely to induce dynamical collapse, since it is excited even when the whole cloud is magnetically subcritical. For both the boundary conditions the magnetic field increases the wavelength of the most unstable mode. We find that the magnetic force suppresses compression perpendicular to the magnetic field especially in the region of low density.},
	author = {Hanawa, Tomoyuki and Kudoh, Takahiro and Tomisaka, Kohji},
	urldate = {2017-11-09},
	date = {2017-09-15},
	eprinttype = {arxiv},
	eprint = {1709.05149},
	file = {PDF:/Users/zhafen/Zotero/storage/NANAKFD3/Hanawa, Kudoh, Tomisaka - 2017 - Fragmentation of Filamentary Cloud Permeated by Perpendicular Magnetic Field.pdf:application/pdf},
}

@article{Sorini2017,
	title = {A Fundamental Test for Galaxy Formation Models: Matching the Lyman-\${\textbackslash}alpha\$ Absorption Profiles of Galactic Halos over Three Decades in Distance},
	url = {http://arxiv.org/abs/1709.03988},
	abstract = {Galaxy formation depends critically on the physical state of gas in the circumgalactic medium ({CGM}) and its interface with the intergalactic medium ({IGM}), determined by the complex interplay between inflows from the {IGM} and outflows from supernovae or {AGN} feedback. The average Lyman-alpha (Ly-a) absorption profile around galactic halos represents a powerful tool to probe their gaseous environments. We compare predictions from Illustris and Nyx hydrodynamical simulations with the observed absorption around foreground quasars, damped Ly-a systems, and Lyman-break galaxies. We show how large-scale {BOSS} and small-scale quasar pair measurements can be combined to precisely constrain the absorption profile over three decades in transverse distance 20kpc\${\textbackslash}lesssim b{\textbackslash}lesssim\$20Mpc. Far from galaxies \${\textbackslash}gtrsim2\$Mpc, the simulations converge to the same profile and provide a reasonable match to the observations. This asymptotic agreement arises because the \${\textbackslash}Lambda\${CDM} model successfully describes the ambient {IGM}, and represents a critical advantage of studying the mean absorption profile. However, significant differences between the simulations, and between simulations and observations are present on scales 20kpc\${\textbackslash}lesssim b{\textbackslash}lesssim\$2Mpc, illustrating the challenges of accurately modeling and resolving galaxy formation physics. It is noteworthy that these differences are observed as far out as \${\textbackslash}sim2\$Mpc, indicating that the `sphere-of-influence' of galaxies could extend to approximately \${\textbackslash}sim20\$ times the halo virial radius (\${\textbackslash}sim100\$kpc). Current observations are very precise on these scales and can thus strongly discriminate between different galaxy formation models. We demonstrate that the Ly-a absorption profile is primarily sensitive to the underlying temperature-density relationship of diffuse gas around galaxies, and argue that it thus provides a fundamental test of galaxy formation models.},
	author = {Sorini, Daniele and Oñorbe, José and Hennawi, Joseph F. and Lukić, Zarija},
	urldate = {2017-11-09},
	date = {2017-09-12},
	eprinttype = {arxiv},
	eprint = {1709.03988},
	file = {PDF:/Users/zhafen/Zotero/storage/ISPT3YJI/Sorini et al. - 2017 - A Fundamental Test for Galaxy Formation Models Matching the Lyman-\$alpha\$ Absorption Profiles of Galactic Halos o.pdf:application/pdf},
}

@article{Tescari2017,
	title = {The {SAMI} Galaxy Survey: understanding observations of large-scale outflows at low redshift with {EAGLE} simulations},
	url = {http://arxiv.org/abs/1709.01939},
	doi = {10.1093/mnras/stx2315},
	abstract = {This work presents a study of galactic outflows driven by stellar feedback. We extract main sequence disc galaxies with stellar mass \$10{\textasciicircum}9{\textbackslash}le\$ M\$\_\{{\textbackslash}star\}/\$M\$\_\{{\textbackslash}odot\} {\textbackslash}le 5.7{\textbackslash}times10{\textasciicircum}\{10\}\$ at redshift \$z=0\$ from the highest resolution cosmological simulation of the Evolution and Assembly of {GaLaxies} and their Environments ({EAGLE}) set. Synthetic gas rotation velocity and velocity dispersion (\${\textbackslash}sigma\$) maps are created and compared to observations of disc galaxies obtained with the Sydney-{AAO} Multi-object Integral field spectrograph ({SAMI}), where \${\textbackslash}sigma\$-values greater than \$150\$ km s\${\textasciicircum}\{-1\}\$ are most naturally explained by bipolar outflows powered by starburst activity. We find that the extension of the simulated edge-on (pixelated) velocity dispersion probability distribution depends on stellar mass and star formation rate surface density (\${\textbackslash}Sigma\_\{{\textbackslash}rm {SFR}\}\$), with low-M\$\_\{{\textbackslash}star\}/\$low-\${\textbackslash}Sigma\_\{{\textbackslash}rm {SFR}\}\$ galaxies showing a narrow peak at low \${\textbackslash}sigma\$ (\${\textbackslash}sim30\$ km s\${\textasciicircum}\{-1\}\$) and more active, high-M\$\_\{{\textbackslash}star\}/\$high-\${\textbackslash}Sigma\_\{{\textbackslash}rm {SFR}\}\$ galaxies reaching \${\textbackslash}sigma{\textgreater}150\$ km s\${\textasciicircum}\{-1\}\$. Although supernova-driven galactic winds in the {EAGLE} simulations may not entrain enough gas with T \${\textless}10{\textasciicircum}5\$ K compared to observed galaxies, we find that gas temperature is a good proxy for the presence of outflows. There is a direct correlation between the thermal state of the gas and its state of motion as described by the \${\textbackslash}sigma\$-distribution. The following equivalence relations hold in {EAGLE}: \$i)\$ low-\${\textbackslash}sigma\$ peak \${\textbackslash},{\textbackslash}Leftrightarrow{\textbackslash},\$ disc of the galaxy \${\textbackslash},{\textbackslash}Leftrightarrow{\textbackslash},\$ gas with T \${\textless}10{\textasciicircum}5\$ K; \$ii)\$ high-\${\textbackslash}sigma\$ tail \${\textbackslash},{\textbackslash}Leftrightarrow{\textbackslash},\$ galactic winds \${\textbackslash},{\textbackslash}Leftrightarrow{\textbackslash},\$ gas with T \${\textbackslash}ge 10{\textasciicircum}5\$ K.},
	author = {Tescari, E. and Cortese, L. and Power, C. and Wyithe, J. S. B. and Ho, I. -T. and Crain, R. A. and Bland-Hawthorn, J. and Croom, S. M. and Kewley, L. J. and Schaye, J. and Bower, R. G. and Theuns, T. and Schaller, M. and Barnes, L. and Brough, S. and Bryant, J. J. and Goodwin, M. and Gunawardhana, M. L. P. and Lawrence, J. S. and Leslie, S. K. and López-Sánchez, Á. R. and Lorente, N. P. F. and Medling, A. M. and Richards, S. N. and Sweet, S. M. and Tonini, C.},
	urldate = {2017-11-09},
	date = {2017-09-06},
	eprinttype = {arxiv},
	eprint = {1709.01939},
	file = {PDF:/Users/zhafen/Zotero/storage/WIMPA4BN/Tescari et al. - 2017 - The SAMI Galaxy Survey understanding observations of large-scale outflows at low redshift with EAGLE simulations.pdf:application/pdf},
}

@article{correa2017,
	title = {The formation of hot gaseous haloes around galaxies},
	volume = {473},
	url = {http://arxiv.org/abs/1709.01938},
	doi = {10.1093/mnras/stx2332},
	abstract = {We use a suite of hydrodynamical cosmological simulations from the Evolution and Assembly of {GaLaxies} and their Environments ({EAGLE}) project to investigate the formation of hot hydrostatic haloes and their dependence on feedback mechanisms. We find that the appearance of a strong bimodality in the probability density function ({PDF}) of the ratio of the radiative cooling and dynamical times for halo gas provides a clear signature of the formation of a hot corona. Haloes of total mass \$10{\textasciicircum}\{11.5\}-10{\textasciicircum}\{12\}\{{\textbackslash}rm\{M\}\}\_\{{\textbackslash}odot\}\$ develop a hot corona independent of redshift, at least in the interval \$z=0-4\$ where the simulation has sufficiently good statistics. We analyse the build up of the hot gas mass in the halo, \$M\_\{{\textbackslash}rm\{hot\}\}\$, as a function of halo mass and redshift and find that while more energetic galactic winds powered by {SNe} increases \$M\_\{{\textbackslash}rm\{hot\}\}\$, {AGN} feedback reduces it by ejecting gas from the halo. We also study the thermal properties of gas accreting onto haloes and measure the fraction of shock-heated gas as a function of redshift and halo mass. We develop analytic and semianalytic approaches to estimate a `critical halo mass', \$M\_\{{\textbackslash}rm\{crit\}\}\$, for hot halo formation. We find that the mass for which the heating rate produced by accretion shocks equals the radiative cooling rate, reproduces the mass above which haloes develop a significant hot atmosphere. This yields a mass estimate of \$M\_\{{\textbackslash}rm\{crit\}\} {\textbackslash}approx 10{\textasciicircum}\{11.7\}\{{\textbackslash}rm\{M\}\}\_\{{\textbackslash}odot\}\$ at \$z=0\$, which agrees with the simulation results. The value of \$M\_\{{\textbackslash}rm\{crit\}\}\$ depends more strongly on the cooling rate than on any of the feedback parameters.},
	pages = {538--559},
	number = {1},
	author = {Correa, Camila A. and Schaye, Joop and Wyithe, J. Stuart B. and Duffy, Alan R. and Theuns, Tom and Crain, Robert A. and Bower, Richard},
	urldate = {2017-11-09},
	date = {2017-09-06},
	eprinttype = {arxiv},
	eprint = {1709.01938},
	keywords = {Astrophysics - Astrophysics of Galaxies},
	file = {arXiv Fulltext PDF:/Users/zhafen/Zotero/storage/8XCWZMII/Correa et al. - 2018 - The formation of hot gaseous haloes around galaxie.pdf:application/pdf;PDF:/Users/zhafen/Zotero/storage/IIYRLTUN/Correa et al. - 2017 - The formation of hot gaseous haloes around galaxies.pdf:application/pdf},
}

@article{Mathews2017,
	title = {Circumgalactic Oxygen Absorption and Feedback},
	volume = {846},
	issn = {2041-8213},
	url = {http://arxiv.org/abs/1708.07140},
	doi = {10.3847/2041-8213/aa8861},
	abstract = {{OVI} absorption in quasar spectra caused by intervening circumgalactic atmospheres suggests a downturn in the atmospheric column density in sightlines passing beyond about 100 kpc from central star-forming galaxies. This turnover supports the hypothesis that the oxygen originates in the central galaxies. When converted into oxygen space density using an Abel integral inversion, the {OVI} columns require greater than \${\textbackslash}approx 10{\textasciicircum}9 M\_{\textbackslash}odot\$ of oxygen concentrated near 100 kpc. Circumgalactic gas within this radius cools in less than 1 Gyr and radiates \${\textbackslash}sim 10{\textasciicircum}\{42.2\}\$ erg s\${\textasciicircum}\{-1\}\$ overall. The feedback power necessary to maintain such oxygen-rich atmospheres for many Gyrs cannot be easily supplied by galactic supernovae. However, massive central black holes in star-forming galaxies may generate sufficient accretion power and intermittent shock waves at \$r {\textbackslash}sim 100\$ kpc to balance circumgalactic radiation losses in late-type \$L{\textasciicircum}{\textbackslash}star\$ galaxies. The relative absence of {OVI} absorption observed in early-type, passive \$L{\textasciicircum}\{{\textbackslash}star\}\$ galaxies may arise from enhanced {AGN} feedback from their more massive central black holes.},
	pages = {L24},
