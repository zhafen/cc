@article{Davies2018,
	title = {Kiloparsec Scale Properties of Star-Formation Driven Outflows at z{\textasciitilde}2.3 in the {SINS}/{zC}-{SINF} {AO} Survey},
	url = {https://arxiv.org/abs/1808.10700},
	abstract = {We investigate the relationship between star formation activity and outflow properties on kiloparsec scales in a sample of 28 star forming galaxies at \$z{\textbackslash}sim\$ 2-2.6, using adaptive optics assisted integral field observations from {SINFONI} on the {VLT}. The narrow and broad components of the H\${\textbackslash}alpha\$ emission are used to simultaneously determine the local star formation rate surface density (\${\textbackslash}Sigma\_\{{\textbackslash}rm {SFR}\}\$), and the outflow velocity \$v\_\{{\textbackslash}rm out\}\$ and mass outflow rate \${\textbackslash}dot\{M\}\_\{{\textbackslash}rm out\}\$, respectively. We find clear evidence for faster outflows with larger mass loading factors at higher \${\textbackslash}Sigma\_\{{\textbackslash}rm {SFR}\}\$. The outflow velocities scale as \$v\_\{{\textbackslash}rm out\} {\textbackslash}propto {\textbackslash}Sigma\_\{{\textbackslash}rm {SFR}\}{\textasciicircum}\{0.28 {\textbackslash}pm 0.10\}\$, which suggests that the outflows may be driven by a combination of mechanical energy released by supernova explosions and stellar winds, as well as radiation pressure acting on dust grains. The majority of the outflowing material does not have sufficient velocity to escape from the galaxy halos, but will likely be re-accreted and contribute to the chemical enrichment of the galaxies. The outflow component contains on average \${\textbackslash}sim\$35\% of the H\${\textbackslash}alpha\$ flux at \${\textbackslash}Sigma\_\{{\textbackslash}rm {SFR}\} {\textbackslash}gtrsim{\textasciitilde}1.3{\textasciitilde}\{{\textbackslash}rm M\}\_{\textbackslash}odot{\textasciitilde}\{{\textbackslash}rm yr\}{\textasciicircum}\{-1\}{\textasciitilde}\{{\textbackslash}rm kpc\}{\textasciicircum}\{-2\}\$, but becomes less prominent at lower \${\textbackslash}Sigma\_\{{\textbackslash}rm {SFR}\}\$. The mass loading factor, \${\textbackslash}eta\$ = \${\textbackslash}dot\{M\}\_\{{\textbackslash}rm out\}\$/{SFR}, is positively correlated with the \${\textbackslash}Sigma\_\{{\textbackslash}rm {SFR}\}\$ but is relatively low even at the highest \${\textbackslash}Sigma\_\{{\textbackslash}rm {SFR}\}\$ (\${\textbackslash}eta{\textless}{\textasciitilde}0.3\$, assuming \$n\_e{\textasciitilde}={\textasciitilde}380{\textasciitilde}\{{\textbackslash}rm cm\}{\textasciicircum}\{-3\}\$). This may be in tension with the high \${\textbackslash}eta\$ values (\${\textbackslash}gtrsim\$1) required by cosmological simulations, unless a significant fraction of the outflowing mass is in other gas phases and is able to escape the galaxy halos.},
	author = {Davies, Rebecca L. and Schreiber, Natascha M. Förster and Übler, Hannah and Genzel, Reinhard and Lutz, Dieter and Renzini, Alvio and Tacchella, Sandro and Tacconi, Linda J. and Belli, Sirio and Burkert, Andreas and Carollo, C. Marcella and Davies, Richard I. and Herrera-Camus, Rodrigo and Lilly, Simon J. and Mancini, Chiara and Naab, Thorsten and Nelson, Erica J. and Price, Sedona H. and Shimizu, Thomas Taro and Sternberg, Amiel and Wisnioski, Emily and Wuyts, Stijn},
	urldate = {2018-10-24},
	date = {2018-08-31},
	eprinttype = {arxiv},
	eprint = {1808.10700},
}

