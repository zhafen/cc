	doi = {10.1093/mnras/stz545},
	shorttitle = {{NIHAO} {XIX}},
	abstract = {We have used the {NIHAO} (Numerical Investigation of a Hundred Astrophysical Objects) simulations to explore how supernovae ({SNe}) affect star formation in galaxies. We find that {SN} feedback operates on all scales from the interstellar medium ({ISM}) to several virial radii. {SNe} regulate star formation by preventing condensation of {HI} into H2 and by moving cold neutral gas to the hot {HII} phase. The first effect explains why the cold neutral gas in dwarf galaxies forms stars inefficiently. The second maintains the hot {ISM} of massive galaxies ({HII} vents out at lower masses). At v\_vir ≳ 67 km s{\textasciicircum}\{-1\}, the outflow rate follows the relation: {\textbackslash}dot\{M\}\_out=23 (v\_vir/67 km s{\textasciicircum}\{-1\}){\textasciicircum}\{-4.6\} {SFR}. 20\{\{ per cent\}\}-70\{\{ per cent\}\} of the gas expelled from galaxies escapes from the halo (ejective feedback) but outflows are dominated by cold swept-up gas, most of which falls back on to the galaxy on a \{∼ \} 1 Gyr time-scale. This `fountain feedback' reduces the masses of galaxies by a factor of 2-4, since gas spends half to three quarter of its time in the fountain. Less than 10\{\{ per cent\}\} of the ejected gas mixes with the hot circumgalactic medium and this gas is usually not reaccreted. On scales as large as 6rvir, galactic winds divert the incoming gas from cosmic filaments and prevent it from accreting on to galaxies (pre-emptive feedback). This process is the main reason for the low baryon content of ultradwarves.},
	pages = {2511--2531},
	journaltitle = {Monthly Notices of the Royal Astronomical Society},
	shortjournal = {Monthly Notices of the Royal Astronomical Society},
	author = {Tollet, Édouard and Cattaneo, Andrea and Macciò, Andrea V. and Dutton, Aaron A. and Kang, Xi},
	urldate = {2021-05-06},
	date = {2019-05-01},
	keywords = {galaxies: evolution, galaxies: formation, {ISM}: jets and outflows, supernovae: general},
	file = {Submitted Version:/Users/zhafen/Zotero/storage/GCJ5VTF3/Tollet et al. - 2019 - NIHAO XIX how supernova feedback shapes the galax.pdf:application/pdf},
}

@article{tejos2021a,
	title = {Telltale signs of metal recycling in the circumgalactic medium of a \$z {\textbackslash}sim 0.77\$ galaxy},
	url = {http://arxiv.org/abs/2105.01673},
	abstract = {We present gravitational-arc tomography of the cool-warm enriched circumgalactic medium ({CGM}) of an isolated galaxy at \$z {\textbackslash}approx 0.77\$. Combining {VLT}/{MUSE} adaptive-optics and Magellan/{MagE} echelle spectroscopy we obtain partially-resolved kinematics of Mg{\textasciitilde}\{{\textbackslash}sc ii\} in absorption. The unique arc configuration allows us to probe 42 spatially independent positions, covering the minor and major axes at impact parameters of \${\textbackslash}approx 10-30\$ kpc and \${\textbackslash}approx 60\$ kpc, respectively, plus \$4\$ positions in front of the galaxy. We observe a direct kinematic connection between the cool-warm enriched {CGM} (traced by Mg{\textasciitilde}\{{\textbackslash}sc ii\}) and the interstellar medium (traced by [O{\textasciitilde}\{{\textbackslash}sc ii\}] in emission). This provides evidence for the existence of an extended disc out to tens of kiloparsecs that co-rotates with the galaxy. The Mg{\textasciitilde}\{{\textbackslash}sc ii\} velocity dispersion (\${\textbackslash}sigma {\textbackslash}approx 30-100\$ km s\${\textasciicircum}\{-1\}\$, depending on positions) is of the same order as the modeled galaxy rotational velocity (\$v\_\{{\textbackslash}rm rot\} {\textbackslash}approx 80\$ km s\${\textasciicircum}\{-1\}\$), providing evidence for the presence of a turbulent and pressure-supported {CGM} component. We consider the absorption to be modulated by a galactic-scale outflow, as it offers a natural scenario for the observed line-of-sight dispersion and asymmetric profiles observed against both the arcs and the galaxy. An extended enriched co-rotating disc together with the signatures of a galactic outflow are telltale signs of metal recycling in the \$z{\textbackslash}sim 1\$ {CGM}.},
	journaltitle = {{arXiv}:2105.01673 [astro-ph]},
	author = {Tejos, N. and López, S. and Ledoux, C. and Fernández-Figueroa, A. and Rivas, N. and Sharon, K. and Johnston, E. J. and Florian, M. K. and D'Ago, G. and Katsianis, A. and Barrientos, F. and Berg, T. and Corro-Guerra, F. and Hamel, M. and Moya-Sierralta, C. and Poudel, S. and Rigby, J. R. and Solimano, M.},
	urldate = {2021-05-06},
	date = {2021-05-04},
	eprinttype = {arxiv},
	eprint = {2105.01673},
	keywords = {Astrophysics - Astrophysics of Galaxies},
	file = {arXiv Fulltext PDF:/Users/zhafen/Zotero/storage/2ZG4FAK6/Tejos et al. - 2021 - Telltale signs of metal recycling in the circumgal.pdf:application/pdf;arXiv.org Snapshot:/Users/zhafen/Zotero/storage/J85H5QT6/2105.html:text/html},
}

@article{abe2021,
	title = {Formation of the first galaxies in the aftermath of the first supernovae},
	url = {http://arxiv.org/abs/2105.02612},
	abstract = {We perform high-resolution cosmological hydrodynamic simulations to study the formation of first galaxies that reach the masses of \$10{\textasciicircum}\{8-9\}{\textasciitilde}h{\textasciicircum}\{-1\}{\textasciitilde}M\_{\textbackslash}odot\$ at \$z=9\$. The resolution of the simulations is high enough to resolve minihaloes and allow us to successfully pursue the formation of multiple Population (Pop) {III} stars, their supernova ({SN}) explosions, resultant metal-enrichment of the inter-galactic medium ({IGM}) in the course of the build-up of the system. Metals are ejected into the {IGM} by multiple Pop {III} {SNe}, but some of the metal-enriched gas falls back onto the halo after \${\textbackslash}gtrsim 100{\textasciitilde}{\textbackslash}rm Myr\$. The star formation history of the first galaxy depends sensitively on the initial mass function ({IMF}) of Pop {III} stars. The dominant stellar population transits from Pop {III} to Pop {II} at \$z{\textbackslash}sim 12-15\$ in the case of power-law Pop {III} {IMF}, \$\{{\textbackslash}rm d\}n/\{{\textbackslash}rm d\}M {\textbackslash}propto M{\textasciicircum}\{-2.35\}\$ with the mass range \$10-500{\textasciitilde}M\_{\textbackslash}odot\$. At \$z{\textbackslash}lesssim 12\$, stars are stably formed in the first galaxies with a star formation rate of \${\textbackslash}sim 10{\textasciicircum}\{-3\}\$-\$10{\textasciicircum}\{-1\}{\textasciitilde}M\_{\textbackslash}odot/\{{\textbackslash}rm yr\}\$. In contrast, for the case with a flat {IMF}, gas-deprived first galaxies form due to frequent Pop {III} pair-instability {SNe}, resulting in the suppression of subsequent Pop {II} star formation. In addition, we calculate {UV} continuum, Ly\${\textbackslash}alpha\$- and H\${\textbackslash}alpha\$-line fluxes from the first galaxies. We show that the James Webb Space Telescope will be able to detect both {UV} continuum, Ly\${\textbackslash}alpha\$ and H\${\textbackslash}alpha\$ line emission from first galaxies with halo mass \${\textbackslash}gtrsim 10{\textasciicircum}\{9\}{\textasciitilde}M\_{\textbackslash}odot\$ at \$z {\textbackslash}gtrsim 10\$.},
	journaltitle = {{arXiv}:2105.02612 [astro-ph]},
	author = {Abe, Makito and Yajima, Hidenobu and Khochfar, Sadegh and Vecchia, Claudio Dalla and Omukai, Kazuyuki},
	urldate = {2021-05-07},
	date = {2021-05-06},
	eprinttype = {arxiv},
	eprint = {2105.02612},
	keywords = {Astrophysics - Astrophysics of Galaxies},
	file = {arXiv Fulltext PDF:/Users/zhafen/Zotero/storage/PL9ZK2NJ/Abe et al. - 2021 - Formation of the first galaxies in the aftermath o.pdf:application/pdf;arXiv.org Snapshot:/Users/zhafen/Zotero/storage/DDCZA9AK/2105.html:text/html},
}

@article{soltinsky2021,
