@article{banda-barraganShockmulticloudInteractionsGalactic2020,
	title = {Shock-multicloud interactions in galactic outflows -- {II}. Radiative fractal clouds and cold gas thermodynamics},
	url = {http://arxiv.org/abs/2011.05240},
	abstract = {Galactic winds are crucial to the cosmic cycle of matter, transporting material out of the dense regions of galaxies. Observations show the coexistence of different temperature phases in such winds, which is not easy to explain. We present a set of 3D shock-multicloud simulations that account for radiative heating and cooling at temperatures between \$10{\textasciicircum}2{\textbackslash},{\textbackslash}rm K\$ and \$10{\textasciicircum}7{\textbackslash},{\textbackslash}rm K\$. The interplay between shock heating, dynamical instabilities, turbulence, and radiative heating and cooling creates a complex multi-phase flow with a rain-like morphology. Cloud gas fragments and is continuously eroded, becoming efficiently mixed and mass loaded. The resulting warm mixed gas then cools down and precipitates into new dense cloudlets, which repeat the process. Thus, radiative cooling is able to sustain fast-moving dense gas by aiding condensation of gas from warm clouds and the hot wind. In the ensuing outflow, hot gas with temperatures \${\textbackslash}gtrsim 10{\textasciicircum}6{\textbackslash},{\textbackslash}rm K\$ outruns the warm and cold phases, which reach thermal equilibrium near \${\textbackslash}approx 10{\textasciicircum}4{\textbackslash},{\textbackslash}rm K\$ and \${\textbackslash}approx 10{\textasciicircum}2{\textbackslash},{\textbackslash}rm K\$, respectively. Although the volume filling factor of hot gas is higher in the outflow, most of the mass is concentrated in dense gas cloudlets and filaments with these temperatures. More porous multicloud layers result in more vertically extended outflows, and dense gas is more efficiently produced in more compact layers. The cold phase is not accelerated by ram-pressure, but, instead, precipitates from warm and mixed gas out of thermal equilibrium. This cycle can explain the presence of high-velocity H{\textbackslash},\{{\textbackslash}sc i\} gas with \$N\_\{{\textbackslash}rm H{\textbackslash},\{{\textbackslash}scriptstyle I\}\}=10{\textasciicircum}\{19-21\}{\textbackslash},{\textbackslash}rm cm{\textasciicircum}\{-2\}\$ and \${\textbackslash}Delta v\_\{\{{\textbackslash}rm {FWHM}\}\}{\textbackslash}lesssim37{\textbackslash},{\textbackslash}rm km{\textbackslash},s{\textasciicircum}\{-1\}\$ in the Galactic centre outflow.},
	author = {Banda-Barragán, Wladimir and Brüggen, Marcus and Heesen, Volker and Scannapieco, Evan and Cottle, J'Neil and Federrath, Christoph and Wagner, Alexander Y.},
	urldate = {2020-11-11},
	date = {2020-11-10},
	eprinttype = {arxiv},
	eprint = {2011.05240},
}

@article{jiVirialShocksAre2020,
	title = {Virial shocks are suppressed in cosmic ray-dominated galaxy halos},
	url = {http://arxiv.org/abs/2011.04706},
	abstract = {We study the impact of cosmic rays ({CRs}) on the structure of virial shocks, using a large suite of high-resolution cosmological {FIRE}-2 simulations accounting for {CR} injection by supernovae. In massive (\$M\_\{{\textbackslash}rm halo\} {\textbackslash}gtrsim 10{\textasciicircum}\{11\}{\textbackslash},M\_\{{\textbackslash}odot\}\$), low-redshift (\$z{\textbackslash}lesssim 1-2\$) halos, which are expected to form "hot halos" with slowly-cooling gas in quasi-hydrostatic equilibrium (with a stable virial shock), our simulations without {CRs} do exhibit clear virial shocks. The cooler phase condensing out from inflows becomes pressure-confined to over-dense clumps, embedded in low-density, volume-filling hot gas whose cooling time is much longer than inflow time. The gas thus transitions sharply from cool free-falling inflow, to hot and thermal-pressure supported at approximately the virial radius (\${\textbackslash}approx R\_\{{\textbackslash}rm vir\}\$), and the shock is quasi-spherical. With {CRs}, we previously argued that halos in this particular mass and redshift range build up {CR}-pressure-dominated gaseous halos. Here, we show that when {CR} pressure dominates over thermal pressure, there is no significant virial shock. Instead, inflowing gas is gradually decelerated by the {CR} pressure gradient and the gas is relatively subsonic out to and even beyond \$R\_{\textbackslash}mathrm\{vir\}\$. Rapid cooling also maintains sub-virial temperatures in the inflowing gas within \${\textbackslash}sim R\_{\textbackslash}mathrm\{vir\}\$.},
	author = {Ji, Suoqing and Kereš, Dušan and Chan, T. K. and Stern, Jonathan and Hummels, Cameron B. and Hopkins, Philip F. and Quataert, Eliot and Faucher-Giguère, Claude-André},
	urldate = {2020-11-11},
	date = {2020-11-09},
	eprinttype = {arxiv},
	eprint = {2011.04706},
}

@article{perouxCosmicBaryonMetal2020,
	title = {The Cosmic Baryon and Metal Cycles},
	url = {http://arxiv.org/abs/2011.01935},
	doi = {10.1146/annurev-astro-021820-120014},
	abstract = {Characterizing the relationship between stars, gas, and metals in galaxies is a critical component of understanding the cosmic baryon cycle. We compile contemporary censuses of the baryons in collapsed structures, their chemical make-up and dust content. We show that: The H I mass density of the Universe is well determined to redshifts z {\textasciitilde} 5 and shows minor evolution with time. New observations of molecular hydrogen reveal its evolution mirrors that of the global star formation rate density. The constant cosmic molecular gas depletion timescale points to a universal relationship between gas reservoirs and star formation. The metal mass density in cold gas (\$T {\textless} 10{\textasciicircum}4\$ K) contains virtually all the metals produced by stars for z {\textgreater} 2.5. At lower redshifts, the contributors to the total amount of metals are more diverse; at z {\textless} 1, most of the observed metals are bound in stars. Overall there is little evidence for a "missing metals problem" in modern censuses. We characterize the dust content of neutral gas over cosmic time, finding the dust-to-gas and dust-to-metals ratios fall with decreasing metallicity. We calculate the cosmological dust mass density in the neutral gas up to z {\textasciitilde} 5. There is good agreement between multiple tracers of the dust content of the Universe.},
	author = {Peroux, Celine and Howk, J. Christopher},
	urldate = {2020-11-06},
	date = {2020-11-03},
	eprinttype = {arxiv},
	eprint = {2011.01935},
}

@article{gensiorElephantBathtubWhen2020,
	title = {The Elephant in the Bathtub: when the physics of star formation regulate the baryon cycle of galaxies},
	url = {http://arxiv.org/abs/2011.01235},
	abstract = {In simple models of galaxy formation and evolution, star formation is solely regulated by the amount of gas present in the galaxy. However, it has recently been shown that star formation can be suppressed by galactic dynamics in galaxies that contain a dominant spheroidal component and a low gas fraction. This 'dynamical suppression' is hypothesised to also contribute to quenching gas-rich galaxies at high redshift, but its impact on the galaxy population at large remains unclear. In this paper, we assess the importance of dynamical suppression in the context of gas regulator models of galaxy evolution through hydrodynamic simulations of isolated galaxies, with gas-to-stellar mass ratios of 0.01-0.20 and a range of galactic gravitational potentials from disc-dominated to spheroidal. Star formation is modelled using a dynamics-dependent efficiency per free-fall time, which depends on the virial parameter of the gas. We find that dynamical suppression becomes more effective at lower gas fractions and quantify its impact on the star formation rate as a function of gas fraction and stellar spheroid mass surface density. We combine the results of our simulations with observed scaling relations that describe the change of galaxy properties across cosmic time, and determine the galaxy mass and redshift range where dynamical suppression may affect the baryon cycle. We predict that the physics of star formation can limit and regulate the baryon cycle at low redshifts (\$z {\textbackslash}lesssim 1.4\$) and high galaxy masses (\$M\_\{{\textbackslash}ast\} {\textbackslash}gtrsim 3 {\textbackslash}times 10{\textasciicircum}\{10\}{\textasciitilde}M\_\{{\textbackslash}odot\}\$), where dynamical suppression can drive galaxies off the star formation main sequence.},
	author = {Gensior, Jindra and Kruijssen, J. M. Diederik},
	urldate = {2020-11-06},
	date = {2020-11-02},
	eprinttype = {arxiv},
	eprint = {2011.01235},
}

