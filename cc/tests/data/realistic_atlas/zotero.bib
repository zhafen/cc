	file = {PDF:/Users/zhafen/Zotero/storage/INSQDUSL/Kacprzak et al. - 2015 - THE AZIMUTHAL DEPENDENCE OF OUTFLOWS AND ACCRETION DETECTED USING O vi ABSORPTION.pdf:application/pdf},
