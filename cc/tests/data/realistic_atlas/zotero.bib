	abstract = {We present the results of a survey for intervening {MgII} absorbers in the redshift range z \${\textbackslash}simeq\$2-6 in the foreground of four high redshift quasar spectra, 5.79\${\textbackslash}le z\_\{em\}{\textbackslash}le\$6.133, obtained with the {ESO} {VLT} X-shooter. We identify 24 absorbers at \${\textbackslash}ge 5{\textbackslash}sigma\$ significance in the equivalent width range 0.117\${\textbackslash}le W\_\{2796\}{\textbackslash}le 3.655{\textbackslash}unicode\{{xC}5\}\$ with the highest redshift absorber at \$z=4.89031{\textbackslash}pm4{\textbackslash}times10{\textasciicircum}\{-5\}\$. For weak (\$W\_\{2796\}{\textless}0.3{\textbackslash}unicode\{{xC}5\}\$) systems, we measure an incidence rate \${dN}/dz\$=1.35\${\textbackslash}pm\$0.58 at {\textless}z{\textgreater}=2.34 and find that it almost doubles to \${dN}/dz\$=2.58\${\textbackslash}pm\$0.67 by {\textless}z{\textgreater}=4.81. Weak absorbers exceeds the number expected from an exponential fit to stronger systems (\$W\_\{2796\}{\textgreater}0.3{\textbackslash}unicode\{{xC}5\}\$). We find that there must be significant evolution in the absorption halo properties of {MgII} absorbers with \$W\_\{2796\} {\textgreater}0.1{\textbackslash}unicode\{{xC}5\}\$ by {\textless}z{\textgreater}=4.77 and/or that they are associated with galaxies with luminosities beyond the limits of the current luminosity function at z \${\textbackslash}sim\$5. We find that the incidence rate of strong {MgII} absorbers (\$W\_\{2796\}{\textgreater}1.0{\textbackslash}unicode\{{xC}5\}\$) can be explained if they are associated with galaxies with \$L{\textbackslash}ge0.29L\_\{{\textbackslash}ast\}\$ and/or their covering fraction increases. If they continue to only be associated with galaxies with \$L{\textbackslash}ge0.50L\_\{{\textbackslash}ast\}\$ then their physical cross section (\${\textbackslash}sigma\_\{phys\}\$) increases from 0.015 Mpc\${\textasciicircum}2\$ at z=2.3 to 0.041 Mpc\${\textasciicircum}2\$ at {\textless}z{\textgreater}=4.77. We measure \${\textbackslash}Omega\_\{{MgII}\}\$=2.1\${\textasciicircum}\{+6.3\}\_\{-0.6\}{\textbackslash}times10{\textasciicircum}\{-8\}\$, 1.9\${\textasciicircum}\{+2.9\}\_\{-0.2\}{\textbackslash}times10{\textasciicircum}\{-8\}\$, 3.9\${\textasciicircum}\{+7.1\}\_\{-2.4\}{\textbackslash}times10{\textasciicircum}\{-7\}\$ at {\textless}z{\textgreater}=2.48, 3.41, 4.77, respectively. At {\textless}z{\textgreater}=4.77, \${\textbackslash}Omega\_\{{MgII}\}\$ exceeds the value expected from \${\textbackslash}Omega\_\{{HI}\}\$ estimated from the global metallicity of {DLAs} at z \${\textbackslash}simeq\$4.85 by a factor of \${\textbackslash}sim\$44 suggesting that either {MgII} absorbers trace both ionised and neutral gas and/or are more metal rich than the average {DLA} at this redshift.},
	author = {Codoreanu, Alex and Ryan-Weber, Emma V. and Crighton, Neil H. M. and Becker, George and Pettini, Max and Madau, Piero and Venemans, Bram},
	urldate = {2017-11-09},
	date = {2017-08-01},
	eprinttype = {arxiv},
	eprint = {1708.00304},
	file = {PDF:/Users/zhafen/Zotero/storage/NP9526VM/Codoreanu et al. - 2017 - The comoving mass density of MgII from \$zsim2\$ to \$5.5\$.pdf:application/pdf},
}

@article{Dolag2017,
	title = {Distribution and Evolution of Metals in the Magneticum simulations},
	url = {http://arxiv.org/abs/1708.00027},
	abstract = {Metals are ideal tracers of the baryonic cycle within halos. Their composition is a fossil record connecting the evolution of the various stellar components of galaxies to the interaction with the environment by in- and outflows. The Magneticum simulations allow to study halos across a large range of masses and environments, from massive galaxy clusters containing hundreds of galaxies down to isolated field galaxies. They include a detailed treatment of the chemo-energetic feedback from the stellar component and its evolution as well as feedback from the evolution of supermassive black holes. Following the detailed evolution of various metal species and their relative composition due to continuing enrichment of the {IGM} and {ICM} by {SNIa}, {SNII} and {AGB} winds of the evolving stellar population reveals the complex interplay of local star formation processes, mixing, global baryonic flows, secular galactic evolution and environmental processes. We present results from the Magneticum simulations on the chemical properties of simulated galaxies and galaxy clusters, carefully comparing them to observations. We show that the simulations already reach a very high level of realism within their complex descriptions of the chemo-energetic feedback, successfully reproducing a large number of observed properties and scaling relations. Our simulated galaxies clearly indicate that there are no strong secondary parameters (like star formation rates at fixed redshift) driving the scatter in these scaling relations. The remaining differences clearly point to detailed physical processes which have to be included into future simulations.},
	author = {Dolag, Klaus and Mevius, Emilio and Remus, Rhea-Silvia},
	urldate = {2017-11-09},
	date = {2017-07-31},
	eprinttype = {arxiv},
	eprint = {1708.00027},
	file = {PDF:/Users/zhafen/Zotero/storage/I93DVXIQ/Dolag, Mevius, Remus - 2017 - Distribution and Evolution of Metals in the Magneticum simulations.pdf:application/pdf},
}

@article{Lan2017,
	title = {Mg ii Absorbers: Metallicity Evolution and Cloud Morphology},
	volume = {850},
	issn = {1538-4357},
	url = {http://arxiv.org/abs/1707.09830},
	doi = {10.3847/1538-4357/aa93eb},
	abstract = {Metal abundance and its evolution are studied for Mg {II} quasar absorption line systems from their weak, unsaturated spectral lines using stacked spectra from the archived data of Sloan Digital Sky Survey. They show an abundance pattern that resembles that of Galactic halo or Small Magellanic Cloud, with metallicity [Z/H] showing an evolution from redshift z=2 to 0.5: metallicity becomes approximately solar or even larger at z{\textasciitilde}0. We show that the evolution of the metal abundance traces the cumulative amount of the hydrogen fuel consumed in star formation in galaxies. With the aid of a spectroscopic simulation code, we infer the median gas density of the cloud to be roughly 0.3 \${\textbackslash}rm cm{\textasciicircum}\{-3\}\$, with which the elemental abundance in various ionization stages, in particular C I, is consistently explained. This gas density implies that the size of the Mg {II} clouds is of the order of 0.03 kpc, which suggests that individual Mg {II} clouds around a galaxy are of a baryonic mass typically \${\textbackslash}rm 10{\textasciicircum}\{3\} {\textbackslash}, M\_\{{\textbackslash}odot\}\$. This means that Mg {II} clouds are numerous and `foamy', rather than the large entity that covers a sizable fraction of galaxies with a single cloud.},
	pages = {156},
	number = {2},
	journaltitle = {The Astrophysical Journal},
	author = {Lan, Ting-Wen and Fukugita, Masataka},
	urldate = {2017-11-09},
	date = {2017-11-28},
	eprinttype = {arxiv},
	eprint = {1707.09830},
	file = {PDF:/Users/zhafen/Zotero/storage/MPJRYWJG/Lan, Fukugita - 2017 - Mg ii Absorbers Metallicity Evolution and Cloud Morphology(2).pdf:application/pdf},
}

@article{Spring2017,
	title = {Observational Evidence For Constant Gas Accretion Rate Since z = 5},
	url = {http://arxiv.org/abs/1707.08877},
	doi = {10.1093/mnrasl/slx108},
	abstract = {Star formation rate density ({SFRD}) has not been constant throughout the history of the Universe. The rate at which stars form greatly affects the evolution of the Universe, but the factors which drive {SFRD} evolution remain uncertain. There must be sufficient amount of gas to fuel the star formation, either as a reservoir within a galaxy, or as inflow from the intergalactic medium ({IGM}). This work explores how the gas accretion rate onto galaxies over time has affected star formation rate. We propose a novel method of measuring cosmic gas accretion rate. This involves comparing the comoving densities of available Hi and H2 gas and the densities of existing stars at different redshifts. We constrained gas accretion until z = 5, and we found that the gas accretion rate density ({GARD}) is relatively constant in the range from z = 5 to z = 0. This constancy in the {GARD} is not reflected by the {SFRD}, which declines significantly between z = 1.0 and z = 0. This work suggests that the decline is not due to a reduction in {GARD}.},
	author = {Spring, Eleanor F. and Michałowski, Michał J.},
	urldate = {2017-11-09},
	date = {2017-07-27},
	eprinttype = {arxiv},
	eprint = {1707.08877},
	file = {PDF:/Users/zhafen/Zotero/storage/H2JXBL8D/Spring, Michałowski - 2017 - Observational Evidence For Constant Gas Accretion Rate Since z = 5.pdf:application/pdf},
}

@article{Hopkins2017b,
	title = {How to model supernovae in simulations of star and galaxy formation},
	volume = {477},
	issn = {0035-8711},
	url = {http://arxiv.org/abs/1707.07010},
	doi = {10.1093/mnras/sty674},
	abstract = {We study the implementation of mechanical feedback from supernovae ({SNe}) and stellar mass loss in galaxy simulations, within the Feedback In Realistic Environments ({FIRE}) project. We present the {FIRE}-2 algorithm for coupling mechanical feedback, which can be applied to any hydrodynamics method (e.g. fixed-grid, moving-mesh, and mesh-less methods), and black hole as well as stellar feedback. This algorithm ensures manifest conservation of mass, energy, and momentum, and avoids imprinting 'preferred directions' on the ejecta. We show that it is critical to incorporate both momentum and thermal energy of mechanical ejecta in a self-consistent manner, accounting for {SNe} cooling radii when they are not resolved. Using idealized simulations of single {SNe} explosions, we show that the {FIRE}-2 algorithm, independent of resolution, reproduces converged solutions in both energy and momentum. In contrast, common 'fully-thermal' (energy-dump) or 'fully-kinetic' (particle-kicking) schemes in the literature depend strongly on resolution: when applied at mass resolution \${\textbackslash}gtrsim 100{\textbackslash},M\_\{{\textbackslash}odot\}\$, they diverge by orders-of-magnitude from the converged solution. In galaxy-formation simulations, this divergence leads to orders-of-magnitude differences in galaxy properties, unless those models are adjusted in a resolution-dependent way. We show that all models that individually time-resolve {SNe} converge to the {FIRE}-2 solution at sufficiently high resolution (\${\textless}100{\textbackslash},M\_\{{\textbackslash}odot\}\$). However, in both idealized single-{SNe} simulations and cosmological galaxy-formation simulations, the {FIRE}-2 algorithm converges much faster than other sub-grid models without re-tuning parameters.},
	pages = {1578--1603},
	number = {2},
	journaltitle = {Monthly Notices of the Royal Astronomical Society},
	author = {Hopkins, Philip F. and Wetzel, Andrew and Kereš, Dušan and Faucher-Giguère, Claude-André and Quataert, Eliot and Boylan-Kolchin, Michael and Murray, Norman and Hayward, Christopher C. and El-Badry, Kareem},
	urldate = {2017-11-09},
	date = {2018-06-21},
	eprinttype = {arxiv},
	eprint = {1707.07010},
	file = {PDF:/Users/zhafen/Zotero/storage/JZ2L8PHB/Hopkins et al. - 2018 - How to model supernovae in simulations of star and galaxy formation.pdf:application/pdf},
}

@article{Russell2017,
	title = {360-degree videos: a new visualization technique for astrophysical simulations},
	url = {http://arxiv.org/abs/1707.06954},
	abstract = {360-degree videos are a new type of movie that renders over all 4\${\textbackslash}pi\$ steradian. Video sharing sites such as {YouTube} now allow this unique content to be shared via virtual reality ({VR}) goggles, hand-held smartphones/tablets, and computers. Creating 360\${\textasciicircum}{\textbackslash}circ\$ videos from astrophysical simulations is not only a new way to view these simulations as you are immersed in them, but is also a way to create engaging content for outreach to the public. We present what we believe is the first 360\${\textasciicircum}{\textbackslash}circ\$ video of an astrophysical simulation: a hydrodynamics calculation of the central parsec of the Galactic centre. We also describe how to create such movies, and briefly comment on what new science can be extracted from astrophysical simulations using 360\${\textasciicircum}{\textbackslash}circ\$ videos.},
	author = {Russell, Christopher M. P.},
	urldate = {2017-11-09},
	date = {2017-07-21},
	eprinttype = {arxiv},
	eprint = {1707.06954},
}

@article{Heckman2017a,
	title = {{COS}-Burst: Observations of the Impact of Starburst-Driven Winds on the Properties of the Circum-Galactic Medium},
	url = {http://arxiv.org/abs/1707.05933},
	doi = {10.3847/1538-4357/aa80dc},
	abstract = {We report on observations made with the Cosmic Origins Spectrograph ({COS}) on the Hubble Space Telescope ({HST}) using background {QSOs} to probe the circum-galactic medium ({CGM}) around 17 low-redshift galaxies that are undergoing or have recently undergone a strong starburst (the {COS}-Burst program). The sightlines extend out to roughly the virial radius of the galaxy halo. We construct control samples of normal star-forming low-redshift galaxies from the {COS}/{HST} archive that match the starbursts in terms of galaxy stellar mass and impact parameter. We find clear evidence that the {CGM} around the starbursts differs systematically compared to the control galaxies. The Ly\${\textbackslash}alpha\$, Si {III}, C {IV}, and possibly O {VI} absorption-lines are stronger as a function of impact parameter, and the ratios of the equivalent widths of C {IV}/Ly\${\textbackslash}alpha\$ and Si {III}/Ly\${\textbackslash}alpha\$ are both larger than in normal star-forming galaxies. We also find that the widths and the velocity offsets (relative to \$v\_\{sys\}\$) of the Ly\${\textbackslash}alpha\$ absorption-lines are significantly larger in the {CGM} of the starbursts, implying velocities of the absorbing material that are roughly twice the halo virial velocity. We show that these properties can be understood as a consequence of the interaction between a starburst-driven wind and the pre-existing {CGM}. These results underscore the importance of winds driven from intensely star-forming galaxies in helping drive the evolution of galaxies and the intergalactic medium. They also offer a new probe of the properties of starburst-driven winds and of the {CGM} itself.},
	author = {Heckman, Timothy and Borthakur, Sanchayeeta and Wild, Vivienne and Schiminovich, David and Bordoloi, Rongmon},
	urldate = {2017-11-09},
	date = {2017-07-18},
	eprinttype = {arxiv},
	eprint = {1707.05933},
	file = {PDF:/Users/zhafen/Zotero/storage/NSMDSFK8/Heckman et al. - 2017 - COS-Burst Observations of the Impact of Starburst-Driven Winds on the Properties of the Circum-Galactic Medium.pdf:application/pdf},
}

