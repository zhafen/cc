@article{Yesuf2017,
	title = {No evidence for feedback: Unexceptional Low-ionization winds in Host galaxies of Low Luminosity Active Galactic Nuclei at Redshift z {\textasciitilde}1},
	url = {http://arxiv.org/abs/1704.08348},
	doi = {10.3847/1538-4357/aa6fae},
	abstract = {We study winds in 12 X-ray {AGN} host galaxies at z {\textasciitilde} 1. We find, using the low-ionization Fe {II} 2586 absorption in the stacked spectra, that the probability distribution function ({PDF}) of the centroid velocity shift in {AGN} has a median, 16th and 84th percentiles of (-87, -251, +86) km/s respectively. The {PDF} of the velocity dispersion in {AGN} has a median, 84th and 16th percentile of (139, 253, 52) km/s respectively. The centroid velocity and the velocity dispersions are obtained from a two component ({ISM}+wind) absorption line model. The equivalent width {PDF} of the outflow in {AGN} has median, 84th and 16th percentiles of (0.4, 0.8, 0.1) Angstrom. There is a strong {ISM} component in Fe {II} 2586 absorption with (1.2, 1.5, 0.8) Angstrom, implying presence of substantial amount cold gas in the host galaxies. For comparison, star-forming and X-ray undetected galaxies at a similar redshift, matched roughly in stellar mass and galaxy inclination, have a centroid velocity {PDF} with percentiles of (-74, -258, +90) km/s, and a velocity dispersion {PDF} percentiles of (150, 259, 57) km/s. Thus, winds in the {AGN} are similar to star-formation-driven winds, and are too weak to escape and expel substantial cool gas from galaxies. Our sample doubles the previous sample of {AGN} studied at z {\textasciitilde} 0.5 and extends the analysis to z {\textasciitilde} 1. A joint reanalysis of the z {\textasciitilde} 0.5 {AGN} sample and our sample yields consistent results to the measurements above.},
	author = {Yesuf, Hassen M. and Koo, David C. and Faber, S. M. and Prochaska, J. Xavier and Guo, Yicheng and Liu, F. S. and Cunningham, Emily C. and Coil, Alison L. and Guhathakurta, Puragra},
	urldate = {2017-11-09},
	date = {2017-04-26},
	eprinttype = {arxiv},
	eprint = {1704.08348},
	file = {PDF:/Users/zhafen/Zotero/storage/Y4PPXFMS/Yesuf et al. - 2017 - No evidence for feedback Unexceptional Low-ionization winds in Host galaxies of Low Luminosity Active Galactic Nuc.pdf:application/pdf},
}

@article{Muzahid2017,
	title = {{COS}-Weak: Probing the {CGM} using analogs of weak Mg {II} absorbers at z {\textless} 0.3},
	url = {http://arxiv.org/abs/1709.03999},
	abstract = {We present a sample of 34 weak metal line absorbers at \$z{\textless} 0.3\$ complied via the simultaneous detections (\$3{\textbackslash}sigma\$) of the {SiII}\${\textbackslash}lambda1260\$ and {CII}\${\textbackslash}lambda1334\$ absorption lines, with \$W\_\{r\}\$({SiII})\${\textless}0.2\$ {\textbackslash}{AA}{\textbackslash} and \$W\_\{r\}\$({CII})\${\textless}0.3\$ {\textbackslash}{AA}, in archival {HST}/{COS} spectra. Our sample increases the number of known low-\$z\$ "weak absorbers" by a factor of \${\textgreater}5\$. The column densities of {HI} and low-ionization metal lines obtained from Voigt profile fitting are used to build simple photoionization models using {CLOUDY}. The inferred densities and total hydrogen column densities are in the ranges of \$-3.3 {\textless} {\textbackslash}log n\_\{{\textbackslash}rm H\}/\{{\textbackslash}rm cm{\textasciicircum}\{-3\}\} {\textless} -2.4\$ and \$16.0 {\textless} {\textbackslash}log N\_\{{\textbackslash}rm H\}/\{{\textbackslash}rm cm{\textasciicircum}\{-2\}\}{\textless}20.3\$, respectively. The line of sight thicknesses of the absorbers have a wide range of \${\textbackslash}sim\$1 pc\$-\$50 kpc with a median value of \${\textbackslash}sim\$500 pc. The high-ionization {OVI} absorption, detected in 12/18 cases, always stems from a different gas-phase. Most importantly, 85\% (50\%) of these absorbers show a metallicity of \${\textbackslash}rm [Si/H] {\textgreater} -1.0\$ (0.0). The fraction of systems showing high metallicity (i.e., \${\textbackslash}rm [Si/H]{\textgreater}-1.0\$) in our sample is significantly higher than the {HI}-selected sample (Wotta et al. 2016) and the galaxy-selected sample (Prochaska et al. 2017) of absorbers probing the circum-galactic medium ({CGM}) at similar redshift. A search for galaxies has revealed a significant galaxy-overdensity around these weak absorbers compared to random places with a median impact parameter of 166 kpc to the nearest galaxy. Moreover, we find the presence of multiple galaxies in \${\textbackslash}sim80\$\% of the cases, suggesting group environments. The observed \$d{\textbackslash}mathcal\{N\}/dz\$ of \$0.8{\textbackslash}pm0.2\$ indicates that such metal-enriched, compact, dense structures are ubiquitous in the halos of low-\$z\$ galaxies that are in groups. We suggest that these are transient structures that are related to outflows and/or stripping of metal-rich gas from galaxies.},
	author = {Muzahid, Sowgat and Fonseca, Gloria and Roberts, Amber and Rosenwasser, Benjamin and Richter, Philipp and Narayanan, Anand and Churchill, Chris and Charlton, Jane},
	urldate = {2017-11-09},
