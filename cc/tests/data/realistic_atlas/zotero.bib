	date = {2012-05-01},
	keywords = {cosmology: observations, quasars: general, gravitational lensing: strong},
}

@article{Cooke2006,
	title = {Survey for Galaxies Associated with z ∼ 3 Damped Lyα Systems. {II}. Galaxy‐Absorber Correlation Functions},
	volume = {652},
