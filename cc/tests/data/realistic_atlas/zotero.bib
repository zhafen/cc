@article{lanExploringPhysicalProperties2019,
	title = {Exploring the physical properties of the cool circumgalactic medium with a semi-analytic model},
	volume = {486},
	issn = {0035-8711},
	url = {http://arxiv.org/abs/1810.11771},
	doi = {10.1093/mnras/stz839},
	abstract = {We develop a semi-analytic model to explore the physical properties of cool pressure-confined circumgalactic clouds with mass ranging from \$10\$ to \$10{\textasciicircum}\{8\} {\textbackslash}, {\textbackslash}rm M\_\{{\textbackslash}odot\}\$ in a hot diffuse halo. We consider physical effects that control the motion and mass loss of the clouds, and estimate the lifetime and the observed properties of individual cool gas clouds inferred from the {CLOUDY} simulation. Our results show that the cool pressure-confined gas clouds have physical properties consistent with absorption line systems with neutral hydrogen column densities \$N\_\{{\textbackslash}rm {HI}\}{\textbackslash}geq10{\textasciicircum}\{18.5\} {\textbackslash}rm cm{\textasciicircum}\{-2\}\$ such as strong metal absorbers, sub-{DLAs}, and {DLAs}. The cool circumgalactic clouds are transient due to evaporation and recycling and therefore a constant replenishment is needed to maintain the cool {CGM}. We further model the ensemble properties of the cool {CGM} with clouds originated from outflows, inflows, or/and in-situ formation with a range of initial cloud mass function and velocity distribution. We find that only with a certain combination of parameters, an outflow model can broadly reproduce three cool gas properties around star-forming galaxies simultaneously: the spatial distribution, down-the-barrel outflow absorption, and gas velocity dispersion. Both a constant insitu model and gas inflow model can reproduce the observed covering fractions of high \${\textbackslash}rm N\_\{{HI}\}\$ gas around passive galaxies but they fail to reproduce sufficient number of low \${\textbackslash}rm N\_\{{HI}\}\$ systems. The limitations and the failures of the current models are discussed. Our results illustrate that semi-analytic modeling is a promising tool to understand the physics of the cool {CGM} which is usually unresolved by state-of-the-art cosmological hydrodynamic simulations.},
	pages = {608--622},
	number = {1},
	journaltitle = {Monthly Notices of the Royal Astronomical Society},
	author = {Lan, Ting-Wen and Mo, Houjun},
	urldate = {2020-09-09},
	date = {2019-06-11},
	eprinttype = {arxiv},
	eprint = {1810.11771},
}

@article{lanlandingwenConstrainingMagneticFields2020,
	title = {Constraining magnetic fields in the circumgalactic medium},
	volume = {496},
	issn = {0035-8711},
	url = {http://arxiv.org/abs/2006.09369},
	doi = {10.1093/mnras/staa1750},
	abstract = {We study the properties of magnetic fields in the circumgalactic medium ({CGM}) of z \&lt; 1 galaxies by correlating Faraday rotation measures ({RMs}) of ∼1000 high-redshift radio sources with the foreground galaxy number density estimated from the {DESI} Legacy Imaging Surveys. This method enables us to extract signals of {RMs} contributed by intervening gas around multiple galaxies. Our results show that there is no detectable correlation between the distribution of {RMs} and the number of foreground galaxies, contrary to several previous results. Utilizing the non-detection signals, we estimate 3σ upper limits to the {RMs} from the {CGM} of \${\textbackslash}sim {\textbackslash}!20 {\textbackslash}rm {\textbackslash} rad{\textbackslash}, m{\textasciicircum}\{-2\}\$ within 50 kpc and \${\textbackslash}sim {\textbackslash}!10 {\textbackslash}rm {\textbackslash} rad {\textbackslash}, m{\textasciicircum}\{-2\}\$ at separations of 100 kpc. By adopting a column density distribution of ionized gas obtained from absorption-line measurements, we further estimate the strengths of coherent magnetic fields parallel to the line of sight of \${\textbackslash}lt {\textbackslash}rm 2 {\textbackslash} {\textbackslash}mu G\$ in the {CGM}. We show that the estimated upper limits of {RMs} and magnetic field strengths are sufficient to constrain outputs of recent galaxy magnetohydrodynamic simulations. Finally, we discuss possible causes for the inconsistency between our results and previous works.},
	pages = {3142--3151},
	number = {3},
	journaltitle = {Monthly Notices of the Royal Astronomical Society},
	author = {Lan 藍鼎文, Ting-Wen and Prochaska, J. Xavier},
	urldate = {2020-09-09},
	date = {2020-08-11},
	eprinttype = {arxiv},
	eprint = {2006.09369},
}

