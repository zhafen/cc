@article{Petitjean1993,
	title = {Evidence for structure in the {HI} column density distribution of {QSO} absorbers},
	volume = {262},
	abstract = {The {HI} column density distribution function of {QSO} absorption line systems is inverstigated using recent data with high spectral resolution, and extensive surveys of the Lyman-limit systems and damped Lyman-alpha systems. The hypthesis is that differential distribution function is fitetd by a single power law is rejected at the 99 per cent confidence level. A double power law with a break at N({HI}) = 10{\textasciicircum}16/cm2 also provides a poor fit over the range in which the sample is complete. Whilst there are discontinuities in the distribution there is a clear flattening at N({HI}) = 10{\textasciicircum}16/cm2 , compared to lower column densities. The observed features can be understood using models of photoionized clouds which are confined by an external pressure with density profiles governed by gravity. In particular the flattening can be explained in terms of a transition between metal-poor and metal-rich systems.},
	pages = {499--505},
	journaltitle = {Monthly Notices of the Royal Astronomical Society},
	author = {Petitjean, P and Webb, J.K. and Rauch, M. and Carswell, R.F. and {Lanzetta K.}},
	date = {1993},
	keywords = {Absorption, H I, Include: {LLS} Paper, intergalactic medium, quasars:absorption lines},
}

@article{Liang2015,
	title = {Column density profiles of multiphase gaseous haloes},
	volume = {458},
	issn = {0035-8711},
	url = {http://arxiv.org/abs/1507.07002},
	doi = {10.1093/mnras/stw375},
	abstract = {We analyze circumgalactic medium ({CGM}) in a suite of high-resolution cosmological re-simulations of a Milky-Way size galaxy and show that {CGM} properties are quite sensitive to details of star formation--feedback loop modelling. The simulation that produces a realistic late-type galaxy, fails to reproduce existing observations of the {CGM}. In contrast, simulation that does not produce a realistic galaxy has the predicted {CGM} in better agreement with observations. This illustrates that properties of galaxies and properties of their {CGM} provide strong \$\{{\textbackslash}it complementary\}\$ constraints on the processes governing galaxy formation. Our simulations predict that column density profiles of ions are well described by an exponential function of projected distance \$d\$: \$N {\textbackslash}propto e{\textasciicircum}\{-d/h\_s\}\$. Simulations thus indicate that the sharp drop in absorber detections at larger distances in observations does not correspond to a "boundary" of an ion, but reflects the underlying steep exponential column density profile. Furthermore, we find that ionization energy of ions is tightly correlated with the scale height \$h\_s\$: \$h\_s {\textbackslash}propto E\_\{{\textbackslash}rm ion\}{\textasciicircum}\{0.74\}\$. At \$z {\textbackslash}approx 0\$, warm gas traced by low-ionization species (e.g., Mg {II} and C {IV}) has \$ h\_s {\textbackslash}approx 0.03-0.07 R\_\{{\textbackslash}rm vir\}\$, while higher ionization species (O {VI} and Ne {VIII}) have \$h\_s {\textbackslash}approx 0.32-0.45R\_\{{\textbackslash}rm vir\}\$. Finally, the scale heights of ions in our simulations evolve slower than the virial radius for \$z{\textbackslash}leq 2\$, but similarly to the halo scale radius, \$r\_s\$. Thus, we suggest that the column density profiles of galaxies at different redshifts should be scaled by \$r\_s\$ rather than the halo virial radius.},
	pages = {1164--1187},
	number = {2},
	journaltitle = {Monthly Notices of the Royal Astronomical Society},
	author = {Liang, Cameron J. and Kravtsov, Andrey V. and Agertz, Oscar},
	date = {2016-05-11},
	eprinttype = {arxiv},
	eprint = {1507.07002},
	keywords = {Absorption, H I, Include: {LLS} Paper, galaxies, halos, Metals, {CGM}, cosmology, feedback, simulations, theory},
}

@article{Guimaraes2009,
	title = {Damped and sub-damped Lyman-a absorbers in z \${\textgreater}\$ 4 {QSOs}},
	volume = {508},
	issn = {0004-6361},
	url = {http://www.aanda.org/10.1051/0004-6361/200811541},
	doi = {10.1051/0004-6361/200811541},
	abstract = {We present the results of a survey of damped ({DLA}, log N(H I) {\textgreater} 20.3) and sub-damped Lyman-alpha systems (19.5 {\textless} log N(H I) {\textless} 20.3) at z {\textgreater} 2.55 along the lines-of-sight to 77 quasars with emission redshifts in the range 4 {\textless} z(em) {\textless} 6.3. Intermediate resolution (R similar to 4300) spectra were obtained with the Echellette Spectrograph and Imager ({ESI}) mounted on the Keck telescope. A total of 100 systems with log N(H I) {\textgreater} 19.5 were detected of which 40 systems are damped Lyman-alpha systems for an absorption length of Delta X = 378. About half of the lines of sight of this homogeneous survey have never been investigated for {DLAs}. We study the evolution with redshift of the cosmological density of the neutral gas and find, consistent with previous studies at similar resolution, that Omega({DLA},{HI}) decreases at z {\textgreater} 3.5. The overall cosmological evolution of Omega({HI}) shows a peak around this redshift. The H I column density distribution for log N(H I) {\textgreater}= 20.3 is fitted, consistent with previous surveys, with a single power-law of index alpha similar to -1.8 +/- 0.25. This power-law overpredicts data at the high-end and a second, much steeper, power-law (or a gamma function) is needed. There is a flattening of the function at lower H i column densities with an index of alpha similar to -1.4 for the column density range log N(H I) = 19.5-21. The fraction of H I mass in sub-{DLAs} is of the order of 30\%. The H I column density distribution does not evolve strongly from z similar to 2.5 to z similar to 4.5.},
	pages = {133--140},
	number = {1},
	journaltitle = {Astronomy and Astrophysics},
	author = {Guimarães, R and Petitjean, P and de Carvalho, R. R. and Djorgovski, S G and Noterdaeme, P and Castro, S and da R. Poppe, P. C. and Aghaee, A.},
	date = {2009},
	eprinttype = {arxiv},
	eprint = {0909.4839},
	note = {{ISBN}: 0004-6361},
	keywords = {Absorption, {DLAs}, H I, evolution, formation, High Redshift, quasars, absorption lines, intergalactic medium, cosmology, galaxy, observations},
}

