@article{shardaRoleGasKinematics2021,
	title = {The role of gas kinematics in setting metallicity gradients at high redshift},
	url = {http://arxiv.org/abs/2102.09740},
	abstract = {In this work, we explore the diversity of ionised gas kinematics (rotational velocity \$v\_\{{\textbackslash}phi\}\$ and velocity dispersion \${\textbackslash}sigma\_g\$) and gas-phase metallicity gradients at \$0.1 {\textbackslash}leq z {\textbackslash}leq 2.5\$ using a compiled data set of 74 galaxies resolved with ground-based integral field spectroscopy. We find that galaxies with the highest and the lowest \${\textbackslash}sigma\_g\$ have preferentially flat metallicity gradients, whereas those with intermediate values of \${\textbackslash}sigma\_g\$ show a large scatter in the metallicity gradients. Additionally, steep negative gradients appear almost only in rotation-dominated galaxies (\$v\_\{{\textbackslash}phi\}/{\textbackslash}sigma\_g {\textgreater} 1\$), whereas most dispersion-dominated galaxies show flat gradients. We use our recently developed analytic model of metallicity gradients to provide a physical explanation for these observed trends. In the case of high \${\textbackslash}sigma\_g\$, the inward radial advection of gas dominates over metal production and causes efficient metal mixing, thus giving rise to flat gradients. For low \${\textbackslash}sigma\_g\$, it is the cosmic accretion of metal-poor gas diluting the metallicity that gives rise to flat gradients. Finally, the reason for intermediate \${\textbackslash}sigma\_g\$ showing the steepest negative gradients is that both inward radial advection and cosmic accretion are weak as compared to metal production, which leads to the creation of steeper gradients. The larger scatter at intermediate \${\textbackslash}sigma\_g\$ may be due in part to preferential ejection of metals in galactic winds, which can decrease the strength of the production term. Our analysis shows that gas kinematics plays a critical role in setting metallicity gradients in high-redshift galaxies.},
	author = {Sharda, Piyush and Wisnioski, Emily and Krumholz, Mark R. and Federrath, Christoph},
	urldate = {2021-02-22},
	date = {2021-02-18},
	eprinttype = {arxiv},
	eprint = {2102.09740},
}

