@article{Fumagalli2013,
	title = {Metal abundances in the high-redshift intergalactic medium},
	volume = {85},
	url = {http://arxiv.org/abs/1312.5321},
	abstract = {Twenty years of high-resolution spectroscopy at the 8-10 m class telescopes have drastically expanded our view of the gas-phase metallicity in the z{\textgreater}2 universe. This contribution briefly summarizes how these studies reveal a widespread metal pollution in the intergalactic medium with a median abundance [C/H] {\textasciitilde} -3.5 at z{\textasciitilde}3 that is increasing by a factor of {\textasciitilde}2-3 from z{\textasciitilde}4.3 to z{\textasciitilde}2.4. At the higher densities that are typical of galactic halos, observations uncover a metallicity spread of five orders of magnitude in Lyman limit systems, ranging from super-solar ([M/H]{\textasciitilde}+0.7) to pristine ([M/H]{\textless} -4) gas clouds. Finally, the neutral damped Ly-alpha systems are enriched to a median metallicity of [M/H] {\textasciitilde} -1.5 that slowly declines with redshift up to z {\textasciitilde} 4.5, at which point it appears to more rapidly evolve as one approaches the end of reionization.},
	pages = {355},
	journaltitle = {Memorie della Società Astronomica Italiana},
	author = {Fumagalli, Michele},
	date = {2013},
	eprinttype = {arxiv},
	eprint = {1312.5321},
	keywords = {Absorption, H I, Include: {LLS} Paper, galaxies, High Redshift, quasars, absorption lines, abundances, in-, ism, Metals, primordial nucleosynthesis, spectroscopic, techniques, tergalactic medium},
}

