@article{Ribaudo2011,
	title = {A {HUBBLE} {SPACE} {TELESCOPE} {STUDY} {OF} {LYMAN} {LIMIT} {SYSTEMS}: {CENSUS} {AND} {EVOLUTION}},
	volume = {736},
	issn = {0004-637X},
	url = {http://arxiv.org/abs/1105.0659},
	doi = {10.1088/0004-637X/736/1/42},
	abstract = {We present a survey for optically thick Lyman limit absorbers at z{\textless}2.6 using archival Hubble Space Telescope observations with the Faint Object Spectrograph and Space Telescope Imaging Spectrograph. We identify 206 Lyman limit systems ({LLSs}) increasing the number of catalogued {LLSs} at z{\textless}2.6 by a factor of {\textasciitilde}10. We compile a statistical sample of 50 tau\_LLS {\textgreater} 2 {LLSs} drawn from 249 {QSO} sight lines that avoid known targeting biases. The incidence of such {LLSs} per unit redshift, l(z)=dn/dz, at these redshifts is well described by a single power law, l(z) = C1 (1+z){\textasciicircum}gamma, with gamma=1.33 +/- 0.61 at z{\textless}2.6, or with gamma=1.83 +/- 0.21 over the redshift range 0.2 {\textless} z {\textless} 4.9. The incidence of {LLSs} per absorption distance, l(X), decreases by a factor of {\textasciitilde}1.5 over the {\textasciitilde}0.6 Gyr from z=4.9 to 3.5; l(X) evolves much more slowly at low redshifts, decreasing by a similar factor over the {\textasciitilde}8 Gyr from z=2.6 to 0.25. We show that the column density distribution function, f(N({HI})), at low redshift is not well fitted by a single power law index (f(N({HI})) = C2 N({HI}){\textasciicircum}(-beta)) over the column density range 13 {\textless} log N({HI}) {\textless} 22 or log N({HI}) {\textgreater}17.2. While low and high redshift f(N({HI})) distributions are consistent for log N({HI}){\textgreater}19.0, there is some evidence that f(N({HI})) evolves with z for log N({HI}) {\textless} 17.7, possibly due to the evolution of the {UV} background and galactic feedback. Assuming {LLSs} are associated with individual galaxies, we show that the physical cross section of the optically thick envelopes of galaxies decreased by a factor of {\textasciitilde}9 from z{\textasciitilde}5 to 2 and has remained relatively constant since that time. We argue that a significant fraction of the observed population of {LLSs} arises in the circumgalactic gas of sub-L* galaxies.},
	pages = {42},
	number = {1},
	journaltitle = {The Astrophysical Journal},
	author = {Ribaudo, Joseph and Lehner, Nicolas and Howk, J. Christopher},
	date = {2011-07-20},
