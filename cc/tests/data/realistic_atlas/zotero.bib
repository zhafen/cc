@article{williamsALMAMeasuresRapidly2020,
	title = {{ALMA} measures rapidly depleted molecular gas reservoirs in massive quiescent galaxies at z{\textasciitilde}1.5},
	url = {http://arxiv.org/abs/2012.01433},
	abstract = {We present {ALMA} {CO}(2-1) spectroscopy of 6 massive (log\$\_\{10\}\$M\$\_\{{\textbackslash}rm\{*\}\}/{\textbackslash}rm\{M\}\_{\textbackslash}odot{\textgreater}\$11.3) quiescent galaxies at \$z{\textbackslash}sim1.5\$. These data represent the largest sample using {CO} emission to trace molecular gas in quiescent galaxies above \$z{\textgreater}1\$, achieving an average 3\${\textbackslash}sigma\$ sensitivity of M\$\_\{{\textbackslash}rm\{H\_\{2\}\}\}{\textbackslash}sim10{\textasciicircum}\{10\}{\textbackslash}rm\{M\}\_{\textbackslash}odot\$. We detect one galaxy at 4\${\textbackslash}sigma\$ significance and place upper limits on the molecular gas reservoirs of the other 5, finding molecular gas mass fractions M\$\_\{{\textbackslash}rm\{H\_\{2\}\}\}\$/M\$\_\{{\textbackslash}rm\{*\}\}\$=f\$\_\{{\textbackslash}rm\{H\_\{2\}\}\}{\textless}2-6\$\% (3\${\textbackslash}sigma\$ upper limits). This is 1-2 orders of magnitude lower than coeval star-forming galaxies at similar stellar mass, and comparable to galaxies at \$z=0\$ with similarly low {sSFR}. This indicates that their molecular gas reservoirs were rapidly and efficiently used up or destroyed, and that gas fractions are uniformly low (\${\textless}\$6\%) despite the structural diversity of our sample. The implied rapid depletion time of molecular gas (t\$\_\{{\textbackslash}rm\{dep\}\}{\textless}0.6\$ Gyr) disagrees with extrapolations of empirical scaling relations to low {sSFR}. We find that our low gas fractions are instead in agreement with predictions from both the recent {SIMBA} cosmological simulation, and from analytical "bathtub" models for gas accretion onto galaxies in massive dark matter halos (log\$\_\{10\}M\_\{{\textbackslash}rm\{halo\}\}/{\textbackslash}rm\{M\}\_{\textbackslash}odot{\textbackslash}sim14\$ at \$z=0\$). Such high mass halos reach a critical mass of log\$\_\{10\}M\_\{{\textbackslash}rm\{halo\}\}/{\textbackslash}rm\{M\}\_{\textbackslash}odot{\textgreater}12\$ by \$z{\textbackslash}sim4\$ that halt the accretion of baryons early in the Universe. Our data is consistent with a simple picture where galaxies truncate accretion and then consume the existing gas at or faster than typical main sequence rates. Alternatively, we cannot rule out that these galaxies reside in lower mass halos, and low gas fractions may instead reflect either stronger feedback, or more efficient gas consumption.},
	author = {Williams, Christina C. and Spilker, Justin S. and Whitaker, Katherine E. and Davé, Romeel and Woodrum, Charity and Brammer, Gabriel and Bezanson, Rachel and Narayanan, Desika and Weiner, Benjamin},
	urldate = {2020-12-04},
	date = {2020-12-02},
	eprinttype = {arxiv},
	eprint = {2012.01433},
}

@article{semenovCosmicRayDiffusion2020,
	title = {Cosmic Ray Diffusion Suppression in Star-Forming Regions Inhibits Clump Formation in Gas-Rich Galaxies},
	url = {http://arxiv.org/abs/2012.01427},
	abstract = {Observations of the \${\textbackslash}gamma\$-ray emission around star clusters, isolated supernova remnants, and pulsar wind nebulae indicate that cosmic ray ({CR}) diffusion coefficient near the acceleration sites can be suppressed by a large factor compared to the Galaxy average. We explore the effects of such local suppression of {CR} diffusion on galaxy evolution using simulations of isolated disk galaxies with regular and high gas fractions. Our results show that while {CR} propagation with constant diffusivity can make gaseous disks more stable by increasing the midplane pressure, large-scale {CR} pressure gradients cannot prevent local fragmentation when the disk is unstable. In contrast, when {CR} diffusivity is suppressed in star-forming regions, accumulation of {CRs} in these regions results in strong local pressure gradients that prevent formation of massive gaseous clumps. As a result, the distribution of dense gas and star formation changes qualitatively: a globally unstable gaseous disk does not violently fragment into massive star-forming clumps but maintains a regular grand-design spiral structure. This effect regulates star formation and disk structure and is qualitatively different from and complementary to the global role of {CRs} in vertical hydrostatic support of the gaseous disk and in driving galactic winds.},
	author = {Semenov, Vadim A. and Kravtsov, Andrey V. and Caprioli, Damiano},
	urldate = {2020-12-04},
	date = {2020-12-02},
	eprinttype = {arxiv},
	eprint = {2012.01427},
}

@article{afruniMostCoolCGM2020,
	title = {Most of the cool {CGM} of star-forming galaxies is not produced by supernova feedback},
	url = {https://arxiv.org/abs/2012.00770},
	abstract = {The characterization of the large amount of gas residing in the galaxy halos, the so called circumgalactic medium ({CGM}), is crucial to understand galaxy evolution across cosmic time. We focus here on the the cool (\$T{\textbackslash}sim10{\textasciicircum}4\$ K) phase of this medium around star-forming galaxies in the local universe, whose properties and dynamics are poorly understood. We developed semi-analytical parametric models to describe the cool {CGM} as an outflow of gas clouds from the central galaxy, as a result of supernova explosions in the disc (galactic wind). The cloud motion is driven by the galaxy gravitational pull and by the interactions with the hot (\$T{\textbackslash}sim10{\textasciicircum}6\$ K) coronal gas. Through a bayesian analysis, we compare the predictions of our models with the data of the {COS}-Halos and {COS}-{GASS} surveys, which provide accurate kinematic information of the cool {CGM} around more than 40 low-redshift star-forming galaxies, probing distances up to the galaxy virial radii. Our findings clearly show that a supernova-driven outflow model is not suitable to describe the dynamics of the cool circumgalactic gas. Indeed, to reproduce the data, we need extreme scenarios, with initial outflow velocities and mass loading factors that would lead to unphysically high energy coupling from the supernovae to the gas and with supernova efficiencies largely exceeding unity. This strongly suggests that, since the outflows cannot reproduce most of the cool gas absorbers, the latter are likely the result of cosmological inflow in the outer galaxy halos, in analogy to what we have previously found for early-type galaxies.},
	author = {Afruni, Andrea and Fraternali, Filippo and Pezzulli, Gabriele},
	urldate = {2020-12-03},
	date = {2020-12-01},
	eprinttype = {arxiv},
	eprint = {2012.00770},
}

