@article{pinaBaryonicSpecificAngular2020,
	title = {The baryonic specific angular momentum of disc galaxies},
	url = {http://arxiv.org/abs/2009.06645},
	abstract = {(Abridged) Specific angular momentum is one of the key parameters controlling the evolution of galaxies, and it is closely related with the coupling between dark and visible matter. In this work we aim to derive the baryonic specific angular momentum of disc galaxies and study its relation with the dark matter specific angular momentum. Using a combination of high-quality {HI} rotation curves and {HI}/near-{IR} surface densities, we homogeneously measure the stellar (\$j\_\{{\textbackslash}rm *\}\$) and gas (\$j\_\{{\textbackslash}rm gas\}\$) specific angular momentum for a large sample of nearby disc galaxies. This allows us to determine the baryonic specific angular momentum (\$j\_\{{\textbackslash}rm bar\}\$) with high accuracy and across a very wide range in mass. We confirm that the \$j\_\{{\textbackslash}ast\}-M\_{\textbackslash}ast\$ relation is an unbroken power-law from \$7 {\textbackslash}lesssim\$ log(\$M\_{\textbackslash}ast\$/\$M\_{\textbackslash}odot) {\textbackslash}lesssim 11.5\$, with slope \$0.54 {\textbackslash}pm 0.02\$. Concerning the gas component, we find that the \$j\_\{{\textbackslash}rm gas\}-M\_\{{\textbackslash}rm gas\}\$ relation is also an unbroken power-law from \$6 {\textbackslash}lesssim\$ log(\$M\_\{{\textbackslash}rm gas\}\$/\$M\_{\textbackslash}odot) {\textbackslash}lesssim 11\$ with a steeper slope of \$1.01 {\textbackslash}pm 0.04\$. Regarding the baryonic relation, our data supports a correlation characterized by single power-law with slope \$0.58 {\textbackslash}pm 0.02\$. Our analysis shows that our most massive spirals and smallest dwarfs lie along the same \$j\_\{{\textbackslash}rm bar\}-M\_\{{\textbackslash}rm bar\}\$. While the relations are tight and unbroken, we find internal correlations inside them: at fixed \$M\_{\textbackslash}ast\$, galaxies with larger \$j\_{\textbackslash}ast\$ have larger disc scale lengths, and at fixed \$M\_\{{\textbackslash}rm bar\}\$, gas-poor galaxies have lower \$j\_\{{\textbackslash}rm bar\}\$ than expected. We estimate the retained fraction of baryonic specific angular momentum, finding it constant across all our mass range, with a value of 0.7, indicating that the baryonic specific angular momentum of present-day disc galaxies is comparable to the initial specific angular momentum of their dark matter haloes.},
	author = {Piña, Pavel E. Mancera and Posti, Lorenzo and Fraternali, Filippo and Adams, Elizabeth A. K. and Oosterloo, Tom},
	urldate = {2020-09-16},
	date = {2020-09-14},
	eprinttype = {arxiv},
	eprint = {2009.06645},
}

