@article{Angles-Alcazar2017a,
	title = {Black holes on {FIRE}: stellar feedback limits early feeding of galactic nuclei},
	volume = {472},
	issn = {1745-3925},
	url = {https://academic.oup.com/mnrasl/article/472/1/L109/4349744},
	doi = {10.1093/mnrasl/slx161},
	abstract = {We introduce massive black holes ({BHs}) in the Feedback In Realistic Environments project and perform high-resolution cosmological hydrodynamic simulations of quasar-mass halos (\$M\_\{{\textbackslash}rm halo\}(z=2) {\textbackslash}approx 10{\textasciicircum}\{12.5\}{\textbackslash},{\textbackslash}rm\{M\}\_\{{\textbackslash}odot\}\$) down to \$z=1\$. These simulations model stellar feedback by supernovae, stellar winds, and radiation, and {BH} growth using a gravitational torque-based prescription tied to resolved properties of galactic nuclei. We do not include {BH} feedback. We show that early {BH} growth occurs through short (\${\textbackslash}lesssim 1{\textbackslash},\$Myr) accretion episodes that can reach or even exceed the Eddington rate. In this regime, {BH} growth is limited by bursty stellar feedback continuously evacuating gas from galactic nuclei, and {BHs} remain under-massive relative to the local \$M\_\{{\textbackslash}rm {BH}\}\$-\$M\_\{{\textbackslash}rm bulge\}\$ relation. {BH} growth is more efficient at later times, when the nuclear stellar potential retains a significant gas reservoir, star formation becomes less bursty, and galaxies settle into a more ordered state, with {BHs} rapidly converging onto the scaling relation when the host reaches \$M\_\{{\textbackslash}rm bulge\} {\textbackslash}sim 10{\textasciicircum}\{10\}{\textbackslash},{\textbackslash}rm\{M\}\_\{{\textbackslash}odot\}\$. Our results are not sensitive to the details of the accretion model so long as {BH} growth is tied to the gas content within \${\textbackslash}sim 100{\textbackslash},\$pc of the {BH}. Our simulations imply that bursty stellar feedback has strong implications for {BH} and {AGN} demographics, especially in the early Universe and for low-mass galaxies.},
	pages = {L109--L114},
	number = {1},
	journaltitle = {Monthly Notices of the Royal Astronomical Society: Letters},
	author = {Anglés-Alcázar, Daniel and Faucher-Giguère, Claude-André and Quataert, Eliot and Hopkins, Philip F. and Feldmann, Robert and Torrey, Paul and Wetzel, Andrew and Kereš, Dušan},
	date = {2017-11-21},
	eprinttype = {arxiv},
	eprint = {1707.03832},
	keywords = {Galaxies: Evolution, Galaxies: Formation, Cosmology: Theory, Black hole physics, Galaxies: Active, Quasars: Supermassive black holes},
}

@article{Tumlinson2011,
	title = {The Large, Oxygen-Rich Halos of Star-Forming Galaxies Are a Major Reservoir of Galactic Metals},
	volume = {334},
	issn = {0036-8075},
	url = {http://www.sciencemag.org/cgi/doi/10.1126/science.1209840},
	doi = {10.1126/science.1209840},
	abstract = {The circumgalactic medium ({CGM}) is fed by galaxy outflows and accretion of intergalactic gas, but its mass, heavy element enrichment, and relation to galaxy properties are poorly constrained by observations. In a survey of the outskirts of 42 galaxies with the Cosmic Origins Spectrograph onboard the Hubble Space Telescope, we detected ubiquitous, large (150 kiloparsec) halos of ionized oxygen surrounding star-forming galaxies, but we find much less ionized oxygen around galaxies with little or no star formation. This ionized {CGM} contains a substantial mass of heavy elements and gas, perhaps far exceeding the reservoirs of gas in the galaxies themselves. It is a basic component of nearly all star-forming galaxies that is removed or transformed during the quenching of star formation and the transition to passive evolution.},
	pages = {948--952},
	number = {6058},
	journaltitle = {Science},
	author = {Tumlinson, J. and Thom, C. and Werk, J. K. and Prochaska, J. X. and Tripp, T. M. and Weinberg, D. H. and Peeples, M. S. and O'Meara, J. M. and Oppenheimer, B. D. and Meiring, J. D. and Katz, N. S. and Dave, R. and Ford, A. B. and Sembach, K. R.},
	date = {2011-11-18},
	pmid = {22096191},
	eprinttype = {arxiv},
	eprint = {1111.3980},
	note = {{ISBN}: 1095-9203 (Electronic){\textbackslash}r0036-8075 (Linking)},
}

@article{Johnson2015,
	title = {On the possible environmental effect in distributing heavy elements beyond individual gaseous haloes},
	volume = {449},
	issn = {1365-2966},
	url = {http://academic.oup.com/mnras/article/449/3/3263/2893071/On-the-possible-environmental-effect-in},
	doi = {10.1093/mnras/stv553},
	abstract = {We present a study of extended galaxy halo gas through {HI} and {OVI} absorption over two decades in projected distance at \$z{\textbackslash}approx0.2\$. The study is based on a sample of \$95\$ galaxies from a highly complete (\$ {\textgreater} 80{\textbackslash}\%\$) survey of faint galaxies (\$L {\textgreater} 0.1L\_*\$) with archival quasar absorption spectra and \$53\$ galaxies from the literature. A clear anti-correlation is found between {HI} ({OVI}) column density and virial radius normalized projected distance, \$d/R\_\{{\textbackslash}rm h\}\$. Strong {HI} ({OVI}) absorption systems with column densities greater than \$10{\textasciicircum}\{14.0\}\$ (\$10{\textasciicircum}\{13.5\}\$) cm\${\textasciicircum}\{-2\}\$ are found for \$48\$ of \$54\$ (\$36\$ of \$42\$) galaxies at \$d {\textless} {\textbackslash},R\_\{{\textbackslash}rm h\}\$ indicating a mean covering fraction of \${\textbackslash}langle{\textbackslash}kappa\_\{{\textbackslash}rm {HI}\}{\textbackslash}rangle=0.89\$ (\${\textbackslash}langle{\textbackslash}kappa\_\{{\textbackslash}rm {OVI}\}{\textbackslash}rangle=0.86\$). {OVI} absorbers are found at \$d{\textbackslash}approx R\_\{{\textbackslash}rm h\}\$, beyond the extent observed for lower ionization species. At \$d/R\_\{{\textbackslash}rm h\}=1-3\$ strong {HI} ({OVI}) absorption systems are found for only \$7\$ of \$43\$ (\$5\$ of \$34\$) galaxies (\${\textbackslash}langle{\textbackslash}kappa\_\{{\textbackslash}rm {HI}\}{\textbackslash}rangle=0.16\$ and \${\textbackslash}langle{\textbackslash}kappa\_\{{\textbackslash}rm {OVI}\}{\textbackslash}rangle=0.15\$). Beyond \$d=3{\textbackslash},R\_\{{\textbackslash}rm h\}\$, the {HI} and {OVI} covering fractions decrease to levels consistent with coincidental systems. The high completeness of the galaxy survey enables an investigation of environmental dependence of extended gas properties. Galaxies with nearby neighbors exhibit a modest increase in {OVI} covering fraction at \$d{\textgreater}R\_\{{\textbackslash}rm h\}\$ compared to isolated galaxies (\${\textbackslash}kappa\_\{{\textbackslash}rm {OVI}\}{\textbackslash}approx0.13\$ versus \$0.04\$) but no excess {HI} absorption. These findings suggest that environmental effects play a role in distributing heavy elements beyond the enriched gaseous halos of individual galaxies. Finally, we find that differential {HI} and {OVI} absorption between early- and late-type galaxies continues from \$d {\textless} R\_\{{\textbackslash}rm h\}\$ to \$d{\textbackslash}approx3{\textbackslash},R\_\{{\textbackslash}rm h\}\$.},
	pages = {3263--3273},
	number = {3},
	journaltitle = {Monthly Notices of the Royal Astronomical Society},
	author = {Johnson, Sean D. and Chen, Hsiao-Wen and Mulchaey, John S.},
	date = {2015-05-21},
	eprinttype = {arxiv},
	eprint = {1503.04199},
	keywords = {Galaxies: haloes, Intergalactic medium, Quasars: absorption lines, Galaxies: interactions},
}

@article{El-Badry2018,
	title = {Gas kinematics in {FIRE} simulated galaxies compared to spatially unresolved H i observations},
	volume = {477},
	issn = {0035-8711},
	url = {https://academic.oup.com/mnras/article/477/2/1536/4953425},
	doi = {10.1093/mnras/sty730},
	abstract = {The shape of a galaxy's spatially unresolved, globally integrated 21-cm emission line depends on its internal gas kinematics: galaxies with rotation-supported gas disks produce double-horned profiles with steep wings, while galaxies with dispersion-supported gas produce Gaussian-like profiles with sloped wings. Using mock observations of simulated galaxies from the {FIRE} project, we show that one can therefore constrain a galaxy's gas kinematics from its unresolved 21-cm line profile. In particular, we find that the kurtosis of the 21-cm line increases with decreasing \$V/{\textbackslash}sigma\$, and that this trend is robust across a wide range of masses, signal-to-noise ratios, and inclinations. We then quantify the shapes of 21-cm line profiles from a morphologically unbiased sample of \${\textbackslash}sim\$2000 low-redshift, {HI}-detected galaxies with \$M\_\{{\textbackslash}rm star\} = 10{\textasciicircum}\{7-11\} M\_\{{\textbackslash}odot\}\$ and compare to the simulated galaxies. At \$M\_\{{\textbackslash}rm star\} {\textbackslash}gtrsim 10{\textasciicircum}\{10\} M\_\{{\textbackslash}odot\}\$, both the observed and simulated galaxies produce double-horned profiles with low kurtosis and steep wings, consistent with rotation-supported disks. Both the observed and simulated line profiles become more Gaussian-like (higher kurtosis and less-steep wings) at lower masses, indicating increased dispersion support. However, the simulated galaxies transition from rotation to dispersion support more strongly: at \$M\_\{{\textbackslash}rm star\} = 10{\textasciicircum}\{8-10\}M\_\{{\textbackslash}odot\}\$, most of the simulations produce more Gaussian-like profiles than typical observed galaxies with similar mass, indicating that gas in the low-mass simulated galaxies is, on average, overly dispersion-supported. Most of the lower-mass simulated galaxies also have somewhat lower gas fractions than the median of the observed population. The simulations nevertheless reproduce the observed line-width baryonic Tully-Fisher relation, which is insensitive to rotation vs. dispersion support.},
	pages = {1536--1548},
	number = {2},
	journaltitle = {Monthly Notices of the Royal Astronomical Society},
	author = {El-Badry, Kareem and Bradford, Jeremy and Quataert, Eliot and Geha, Marla and Boylan-Kolchin, Michael and Weisz, Daniel R. and Wetzel, Andrew and Hopkins, Philip F. and Chan, T. K. and Fitts, Alex and Kereš, Dušan and Faucher-Giguère, Claude-André},
	date = {2018-06-21},
	eprinttype = {arxiv},
	eprint = {1801.03933},
	keywords = {Galaxies: Dwarf, Galaxies: Irregular, Galaxies: Kinematics and dynamics},
}

@article{Stern2018,
	title = {Does Circumgalactic O vi Trace Low-pressure Gas Beyond the Accretion Shock? Clues from H i and Low-ion Absorption, Line Kinematics, and Dust Extinction},
	volume = {865},
	issn = {1538-4357},
	url = {http://arxiv.org/abs/1803.05446},
	doi = {10.3847/1538-4357/aac884},
	abstract = {Large {OVI} columns are observed around star-forming, low-redshift {\textasciitilde}L* galaxies, with a dependence on impact parameter indicating that most O{\textasciicircum}5+ particles reside beyond half the halo virial radius ({\textgreater}{\textasciitilde}100 kpc). In order to constrain the nature of the gas traced by {OVI}, we analyze additional observables of the outer halo, namely {HI} to {OVI} column ratios of 1-10, an absence of low-ion absorption, a mean differential extinction of E(B-V){\textasciitilde}10{\textasciicircum}-3, and a linear relation between {OVI} column and velocity width. We contrast these observations with two physical scenarios: (1) {OVI} traces high-pressure ({\textasciitilde}30 cm{\textasciicircum}-3 K) collisionally-ionized gas cooling from a virially-shocked phase, and (2) {OVI} traces low-pressure ({\textless}{\textasciitilde}1 cm{\textasciicircum}-3 K) gas beyond the accretion shock, where the gas is in ionization and thermal equilibrium with the {UV} background. We demonstrate that the high-pressure scenario requires multiple gas phases to explain the observations, and a large deposition of energy at {\textgreater}{\textasciitilde}100 kpc to offset the energy radiated by the cooling gas. In contrast, the low-pressure scenario can explain all considered observations with a single gas phase in thermal equilibrium, provided that the baryon overdensity is comparable to the dark-matter overdensity, and that the gas is enriched to {\textgreater}{\textasciitilde}Z\_sun/3 with an {ISM}-like dust-to-metal ratio. The low-pressure scenario implies that {OVI} traces a cool flow with mass flow rate of {\textasciitilde}5 M\_sun yr{\textasciicircum}-1, comparable to the star formation rate of the central galaxies. The {OVI} line widths are consistent with the velocity shear expected within this flow. The low-pressure scenario predicts a bimodality in absorption line ratios at {\textasciitilde}100 kpc, due to the pressure jump across the accretion shock.},
	pages = {91},
	number = {2},
	journaltitle = {The Astrophysical Journal},
	author = {Stern, Jonathan and Faucher-Giguère, Claude-André and Hennawi, Joseph F. and Hafen, Zachary and Johnson, Sean D. and Fielding, Drummond},
	urldate = {2018-11-12},
	date = {2018-09-25},
	eprinttype = {arxiv},
	eprint = {1803.05446},
}

@article{McClure-Griffiths2018,
	title = {Cold gas outflows from the Small Magellanic Cloud traced with {ASKAP}},
	volume = {2},
	issn = {2397-3366},
	url = {http://arxiv.org/abs/1811.01772},
	doi = {10.1038/s41550-018-0608-8},
	abstract = {Feedback from massive stars plays a critical role in the evolution of the Universe by driving powerful outflows from galaxies that enrich the intergalactic medium and regulate star formation. An important source of outflows may be the most numerous galaxies in the Universe: dwarf galaxies. With small gravitational potential wells, these galaxies easily lose their star-forming material in the presence of intense stellar feedback. Here, we show that the nearby dwarf galaxy, the Small Magellanic Cloud ({SMC}), has atomic hydrogen outflows extending at least 2 kiloparsecs (kpc) from the star-forming bar of the galaxy. The outflows are cold, \$T{\textless}400{\textasciitilde}\{{\textbackslash}rm K\}\$, and may have formed during a period of active star formation \$25 - 60\$ million years (Myr) ago. The total mass of atomic gas in the outflow is \${\textbackslash}sim 10{\textasciicircum}7\$ solar masses, \$\{{\textbackslash}rm M\_\{{\textbackslash}odot\}\}\$, or \${\textbackslash}sim 3\$\% of the total atomic gas of the galaxy. The inferred mass flux in atomic gas alone, \${\textbackslash}dot\{M\}\_\{{HI}\}{\textbackslash}sim 0.2 - 1.0{\textasciitilde}\{{\textbackslash}rm M\_\{{\textbackslash}odot\}{\textasciitilde}yr{\textasciicircum}\{-1\}\}\$, is up to an order of magnitude greater than the star formation rate. We suggest that most of the observed outflow will be stripped from the {SMC} through its interaction with its companion, the Large Magellanic Cloud ({LMC}), and the Milky Way, feeding the Magellanic Stream of hydrogen encircling the Milky Way.},
	pages = {901--906},
	number = {11},
	journaltitle = {Nature Astronomy},
	author = {{McClure}-Griffiths, N. M. and Dénes, H. and Dickey, J. M. and Stanimirović, S. and Staveley-Smith, L. and Jameson, Katherine and Di Teodoro, Enrico and Allison, James R. and Collier, J. D. and Chippendale, A. P. and Franzen, T. and Gürkan, Gülay and Heald, G. and Hotan, A. and Kleiner, D. and Lee-Waddell, K. and {McConnell}, D. and Popping, A. and Rhee, Jonghwan and Riseley, C. J. and Voronkov, M. A. and Whiting, M.},
	urldate = {2018-11-06},
	date = {2018-11-29},
	eprinttype = {arxiv},
	eprint = {1811.01772},
}

@article{Remolina-Gutierrez2018,
	title = {Lyman-\${\textbackslash}alpha\$ photons through rotating outflows},
	url = {http://arxiv.org/abs/1811.01266},
	abstract = {Outflows and rotation are two ubiquitous kinematic features in the gas kinematics of galaxies. Here we introduce a semi-analytic model to quantify how rotating outflows impact the morphology of the Lyman-\${\textbackslash}alpha\$ emission line. The model is contrasted against Monte Carlo radiative transfer simulations of outflowing gas with additional solid body rotation. We explore a range of neutral Hydrogen optical depth of \$10{\textasciicircum}5 {\textbackslash}leq {\textbackslash}tau\_\{{\textbackslash}mathrm\{H\}\} {\textbackslash}leq10{\textasciicircum}7\$, rotational velocity \$0 {\textbackslash}leq v\_\{{\textbackslash}mathrm\{rot\}\}/{\textbackslash}mathrm\{km{\textbackslash} s\}{\textasciicircum}\{-1\} {\textbackslash}leq 100\$ and outflow velocity \$0{\textbackslash}leq v\_\{{\textbackslash}mathrm\{out\}\}/{\textbackslash}mathrm\{km{\textbackslash} s\}{\textasciicircum}\{-1\} {\textbackslash}leq 50\$. We find three consequences of rotation. First, it introduces a dependency with viewing angle; second it broadens the line and third it increases the flux at the line's center. For all viewing angles, the semi-analytic model reproduces the radiative transfer results for the line width and flux change at the line's center within a \$7{\textbackslash}\%\$ and \$50{\textbackslash}\%\$ precision for an optical depth of \${\textbackslash}tau\_\{{\textbackslash}mathrm\{H\}\}=10{\textasciicircum}5\$, respectively, and within \$2{\textbackslash}\%\$ and \$1{\textbackslash}\%\$ for an optical depth of \${\textbackslash}tau\_\{{\textbackslash}mathrm\{H\}\}=10{\textasciicircum}7\$. Using this model we also show that the peaks of integrated spectra taken from opposite sides of an edge-on rotating gas distribution should have a separation of \${\textbackslash}frac\{1\}\{2\}v\_\{{\textbackslash}mathrm\{rot\}\}\$. The semi-analytic model presented here is a convenient tool to introduce rotational kinematics as a post-processing step of idealized Monte Carlo simulations; it provides a framework to interpret {\textbackslash}lya spectra in systems where rotation is expected or directly measured through kinematic maps.},
	author = {Remolina-Gutiérrez, Maria Camila and Forero-Romero, Jaime E.},
	urldate = {2018-11-06},
	date = {2018-11-03},
	eprinttype = {arxiv},
	eprint = {1811.01266},
}

@article{Coba2018,
	title = {Systematic study of outflows in the Local Universe using {CALIFA}: I. Sample selection and main 1. Coba, C. L. et al. Systematic study of outflows in the Local Universe using {CALIFA}: I. Sample selection and main properties. (2018). at {\textless}http://arxiv.org/abs/},
	url = {http://arxiv.org/abs/1811.01253},
	abstract = {We present a sample of 17 objects from the {CALIFA} survey where we find initial evidence of galactic winds based on their off-axis ionization properties. We identify the presence of outflows using various optical diagnostic diagrams (e.g., {EW}(H\${\textbackslash}alpha\$), [Nii]/H\${\textbackslash}alpha\$, [Sii]/H\${\textbackslash}alpha\$, [Oi]/H\${\textbackslash}alpha\$ line-ratio maps). We find that all 17 candidate outflow galaxies lie along the sequence of active star formation in the M\$\_{\textbackslash}star\$ vs. star-formation rate diagram, without a clear excess in the integrated {SFR}. The location of galaxies along the star-formation main sequence ({SFMS}) does not influence strongly the presence or not of outflows. The analysis of the star-formation rate density (\${\textbackslash}Sigma\_\{{\textbackslash}rm {SFR}\}\$) reveals that the {CALIFA} sources present higher values when compared with normal star-forming galaxies. The strength of this relation depends on the calibrator used to estimate the {SFR}. This excess in \${\textbackslash}Sigma\_\{{\textbackslash}rm {SFR}\}\$ is significant within the first effective radius supporting the idea that most outflows are driven by processes in the inner regions of a galaxy. We find that the molecular gas mass density (\${\textbackslash}Sigma\_{\textbackslash}mathrm\{gas\}\$) is a key parameter that plays an important role in the generation of outflows through its association with the local {SFR}. The canonical threshold reported for the generation of outflows -- \${\textbackslash}Sigma\_\{{\textbackslash}rm {SFR}\}{\textgreater}0.1\$ \${\textbackslash}mathrm\{M\}\_{\textbackslash}odot {\textbackslash}mathrm\{yr\}{\textasciicircum}\{-1\} {\textbackslash}mathrm\{kpc\}{\textasciicircum}\{-2\}\$ -- is only marginally exceeded in our sample. Within the Kennicutt-Schmidt diagram we propose a domain for galaxies hosting starburst-driven outflows defined by \${\textbackslash}Sigma\_\{{\textbackslash}rm {SFR}\}{\textgreater}10{\textasciicircum}\{-2\} {\textbackslash},{\textbackslash}mathrm\{M\}\_{\textbackslash}odot {\textbackslash}mathrm\{yr\}{\textasciicircum}\{-1\} {\textbackslash}mathrm\{kpc\}{\textasciicircum}\{-2\}\$ and \${\textbackslash}Sigma\_{\textbackslash}mathrm\{gas\}{\textgreater}10{\textasciicircum}\{1.2\} {\textbackslash}, {\textbackslash}mathrm\{M\}\_{\textbackslash}odot {\textbackslash}mathrm\{pc\}{\textasciicircum}\{-2\}\$ within a central kiloparcec region.},
	author = {Coba, Carlos Lopez and Sanchez, Sebastian F. and Bland-Hawthorn, Joss and Moissev, Alexei V. and Cruz-Gonzalez, Irene and Garcia-Benito, Ruben and Barrera-Ballesteros, Jorge K. and Galbany, Lluis},
	urldate = {2018-11-06},
	date = {2018-11-03},
	eprinttype = {arxiv},
	eprint = {1811.01253},
}

