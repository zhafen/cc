@article{Hasan2020,
	title = {Evolution of {CIV} Absorbers I. The Cosmic Incidence},
	url = {http://arxiv.org/abs/2007.11751},
	abstract = {We present a large high-resolution study of the distribution and evolution of {CIV} absorbers, including the weakest population with equivalent widths \$W\_r{\textless}0.3\${\textasciitilde}\{{\textbackslash}{AA}\}. By searching 369 high-resolution, high signal-to-noise spectra of quasars at \$1.1{\textbackslash}leq z\_\{em\} {\textbackslash}leq5.3\$ from Keck/{HIRES} and {VLT}/{UVES}, we find \$1318\$ {CIV} absorbers with \$W\_r {\textbackslash}geq 0.05\${\textasciitilde}\{{\textbackslash}{AA}\} (our \${\textbackslash}sim50{\textbackslash}\%\$ completeness limit) at redshifts \$1{\textbackslash}leq z {\textbackslash}leq4.75\$. A Schechter function describes the observed equivalent width distribution with a transition from power-law to exponential decline at \$W\_r {\textbackslash}gtrsim 0.5\${\textasciitilde}\{{\textbackslash}{AA}\}. The power-law slope \${\textbackslash}alpha\$ rises and normalization \$n\_\{{\textbackslash}star\}\$ falls by \${\textbackslash}sim20{\textbackslash}\%\$ from \$1{\textbackslash}leq z{\textless}2.5\$ to \$2.5{\textbackslash}leq z {\textbackslash}leq4.75\$. We find that the co-moving redshift path density, \${dN}/{dX}\$, of \$W\_r {\textbackslash}geq 0.05\${\textasciitilde}\{{\textbackslash}{AA}\} absorbers rises by \${\textbackslash}sim1.8\$ times from \$z=4.75\$ to \$z=1\$, while the increase in the \$W\_r {\textbackslash}geq 0.6\${\textasciitilde}\{{\textbackslash}{AA}\} \${dN}/{dX}\$ is \${\textbackslash}sim4.5\$ times as much. We quantify the observed evolution by a model in which \${dN}/{dX}\$ decreases linearly with redshift. The model suggests that populations with larger \$W\_r\$ thresholds evolve faster with redshift and appear later in the universe. The cosmological \{{\textbackslash}sc Technicolor Dawn\} simulations at \$3{\textbackslash}leq z {\textbackslash}leq5\$ over-produce the observed abundance of absorbers with \$W\_r{\textless}0.3\${\textasciitilde}\{{\textbackslash}{AA}\}, while yielding good agreement at higher \$W\_r\$. Our empirical linear model successfully describes {CIV} evolution in the simulations and the observed evolution of \$W\_r {\textbackslash}geq 0.6\${\textasciitilde}\{{\textbackslash}{AA}\} {CIV} for the past \${\textbackslash}sim12\$ Gyr. Combining our measurements with the literature gives us a picture of {CIV}-absorbing structures becoming more numerous and/or larger in physical size over the last \${\textbackslash}approx13\$ Gyr of cosmic time (\$z{\textbackslash}sim6\$ to \$z{\textbackslash}sim0\$).},
	author = {Hasan, Farhanul and Churchill, Christopher W. and Stemock, Bryson and Mathes, Nigel L. and Finlator, Kristian and Croom, Mark and Nielsen, Nikole M. and Kacprzak, Glenn G. and Murphy, Michael T.},
	urldate = {2020-07-24},
	date = {2020-07-22},
	eprinttype = {arxiv},
	eprint = {2007.11751},
}

@article{Lim2020,
	title = {Properties of the {CGM} and {IGM}: constraints on galaxy formation models from the Sunyaev-Zel'dovich effect},
	url = {http://arxiv.org/abs/2007.11583},
	abstract = {We present a comparison of the physical properties of the gas in the circumgalactic ({CGM}) and intergalactic ({IGM}) media at \$z{\textbackslash}sim0\$ between observations and four cosmological hydrodynamical simulations: Illustris, {TNG}300 of the {IllustrisTNG} project, {EAGLE}, and one of the Magneticum simulations. For the observational data, we use the gas properties that are inferred from cross-correlating the Sunyaev-Zel'dovich effect ({SZE}) from the \{{\textbackslash}it Planck\} {CMB} maps with the haloes and the large-scale structure reconstructed from Sloan Digital Sky Survey data. Both the observational and simulation results indicate that the integrated gas pressure in haloes deviates from the self-similar case, showing that feedback impacts haloes with \$M\_\{500\}{\textbackslash}sim 10{\textasciicircum}\{12-13\}{\textbackslash},\{{\textbackslash}rm M\_{\textbackslash}odot\}\$. The simulations predict that more than half the baryons are displaced from haloes, while the gas fraction inferred from our observational data roughly equals the cosmic baryon fraction throughout the \$M\_\{500\}{\textbackslash}sim 10{\textasciicircum}\{12-14.5\}{\textbackslash},\{{\textbackslash}rm M\_{\textbackslash}odot\}\$ halo mass range. All simulations tested here predict that the mean gas temperature in haloes is about the virial temperature, while that inferred from the {SZE} is up to one order of magnitude lower than that from the simulations (and also from X-ray observations). While a remarkable agreement is found for the average properties of the {IGM} between the observation and some simulations, we show that their dependence on the large-scale tidal field can break the degeneracy between models that show similar predictions otherwise. Finally, we show that the gas pressure and the electron density profiles from simulations are not well described by a generalized {NFW} ({GNFW}) profile. Instead, we present a new model with a mass-dependent shape that fits the profiles accurately.},
	author = {Lim, S. H. and Barnes, David and Vogelsberger, Mark and Mo, H. J. and Nelson, Dylan and Pillepich, Annalisa and Dolag, Klaus and Marinacci, Federico},
	urldate = {2020-07-24},
	date = {2020-07-22},
	eprinttype = {arxiv},
	eprint = {2007.11583},
}

@article{Kamdar2020,
	title = {Spatial and Kinematic Clustering of Stars in the Galactic Disk},
	url = {http://arxiv.org/abs/2007.10990},
	abstract = {The Galactic disk is expected to be spatially, kinematically, and chemically clustered on many scales due to both star formation and non-axisymmetries in the Galactic potential. In this work we calculate the spatial and kinematic two-point correlation functions using a sample of \$1.7 {\textbackslash}times 10{\textasciicircum}6\$ stars within 1 kpc of the Sun with 6D phase space information available from {\textbackslash}textit\{Gaia\} {DR}2. Clustering is detected on spatial scales of 1-300 pc and velocity scales of at least 15 km s\${\textasciicircum}\{-1\}\$. With bound structures included, the data have a power-law index (\${\textbackslash}xi({\textbackslash}Delta r) {\textbackslash}propto {\textbackslash}Delta r{\textasciicircum}\{{\textbackslash}gamma\}\$) of \${\textbackslash}gamma{\textbackslash}approx-2\$ at most spatial scales, which is in line with theoretical predictions. After removing bound structures, the data have a power-law index of \${\textbackslash}gamma{\textbackslash}approx-1\$ for \${\textless}100\$ pc and \${\textbackslash}gamma {\textbackslash}lesssim -1.5\$ for \${\textgreater}100\$ pc. We interpret these results with the aid of a novel star-by-star simulation of the Galaxy in which stars are born in clusters orbiting in a realistic potential that includes spiral arms, a bar, and {GMCs}. We find that the simulation largely agrees with the observations (within a factor of 2-3) at all spatial and kinematic scales. In detail, the correlation function in the simulation is shallower than the data at \${\textless} 20\$ pc scales, and steeper than the data at \${\textgreater} 30\$ pc scales. We also find a persistent clustering signal in the kinematic correlation function for the data at large \${\textbackslash}Delta v\$ (\${\textgreater}5\$ km s\${\textasciicircum}\{-1\}\$) not present in the simulations. We speculate that this mismatch between observations and simulations may be due to two processes not included in the simulation: hierarchical star formation and transient spiral arms. We also use the simulations to predict the clustering signal as a function of pair-wise metallicity and age separations. Ages and metallicities measured with a precision of \$50{\textbackslash}\%\$ and \$0.05\$ dex are required in order to enhance the clustering signal.},
	author = {Kamdar, Harshil and Conroy, Charlie and Ting, Yuan-Sen and El-Badry, Kareem},
	urldate = {2020-07-23},
	date = {2020-07-21},
	eprinttype = {arxiv},
	eprint = {2007.10990},
}

@article{Blana2020,
	title = {Dwarfs in the Milky Way halo outer rim: first in-fall or backsplash satellites?},
	url = {http://arxiv.org/abs/2007.11001},
	abstract = {Leo T is a gas-rich dwarf located at 414kpc \$(1.4R\_\{{\textbackslash}rm vir\})\$ distance from the Milky Way ({MW}) and it is currently assumed to be on its first approach. Here, we present an analysis of orbits calculated backward in time for the dwarf with our new code \{{\textbackslash}sc delorean\}, exploring a range of systematic uncertainties, e.g. {MW} virial mass and accretion, M31 potential, and cosmic expansion. We discover that orbits with tangential velocities in the Galactic Standard-of-Rest frame lower than \${\textbar}{\textbackslash}vec\{u\}\_\{{\textbackslash}rm t\}{\textasciicircum}\{{\textbackslash}rm {GSR}\}{\textbar}{\textbackslash}!{\textbackslash}leq{\textbackslash}! 63{\textasciicircum}\{+47\}\_\{-39\}\{{\textbackslash}rm{\textbackslash}, km{\textbackslash}, s{\textasciicircum}\{-1\}\}\$ result in backsplash solutions, i.e. orbits that entered and left the {MW} dark matter halo in the past, and that velocities above \${\textbar}{\textbackslash}vec\{u\}\_\{{\textbackslash}rm t\}{\textasciicircum}\{{\textbackslash}rm {GSR}\}{\textbar}{\textbackslash}!{\textbackslash}geq{\textbackslash}!21{\textasciicircum}\{+33\}\_\{-21\}\{{\textbackslash}rm{\textbackslash}, km{\textbackslash}, s{\textasciicircum}\{-1\}\}\$ result in wide orbit backsplash solutions with a minimum pericenter range of \$D\_\{{\textbackslash}rm min\}{\textbackslash}!{\textbackslash}geq{\textbackslash}!38{\textasciicircum}\{+26\}\_\{-16\}\{{\textbackslash}rm {\textbackslash},kpc\}\$, which would allow this satellite to survive gas stripping and tidal disruption. Moreover, new proper motion estimates match with our region of backsplash solutions. \%This suggests that Leo T is likely a pristine system. We applied our method to other distant {MW} satellites, finding a range of gas stripped backsplash solutions for the gas-less Cetus and Eridanus {II}, providing a possible explanation for their lack of cold gas, while only first in-fall solutions are found for the {HI} rich Phoenix I. We also find that the cosmic expansion can delay their first pericenter passage when compared to the non-expanding scenario. This study explores the provenance of these distant dwarfs and provides constraints on the environmental and internal processes that shaped their evolution and current properties.},
	author = {Blaña, Matias and Burkert, Andreas and Fellhauer, Michael and Schartmann, Marc and Alig, Christian},
	urldate = {2020-07-23},
	date = {2020-07-21},
	eprinttype = {arxiv},
	eprint = {2007.11001},
}

