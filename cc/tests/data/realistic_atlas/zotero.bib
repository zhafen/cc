@article{Dekel2020,
	title = {Origin of Star-Forming Rings around Massive Centres in Massive Galaxies at \$z{\textbackslash}!},
	url = {http://arxiv.org/abs/2003.08984},
	abstract = {Using analytic modeling and simulations, we address the origin of an abundance of star-forming, clumpy, extended gas rings about massive central bodies in massive galaxies at \$z{\textbackslash}! {\textless}{\textbackslash}! 4\$. Rings form by high-angular-momentum streams and may survive in galaxies of \$M\_\{{\textbackslash}rm star\} {\textbackslash}!{\textgreater}{\textbackslash}! 10{\textasciicircum}\{9.5\}{\textbackslash}, M\_{\textbackslash}odot\$ where merger-driven spin flips and supernova feedback are ineffective. The rings survive above this threshold mass after events of compaction to central nuggets. Ring longevity seemed to conflict with the expected inward mass transport driven by torques from violent disc instability. However, by evaluating the torques from a tightly wound spiral structure, we find that the timescale for transport per orbital time is \${\textbackslash}propto{\textbackslash}! {\textbackslash}delta\_\{{\textbackslash}rm d\}{\textasciicircum}\{-3\}\$, with \${\textbackslash}delta\_\{{\textbackslash}rm d\}\$ the cold-to-total mass ratio interior to the ring. A long-lived ring forms when the ring transport is slower than its replenishment by accretion and the interior depletion by {SFR}, both valid for \${\textbackslash}delta\_\{{\textbackslash}rm d\} {\textbackslash}!{\textless}{\textbackslash}! 0.3\$. The central mass that lowers \${\textbackslash}delta\_\{{\textbackslash}rm d\}\$ and maintains the ring is made of compaction-driven bulge and/or dark-matter, aided by the lower gas fraction at lower \$z\$. The ring could be Toomre unstable for clump and star formation. Mock images of simulated rings through dust indicate qualitative consistency with observed rings about bulges in massive \$z{\textbackslash}!{\textbackslash}sim{\textbackslash}!0.5{\textbackslash}!-{\textbackslash}!3\$ galaxies, in \$H\_\{{\textbackslash}alpha\}\$ and in deep {HST} imaging. {ALMA} mock images indicate that \$z{\textbackslash}!{\textbackslash}sim{\textbackslash}!0.5{\textbackslash}!-{\textbackslash}!1\$ rings should be detectable. We quote expected observable properties and abundances of rings and their central blue or red nuggets for comparison to observations.},
	author = {Dekel, Avishai and Lapiner, Sharon and Ginzburg, Omri and Freundlich, Jonathan and Jiang, Fangzhou and Lin, Doug and Ceverino, Daniel and Primack, Joel and Giavalisco, Mauro and Ji, Zhiyuan},
	urldate = {2020-03-23},
	date = {2020-03-19},
	eprinttype = {arxiv},
	eprint = {2003.08984},
}

@article{Fielding2020,
	title = {Multiphase Gas and the Fractal Nature of Radiative Turbulent Mixing Layers},
	url = {http://arxiv.org/abs/2003.08390},
	abstract = {A common situation in galactic and intergalactic gas involves cold dense gas in motion relative to hot diffuse gas. Kelvin-Helmholtz instability creates a turbulent mixing layer and populates the intermediate-temperature phase, which often cools rapidly. The energy lost to cooling is balanced by the advection of hot high enthalpy gas into the mixing layer, resulting in growth and acceleration of the cold phase. This process may play a major role in determining the interstellar medium and circumgalactic medium phase structure, and accelerating cold gas in galactic winds and cosmic filaments. Cooling in these mixing layers occurs in a thin corrugated sheet, which we argue has an area with fractal dimension \$D=5/2\$ and a thickness that adjusts to match the hot phase mixing time to the cooling time. These cooling sheet properties form the basis of a new model for how the cooling rate and hot gas inflow velocity depend on the size \$L\$, cooling time \$t\_\{{\textbackslash}rm cool\}\$, relative velocity \$v\_\{{\textbackslash}rm rel\}\$, and density contrast \${\textbackslash}rho\_\{{\textbackslash}rm cold\}/{\textbackslash}rho\_\{{\textbackslash}rm hot\}\$ of the system. Entrainment is expected to be enhanced in environments with short \$t\_\{{\textbackslash}rm cool\}\$, large \$v\_\{{\textbackslash}rm rel\}\$, and large \${\textbackslash}rho\_\{{\textbackslash}rm cold\}/{\textbackslash}rho\_\{{\textbackslash}rm hot\}\$. Using a large suite of three dimensional hydrodynamic simulations, we demonstrate that this fractal cooling layer model accurately captures the energetics and evolution of turbulent interfaces and can therefore be used as a foundation for understanding multiphase mixing with strong radiative cooling.},
	author = {Fielding, Drummond B. and Ostriker, Eve C. and Bryan, Greg L. and Jermyn, Adam S.},
	urldate = {2020-03-20},
	date = {2020-03-18},
	eprinttype = {arxiv},
	eprint = {2003.08390},
}

