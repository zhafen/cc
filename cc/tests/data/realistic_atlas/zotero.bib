@article{Burchett2018,
	title = {Warm-hot gas in X-ray bright galaxy clusters and the H i-deficient circumgalactic medium in dense environments},
	volume = {475},
	issn = {0035-8711},
	url = {https://academic.oup.com/mnras/article/475/2/2067/4712238},
	doi = {10.1093/mnras/stx3170},
	abstract = {We analyze the intracluster medium ({ICM}) and circumgalactic medium ({CGM}) in 7 X-ray detected galaxy clusters using spectra of background {QSOs} ({HST}-{COS}/{STIS}), optical spectroscopy of the cluster galaxies ({MMT}/Hectospec and {SDSS}), and X-ray imaging/spectroscopy ({XMM}-Newton and Chandra). First, we report a very low covering fraction of H I absorption in the {CGM} of these cluster galaxies, f\_c = 25\% +25\%/-15\%, to stringent detection limits (log N(H I) {\textless} 13 cm{\textasciicircum}-2). As field galaxies have an H I covering fraction of {\textasciitilde}100\% at similar radii, the dearth of {CGM} H I in our data indicates that the cluster environment has effectively stripped or overionized the gaseous halos of these cluster galaxies. Second, we assess the contribution of warm-hot (10{\textasciicircum}5 - 10{\textasciicircum}6 K) gas to the {ICM} as traced by O {VI} and broad Ly-alpha ({BLA}) absorption. Despite the high signal-to-noise of our data, we do not detect O {VI} in any cluster, and we only detect {BLA} features in the {QSO} spectrum probing one cluster. We estimate that the total column density of warm-hot gas along this line of sight totals to {\textasciitilde}3\% of that contained in the hot T {\textgreater} 10{\textasciicircum}7 K X-ray emitting phase. Residing at high relative velocities, these features may trace pre-shocked material outside the cluster. Comparing gaseous galaxy halos from the low-density 'field' to galaxy groups and high-density clusters, we find that the {CGM} is progressively depleted of H I with increasing environmental density, and the {CGM} is most severely transformed in galaxy clusters. This {CGM} transformation may play a key role in environmental galaxy quenching.},
	pages = {2067--2085},
	number = {2},
	journaltitle = {Monthly Notices of the Royal Astronomical Society},
	author = {Burchett, Joseph N. and Tripp, Todd M. and Wang, Q Daniel and Willmer, Christopher N A and Bowen, David V. and Jenkins, Edward B.},
	date = {2018-04-01},
	eprinttype = {arxiv},
	eprint = {1705.05892},
	keywords = {Galaxies: evolution, Galaxies: haloes, Quasars: absorption lines, X-rays: galaxies: clusters, Galaxies: clusters: intracluster medium, Galaxies: groups: general},
}

@article{Faerman2017,
	title = {{MASSIVE} {WARM}/{HOT} {GALAXY} {CORONAE} {AS} {PROBED} {BY} {UV}/X-{RAY} {OXYGEN} {ABSORPTION} {AND} {EMISSION}. I. {BASIC} {MODEL}},
	volume = {835},
	issn = {1538-4357},
	url = {http://stacks.iop.org/0004-637X/835/i=1/a=52?key=crossref.474d1453ad52647b99cae2df72c7a830},
	doi = {10.3847/1538-4357/835/1/52},
	abstract = {We construct an analytic phenomenological model for extended warm/hot gaseous coronae of \$L\_*\$ galaxies. We consider {UV} {OVI} {COS}-Halos absorption line data in combination with Milky Way X-ray {OVII} and {OVIII} absorption and emission. We fit these data with a single model representing the {COS}-Halos galaxies and a Galactic corona. Our model is multi-phased, with hot and warm gas components, each with a (turbulent) log-normal distribution of temperatures and densities. The hot gas, traced by the X-ray absorption and emission, is in hydrostatic equilibrium in a Milky Way gravitational potential. The median temperature of the hot gas is \$1.5 {\textbackslash}times 10{\textasciicircum}6\${\textasciitilde}K and the mean hydrogen density is \${\textbackslash}sim 5 {\textbackslash}times 10{\textasciicircum}\{-5\}{\textasciitilde}\{{\textbackslash}rm cm{\textasciicircum}\{-3\}\}\$. The warm component as traced by the {OVI}, is gas that has cooled out of the high density tail of the hot component. The total warm/hot gas mass is high and is \$1.2 {\textbackslash}times 10{\textasciicircum}\{11\}{\textasciitilde}\{{\textbackslash}rm M\_\{{\textbackslash}odot\}\}\$. The gas metallicity we require to reproduce the oxygen ion column densities is \$0.5\$ solar. The warm {OVI} component has a short cooling time (\${\textbackslash}sim 2 {\textbackslash}times 10{\textasciicircum}8\$ years), as hinted by observations. The hot component, however, is \${\textbackslash}sim 80{\textbackslash}\%\$ of the total gas mass and is relatively long-lived, with \$t\_\{cool\} {\textbackslash}sim 7 {\textbackslash}times 10{\textasciicircum}\{9\}\$ years. Our model supports suggestions that hot galactic coronae can contain significant amounts of gas. These reservoirs may enable galaxies to continue forming stars steadily for long periods of time and account for "missing baryons" in galaxies in the local universe.},
	pages = {52},
	number = {1},
	journaltitle = {The Astrophysical Journal},
	author = {Faerman, Yakov and Sternberg, Amiel and {McKee}, Christopher F.},
	date = {2017-01-18},
	eprinttype = {arxiv},
	eprint = {1602.00689},
	note = {Publisher: {IOP} Publishing},
	keywords = {evolution, formation, galaxies, halos, quasars, absorption lines, galaxies: formation, intergalactic medium, galaxy, quasars: absorption lines, supporting material, galaxies: halos, Galaxy: formation, data behind fi gure, Galaxy: evolution},
}

@incollection{VandeVoort2016a,
	title = {The Effect of Galactic Feedback on Gas Accretion and Wind Recycling},
	url = {http://arxiv.org/abs/1612.00591%0Ahttp://dx.doi.org/10.1007/978-3-319-52512-9_13},
	abstract = {In the absence of galactic winds, the rate at which gas accretes onto galaxies is determined by the gravitational potential and by radiative cooling. However, outflows driven by supernovae and active galactic nuclei not only eject gas from galaxies, but also prevent gas from accreting in the first place. Furthermore, gas previously ejected from a galaxy can re-accrete onto (the same or a different) galaxy. Because this gas has a high metallicity, its cooling rate is relatively high, which will increase its chances to re-accrete. This complex interplay between gas inflows and outflows is discussed in this chapter. Wind recycling is found to be an important process that fuels galaxies at late times and the recycled gas has different properties than gas accreting for the first time. Quantitative conclusions, however, vary between studies, because the amount of wind recycling is dependent on the details of the feedback model. We discuss these differences, known caveats, and ways to make progress in understanding how galaxies are fed at low redshift.},
	pages = {301--321},
	author = {van de Voort, Freeke},
	date = {2017},
	eprinttype = {arxiv},
	eprint = {1612.00591},
	doi = {10.1007/978-3-319-52512-9_13},
}

@article{Bordoloi2017,
	title = {On the {CGM} Fundamental Plane: The Halo Mass Dependency of Circumgalactic {HI}},
	url = {http://arxiv.org/abs/1712.02348},
	abstract = {We analyze the equivalent widths of {HI} Ly-\${\textbackslash}alpha\$ (\$W\_\{Ly{\textbackslash}alpha\}\$) from the inner (R {\textless} 160 kpc) circumgalactic medium ({CGM}) of 85 galaxies at \$z {\textbackslash}sim 0\$ with stellar masses \$M*\$ ranging \$8 {\textbackslash}leq log M* / M\_\{{\textbackslash}odot\} {\textbackslash}leq 11.6\$. Across three orders of magnitude in stellar mass, the {CGM} of present-day galaxies exhibits a very high covering fraction of cool hydrogen gas (\$f\_C = 86.6 {\textbackslash}pm 3.6\$\%) indicating that the {CGM} is ubiquitous in modern, isolated galaxies. These same galaxies show a decline in \$W\_\{Ly{\textbackslash}alpha\}\$ with increasing radius, independent of mass, but the scatter in this trend correlates closely with \$M*\$. Using the radial and stellar mass correlations, we construct a fundamental plane describing the cool {CGM} of modern galaxies: \${\textbackslash}log W{\textasciicircum}\{s\}\_\{{HI} 1215\} {\textbackslash}; = {\textbackslash}; (0.34 {\textbackslash}pm 0.02) -( 0.0026 {\textbackslash}pm 0.0005)*(R) + (0.286 {\textbackslash}pm 0.002)* {\textbackslash}log (M\_\{*\}/M\_\{{\textbackslash}odot\})\$. The {RMS} scatter around this bivariate relation is \${\textbackslash}sim\$0.2 dex. We interpret the explicit correlation between \$W\_\{Ly{\textbackslash}alpha\}\$ and \$M*\$ to arise from the underlying dark matter halo, i.e., \$W\_\{Ly{\textbackslash}alpha\}\$ traces the gravitational potential whereas \$M*\$ tracks \$M\_\{halo\}\$.},
	author = {Bordoloi, Rongmon and Prochaska, J. Xavier and Tumlinson, Jason and Werk, Jessica K. and Tripp, Todd M. and Burchett, Joseph N.},
	urldate = {2018-07-17},
	date = {2017-12-06},
	eprinttype = {arxiv},
	eprint = {1712.02348},
}

@article{Rahmati2018,
	title = {The metallicity distribution of H i systems in the {EAGLE} cosmological simulations},
	volume = {476},
	issn = {0035-8711},
	url = {https://academic.oup.com/mnras/article/476/4/4865/4924513},
	doi = {10.1093/mnras/sty610},
	abstract = {The metallicity of strong {HI} systems, spanning from damped Lyman-alpha absorbers ({DLAs}) to Lyman-limit systems ({LLSs}) is explored between z = 5-{\textgreater}0 using the {EAGLE} high-resolution cosmological hydrodynamic simulation of galaxy formation. The metallicities of {LLSs} and {DLAs} steadily increase with time in agreement with observations. {DLAs} are more metal rich than {LLSs}, although the metallicities in the {LLS} column density range ({NHI} = 10{\textasciicircum}17 -10{\textasciicircum}20 cm{\textasciicircum}-2) are relatively flat, evolving from a median {HI}-weighted metallicity of Z{\textless}10{\textasciicircum}-2 Zsol at z = 3 to {\textasciitilde}10{\textasciicircum}-0.5 Zsol by z = 0. The metal content of {HI} systems tracks the increasing stellar content of the Universe, holding {\textasciitilde}5\% of the integrated total metals released from stars at z = 0. We also consider partial {LLS} ({pLLS}, {NHI} = 10{\textasciicircum}16-10{\textasciicircum}17 cm{\textasciicircum}-2) metallicities, and find good agreement with Wotta et al. (2016) for the fraction of systems above (37\%) and below (63\%) 0.1 Zsol. We also find a large dispersion of {pLLS} metallicities, although we do not reproduce the observed metallicity bimodality and instead we make the prediction that a larger sample will yield more {pLLSs} around 0.1 Zsol. We under-predict the median metallicity of strong {LLSs}, and predict a population of Z {\textless} 10{\textasciicircum}-3 Zsol {DLAs} at z {\textgreater} 3 that are not observed, which may indicate more widespread early enrichment in the real Universe compared to {EAGLE}.},
	pages = {4865--4871},
	number = {4},
	journaltitle = {Monthly Notices of the Royal Astronomical Society},
	author = {Rahmati, Alireza and Oppenheimer, Benjamin D.},
	date = {2018-06-01},
	eprinttype = {arxiv},
	eprint = {1712.03988},
	keywords = {Cosmology: theory, Galaxies: evolution, Galaxies: formation, Intergalactic medium, Quasars: absorption lines, Methods: numerical},
}

@article{Goldbaum2018,
	title = {unyt: Handle, manipulate, and convert data with units in Python},
	volume = {3},
	issn = {2475-9066},
	url = {http://arxiv.org/abs/1806.02417},
	doi = {10.21105/joss.00809},
	abstract = {Software that processes real-world data or that models a physical system must have some way of managing units. While simple approaches like the understood convention that all data are in a unit system (such as the {MKS} {SI} unit system) do work in practice, they are fraught with possible sources of error both by developers and users of the software. In this paper we present unyt, a Python library based on {NumPy} and {SymPy} for handling data that has units. It is designed both to aid quick interactive calculations and to be tightly integrated into a larger Python application or library. We compare unyt with two other Python libraries for handling units, Pint and astropy.units, and find that unyt is faster, has higher test coverage, and has fewer lines of code.},
	pages = {809},
	number = {28},
	journaltitle = {Journal of Open Source Software},
	author = {Goldbaum, Nathan J. and {ZuHone}, John A. and Turk, Matthew J. and Kowalik, Kacper and Rosen, Anna L.},
	urldate = {2018-07-16},
	date = {2018-06-06},
	eprinttype = {arxiv},
	eprint = {1806.02417},
	keywords = {Astrophysics, Instrumentation and Methods for Astrophysics},
}

@article{Leitherer1999,
	title = {Starburst99: Synthesis Models for Galaxies with Active Star Formation},
