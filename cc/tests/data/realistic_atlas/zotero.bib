@article{campitielloGASPXXXIVUnfolding2021,
	title = {{GASP} {XXXIV}: Unfolding the thermal side of ram pressure stripping in the jellyfish galaxy {JO}201},
	url = {http://arxiv.org/abs/2103.03147},
	abstract = {X-ray studies of jellyfish galaxies play a crucial role in understanding the interactions between the interstellar medium ({ISM}) and the intracluster medium ({ICM}). In this paper, we focused on the jellyfish galaxy {JO}201. By combining archival Chandra observations, {MUSE} H\${\textbackslash}alpha\$ cubes, and maps of the emission fraction of the diffuse ionised gas, we investigated both its high energy spectral properties and the spatial correlation between its X-ray and optical emissions. The X-ray emission of {JO}201 is provided by both the Compton thick {AGN} (L\$\_\{{\textbackslash}text\{X\}\}{\textasciicircum}\{0.5-10 {\textbackslash}text\{{keV}\}\}\$=2.7\${\textbackslash}cdot\$10\${\textasciicircum}\{41\}\$ erg s\${\textasciicircum}\{-1\}\$, not corrected for intrinsic absorption) and an extended component (L\$\_\{{\textbackslash}text\{X\}\}{\textasciicircum}\{0.5-10 {\textbackslash}, {\textbackslash}text\{{keV}\}\}{\textbackslash}approx\$1.9-4.5\${\textbackslash}cdot\$10\${\textasciicircum}\{41\}\$ erg s\${\textasciicircum}\{-1\}\$) produced by a warm plasma ({kT}\${\textbackslash}approx\$1 {keV}), whose luminosity is higher than expected from the observed star formation (L\$\_\{{\textbackslash}text\{X\}\}{\textbackslash}sim\$3.8\${\textbackslash}cdot10{\textasciicircum}\{40\}\$ erg s\${\textasciicircum}\{-1\}\$). The spectral analysis showed that the X-ray emission is consistent with the thermal cooling of hot plasma. These properties are similar to the ones found in other jellyfish galaxies showing extended X-ray emission. A point-to-point analysis revealed that this X-ray emission closely follows the {ISM} distribution, whereas {CLOUDY} simulations proved that the ionisation triggered by this warm plasma would be able to reproduce the [{OI}]/H\${\textbackslash}alpha\$ excess observed in {JO}201. We conclude that the galactic X-ray emitting plasma is originated on the surface of the {ISM} as a result of the {ICM}-{ISM} interplay. This process would entail the cooling and accretion of the {ICM} onto the galaxy, which could additionally fuel the star formation, and the emergence of [{OI}]/H\${\textbackslash}alpha\$ excess in the optical spectrum.},
	author = {Campitiello, M. G. and Ignesti, A. and Gitti, M. and Brighenti, F. and Radovich, M. and Wolter, A. and Tomicic, N. and Bellhouse, C. and Poggianti, B. M. and Moretti, A. and Vulcani, B. and Jaffè, Y. L. and Paladino, R. and Muller, A. and Fritz, J. and Lourenco, A. C. C. and Gullieuszik, M.},
	urldate = {2021-03-12},
	date = {2021-03-04},
	eprinttype = {arxiv},
	eprint = {2103.03147},
}

