@article{Torrey2016,
	title = {An instability of feedback regulated star formation in galactic nuclei},
	volume = {000},
	url = {http://arxiv.org/abs/1601.07186},
	abstract = {We examine the stability of feedback-regulated star formation ({SF}) in galactic nuclei and contrast it to {SF} in extended discs. In galactic nuclei the dynamical time becomes shorter than the time over which feedback from young stars evolves. We argue analytically that the balance between stellar feedback and gravity is unstable in this regime. We study this using numerical simulations with pc-scale resolution and explicit stellar feedback taken from stellar evolution models. The nuclear gas mass, young stellar mass, and {SFR} within the central {\textasciitilde}100 pc (the short-timescale regime) never reach steady-state, but instead go through dramatic, oscillatory cycles. Stars form until a critical surface density of young stars is present (such that feedback overwhelms gravity), at which point they begin to expel gas from the nucleus. Since the dynamical times are shorter than the stellar evolution times, the stars do not die as the gas is expelled, but continue to push, triggering a runaway quenching of star formation in the nucleus. However the expelled gas is largely not unbound from the galaxy, but goes into a galactic fountain which re-fills the nuclear region after the massive stars from the previous burst cycle have died off ({\textasciitilde}50 Myr timescale). On large scales ({\textgreater}1 kpc), the galaxy-scale gas content and {SFR} is more stable. We examine the consequences of this episodic nuclear star formation for the Kennicutt-Schmidt ({KS}) relation: while a tight {KS} relation exists on {\textasciitilde}1 kpc scales in good agreement with observations, the scatter increases dramatically in smaller apertures centered on galactic nuclei.},
	issue = {January},
	journaltitle = {{arXiv} eprint},
	author = {Torrey, Paul and Hopkins, Philip F. and Faucher-Giguère, Claude-André and Vogelsberger, Mark and Quataert, Eliot and Kereš, Dušan and Murray, Norman},
	date = {2016},
	eprinttype = {arxiv},
	eprint = {1601.07186},
	keywords = {Galaxies: evolution, Galaxies: formation, {FIRE} paper, Galaxies: high-redshift, Stars: formation, Methods: numerical, galaxies: {ISM}, theory: simulations, galactic nuclei, galaxies: starburst, stellar feedback},
}

@article{Faucher-Giguere2016,
	title = {A stellar feedback origin for neutral hydrogen in high-redshift quasar-mass haloes},
	volume = {461},
	issn = {1745-3925},
	url = {http://arxiv.org/abs/1601.07188},
	doi = {10.1093/mnrasl/slw091},
	abstract = {Observations of quasar pairs reveal that quasar host halos at z{\textasciitilde}2 have large covering fractions of cool dense gas ({\textgreater}{\textasciitilde}60\% for Lyman limit systems within a projected virial radius). Most simulations have so far failed to explain these large observed covering fractions. We analyze a new set of 15 simulated massive halos with explicit stellar feedback from the {FIRE} project, covering the halo mass range M\_h{\textasciitilde}2x10{\textasciicircum}12-10{\textasciicircum}13 Msun at z=2. This extends our previous analysis of the circum-galactic medium of high-redshift galaxies to more massive halos. Feedback from active galactic nuclei ({AGN}) is not included in these simulations. We find covering fractions consistent with those observed around z{\textasciitilde}2 quasars. The large {HI} covering fractions arise from star formation-driven galactic winds, including winds from low-mass satellite galaxies that interact with the cosmological infalling filaments in which they are typically embedded. The simulated covering fractions increase with both halo mass and redshift over the ranges covered, as well as with resolution. Our simulations predict that galaxies occupying dark matter halos of mass similar to quasars but without a luminous {AGN} should have Lyman limit system covering fractions comparable to quasars. This prediction can be tested by measuring covering fractions transverse to sub-millimeter galaxies or to more quiescent galaxies selected based on their high stellar mass.},
	pages = {L32--L36},
	number = {1},
	journaltitle = {Monthly Notices of the Royal Astronomical Society: Letters},
	author = {Faucher-Giguère, Claude-André and Feldmann, Robert and Quataert, Eliot and Kereš, Dušan and Hopkins, Philip F. and Murray, Norman},
	date = {2016-09-01},
	eprinttype = {arxiv},
	eprint = {1601.07188},
	keywords = {H I, {FIRE} paper, galaxies: evolution, galaxies: formation, intergalactic medium, Quasars: absorption lines, haloes, absorption, theory: simulations, cosmology: theory},
}

