	note = {{ISBN}: bibcode:2006NuPhA.777..424N},
	keywords = {Abundances, Nucleosynthesis, Population {III} stars, Supernovae},
}

@article{Keres2009b,
	title = {{SEEDING} {THE} {FORMATION} {OF} {COLD} {GASEOUS} {CLOUDS} {IN} {MILKY} {WAY}-{SIZE} {HALOS}},
	volume = {700},
	issn = {0004-637X},
	url = {http://stacks.iop.org/1538-4357/700/i=1/a=L1?key=crossref.a4f5902a2c5cc05ae694b5a659774512},
	doi = {10.1088/0004-637X/700/1/L1},
	abstract = {We use one of the highest resolution cosmological smoothed particle{\textbackslash}nhydrodynamic simulations to date to demonstrate that cold gaseous clouds{\textbackslash}nform around Milky Way-size galaxies. We further explore mechanisms{\textbackslash}nresponsible for their formation and show that a large fraction of clouds{\textbackslash}noriginate as a consequence of late-time filamentary "cold mode"{\textbackslash}naccretion. Here, filaments that are still colder and denser than the{\textbackslash}nsurrounding halo gas are not able to connect directly to galaxies, as{\textbackslash}nthey do at high redshift, but are instead susceptible to the combined{\textbackslash}naction of cooling and Rayleigh-Taylor instabilities at intermediate{\textbackslash}nradii within the halo leading to the production of cold, dense{\textbackslash}npressure-confined clouds, without an associated dark matter component.{\textbackslash}{nThis} process is aided through the compression of the incoming filaments{\textbackslash}nby the hot halo gas and expanding shocks during the halo buildup. Our{\textbackslash}nmechanism directly seeds clouds from gas with substantial local{\textbackslash}noverdensity, unlike in previous models, and provides a channel for the{\textbackslash}norigin of cloud complexes. These clouds can later "rain" onto galaxies,{\textbackslash}ndelivering fuel for star formation. Owing to the relatively large{\textbackslash}ncross-section of filaments and the net angular momentum carried by the{\textbackslash}ngas, the clouds will be distributed in a modestly flattened region{\textbackslash}naround a galaxy.},
	pages = {L1--L5},
	number = {1},
	journaltitle = {The Astrophysical Journal},
	author = {Kereš, Dušan and Hernquist, Lars},
	date = {2009-07-20},
	eprinttype = {arxiv},
	eprint = {0905.2186},
	keywords = {Galaxies, Numerical, Formation - galaxy, Formation - instabilities - methods, Halo - galaxy},
}

