	keywords = {Absorption, Galaxies: evolution, Galaxies: formation, H I, {LLS}, Radiative transfer, ★, Include: {LLS} Paper, Intergalactic medium, Galaxies: high-redshift, Methods: numerical, Radiative Transfer},
	file = {PDF:/Users/zhafen/Zotero/storage/6VD6F4MP/Rahmati et al. - 2013 - On the evolution of the HI column density distribution in cosmological simulations.pdf:application/pdf},
}

@article{Rahmati2013a,
