@article{Falkendal2020,
	title = {{ALMA} and {MUSE} observations reveal a quiescent multi-phase circumgalactic medium around the z{\textasciitilde}3.6 radio galaxy 4C 19.71},
	url = {http://arxiv.org/abs/2007.10061},
	abstract = {We present {MUSE}/{VLT} imaging spectroscopy of rest-frame ultraviolet emission lines and {ALMA} observations of the [{CI}] \${\textasciicircum}3\$P\$\_1\$-\${\textasciicircum}3\$P\$\_0\$ emission line, probing both the ionized and diffuse molecular medium around the radio galaxy 4C 19.71 at z{\textasciitilde}3.6. This radio galaxy has extended Ly\${\textbackslash}alpha\$ emission over a region {\textasciitilde}100 kpc in size preferentially oriented along the axis of the radio jet. Faint Ly\${\textbackslash}alpha\$ emission extends beyond the radio hot spots. We also find extended {CIV} and {HeII} emission over a region of {\textasciitilde}150 kpc in size, where the most distant emission lies {\textasciitilde}40 kpc beyond the north radio lobe and has narrow {FWHM} line widths of {\textasciitilde}180 km/s and a small relative velocity offset \${\textbackslash}Delta\$v{\textasciitilde}130 km/s from the systemic redshift of the radio galaxy. The [{CI}] is detected in the same region with {FWHM}{\textasciitilde}100 km/s and \${\textbackslash}Delta\$v{\textasciitilde}5 km/s, while [{CI}] is not detected in the regions south of the radio galaxy. We interpret the coincidence in the northern line emission as evidence of relatively quiescent multi-phase gas residing within the halo at a projected distance of {\textasciitilde}75 kpc from the host galaxy. To test this hypothesis, we performed photoionization and photo-dissociated region ({PDR}) modeling, using the code Cloudy, of the three emission line regions: the radio galaxy proper and the northern and southern regions. We find that the [{CI}]/{CIV}\${\textbackslash}lambda{\textbackslash}lambda\$1548,1551 and {CIV}\${\textbackslash}lambda{\textbackslash}lambda\$1548,1551/{HeII} ratios of the two halo regions are consistent with a {PDR} or ionization front in the circumgalactic medium likely energized by photons from the active galactic nuclei. This modeling is consistent with a relatively low metallicity, 0.03{\textless}[Z/Z\$\_\{{\textbackslash}odot\}\$]{\textless}0.1, and diffuse ionization with an ionization parameter (proportional to the ratio of the photon number density and gas density) of log U{\textasciitilde}3 for the two circumgalactic line emission regions. Using rough mass estimates for the molecular [...]},
	author = {Falkendal, Theresa and Lehnert, Matthew D. and Vernet, Joël and De Breuck, Carlos and Wang, Wuji},
	urldate = {2020-07-21},
	date = {2020-07-20},
	eprinttype = {arxiv},
