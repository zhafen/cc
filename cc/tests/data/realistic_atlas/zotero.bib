@article{Kovacs2018,
	title = {Detection of the Missing Baryons toward the Sightline of H1821+643},
	volume = {872},
	issn = {1538-4357},
	url = {http://arxiv.org/abs/1812.04625%0Ahttp://dx.doi.org/10.3847/1538-4357/aaef78},
	doi = {10.3847/1538-4357/aaef78},
	abstract = {Based on constraints from Big Bang nucleosynthesis and the cosmic microwave background, the baryon content of the high-redshift Universe can be precisely determined. However, at low redshift, about one-third of the baryons remain unaccounted for, which poses the long-standing missing baryon problem. The missing baryons are believed to reside in large-scale filaments in the form of warm-hot intergalactic medium ({WHIM}). In this work, we employ a novel stacking approach to explore the hot phases of the {WHIM}. Specifically, we utilize the 470 ks Chandra {LETG} data of the luminous quasar, H1821+643, along with previous measurements of {UV} absorption line systems and spectroscopic redshift measurements of galaxies toward the quasar's sightline. We repeatedly blueshift and stack the X-ray spectrum of the quasar corresponding to the redshifts of the 17 absorption line systems. Thus, we obtain a stacked spectrum with \$8.0\$ Ms total exposure, which allows us to probe X-ray absorption lines with unparalleled sensitivity. Based on the stacked data, we detect an {OVII} absorption line that exhibits a Gaussian line profile and is statistically significant at the \$3.3 {\textbackslash}sigma\$ level. Since the redshifts of the {UV} absorption line systems were known a priori, this is the first definitive detection of an X-ray absorption line originating from the {WHIM}. The equivalent width of the {OVII} line is \$(4.1{\textbackslash}pm1.3) {\textbackslash} {\textbackslash}mathrm\{m{\textbackslash}{AA}\}\$, which corresponds to an {OVII} column density of \$(1.4{\textbackslash}pm0.4){\textbackslash}times10{\textasciicircum}\{15\} {\textbackslash} {\textbackslash}mathrm\{cm{\textasciicircum}\{-2\}\}\$. We constrain the absorbing gas to have a density of \$n\_\{{\textbackslash}rm H\} = (1-2){\textbackslash}times10{\textasciicircum}\{-6\} {\textbackslash} {\textbackslash}rm\{cm{\textasciicircum}\{-3\}\}\$ for a single {WHIM} filament. We derive \${\textbackslash}Omega\_\{{\textbackslash}rm b\} {\textbackslash}rm(O{\textbackslash},{VII}) = (0.0023 {\textbackslash}pm 0.0007) {\textbackslash}, {\textbackslash}left[ f\_\{O{\textbackslash},{VII}\} {\textbackslash}, \{Z/Z\_\{{\textbackslash}odot\}\} {\textbackslash}right]{\textasciicircum}\{-1\}\$ for the cosmological mass density of {OVII}, assuming that all 17 systems contribute equally.},
	pages = {83},
	number = {1},
	journaltitle = {The Astrophysical Journal},
	author = {Kovács, Orsolya E. and Bogdán, Ákos and Smith, Randall K. and Kraft, Ralph P. and Forman, William R.},
	date = {2019-02-13},
	eprinttype = {arxiv},
	eprint = {1812.04625},
	note = {Publisher: {IOP} Publishing},
	keywords = {galaxies, quasars, absorption lines, ism, cosmology, observations, quasars: absorption lines, active, individual, cosmology: observations, galaxies: active, x-rays, 643, h 1821, quasars: individual (H 1821+643), X-rays: {ISM}},
}

