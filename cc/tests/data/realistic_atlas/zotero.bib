@article{Wilkinson2016,
	title = {Ruling out the light {WIMP} explanation of the galactic 511 {keV} line},
	url = {http://arxiv.org/abs/1602.01114},
	abstract = {Over the past few decades, an anomalous 511 {keV} gamma-ray line has been observed from the centre of the Milky Way. Dark matter ({DM}) in the form of light ({\textless} 10 {MeV}) {WIMPs} annihilating into electron-positron pairs has been one of the leading hypotheses of the observed emission. Here we show that this explanation is ruled out by the latest cosmological data, suggesting an astrophysical or more exotic {DM} source of the signal.},
	pages = {1--6},
	author = {Wilkinson, Ryan J. and Vincent, Aaron C. and Boehm, Celine and {McCabe}, Christopher},
	date = {2016},
	eprinttype = {arxiv},
	eprint = {1602.01114},
	keywords = {dark matter, gamma rays, galactic center},
}

@article{Brown2016,
	title = {A Recalibration of Strong Line Oxygen Abundance Diagnostics via the Direct Method and Implications for the High Redshift Universe},
	volume = {21},
	url = {http://arxiv.org/abs/1602.01087},
	abstract = {We use direct method oxygen abundances in combination with strong optical emission lines, stellar masses (\$M\_\{{\textbackslash}star\}\$), and star formation rates ({SFRs}) to recalibrate the N2, O3N2, and N2O2 oxygen abundance diagnostics. We stack spectra of \${\textbackslash}sim\$200,000 star-forming galaxies from the Sloan Digital Sky Survey in bins of \$M\_\{{\textbackslash}star\}\$ and {SFR} offset from the star forming main sequence to measure the weak emission lines needed to apply the direct method. All three new calibrations are reliable to within \${\textbackslash}pm 0.10\$ dex from \${\textbackslash}log(M\_\{{\textbackslash}star\}/M\_\{{\textbackslash}odot\}) {\textbackslash}sim 7.5 - 10.5\$ and up to at least \$200{\textasciitilde}M\_\{{\textbackslash}odot\}\$ yr\${\textasciicircum}\{-1\}\$ in {SFR}. The N2O2 diagnostic is the least subject to systematic biases. We apply the diagnostics to galaxies in the local universe and investigate the \$M\_\{{\textbackslash}star\}\$-\$Z\$-\$\{{\textbackslash}rm {SFR}\}\$ relation. The N2 and O3N2 diagnostics suggest the {SFR} dependence of the \$M\_\{{\textbackslash}star\}\$-\$Z\$-\$\{{\textbackslash}rm {SFR}\}\$ relation varies with both \$M\_\{{\textbackslash}star\}\$ and \${\textbackslash}Delta {\textbackslash}log({SSFR})\$, whereas the N2O2 diagnostic suggests a nearly constant dependence on {SFR}. We apply our calibrations to a sample of high redshift galaxies from the literature, and find them to be metal poor relative to local galaxies with similar \$M\_\{{\textbackslash}star\}\$ and {SFR}. The calibrations do reproduce direct method abundances of the local analogs. We conclude that the \$M\_\{{\textbackslash}star\}\$-\$Z\$-\$\{{\textbackslash}rm {SFR}\}\$ relation evolves with redshift.},
	pages = {1--21},
	issue = {February},
	author = {Brown, Jonathan S. and Martini, Paul and Andrews, Brett H.},
	date = {2016},
	eprinttype = {arxiv},
	eprint = {1602.01087},
	keywords = {galaxies: evolution, galaxies: {ISM}, high redshift, galaxies: abundance, galaxies: active, {ISM}: abundances, observational: archival survey, Oxygen abundance},
}

@article{Gu2016,
	title = {Hierarchical Galaxy Growth and Scatter in the Stellar Mass - Halo Mass Relation},
	url = {http://arxiv.org/abs/1602.01099},
	abstract = {The relation between galaxies and dark matter halos reflects the combined effects of many distinct physical processes. Observations indicate that the \$z=0\$ stellar mass-halo mass ({SMHM}) relation has remarkably small scatter in stellar mass at fixed halo mass (\${\textbackslash}lesssim\$ 0.2 dex) with little dependence on halo mass. We investigate the origins of this scatter by combining N-body simulations with observational constraints on the {SMHM} relation. We find that at the group and cluster scale (\$M\_\{{\textbackslash}rm vir\}{\textgreater}10{\textasciicircum}\{14\}\{M\_{\textbackslash}odot\}\$) the scatter due purely to hierarchical assembly is \${\textbackslash}approx0.16\$ dex, which is comparable to recent direct observational estimates. At lower masses, mass buildup since \$z{\textbackslash}approx2\$ is driven largely by in-situ growth. We include a model for the in-situ buildup of stellar mass and find that an intrinsic scatter in this growth channel of \$0.2\$ dex produces a relation between scatter and halo mass that is consistent with observations from \$10{\textasciicircum}\{12\}\{M\_{\textbackslash}odot\}{\textless}M\_\{{\textbackslash}rm vir\}{\textless}10{\textasciicircum}\{14.75\}\{M\_{\textbackslash}odot\}\$. The approximately constant scatter across a wide range of halo masses at \$z=0\$ thus appears to be a coincidence as it is determined largely by in-situ growth at low masses and by hierarchical assembly at high masses. These results indicate that the scatter in the {SMHM} relation can provide unique insight into the regularity of the galaxy formation process.},
	author = {Gu, Meng and Conroy, Charlie and Behroozi, Peter},
	date = {2016},
	eprinttype = {arxiv},
	eprint = {1602.01099},
	keywords = {galaxies: evolution, theory: simulations, galaxies: halos, semi-analytic, stellar mass-halo mass relation},
}

@article{Forrest2016,
	title = {{UV} to {IR} Luminosities and Dust Attenuation Determined from {\textasciitilde}4000 K-Selected Galaxies at 1{\textless}z{\textless}3 in the {ZFOURGE} Survey},
	url = {http://arxiv.org/abs/1602.01096},
	abstract = {We build a set of composite galaxy {SEDs} by de-redshifting and scaling multi-wavelength photometry from galaxies in the {ZFOURGE} survey, covering the {CDFS}, {COSMOS}, and {UDS} fields. From a sample of {\textasciitilde}4000 K\_s-band selected galaxies, we define 38 composite galaxy {SEDs} that yield continuous low-resolution spectra (R{\textasciitilde}45) over the rest-frame range 0.1-4 um. Additionally, we include far infrared photometry from the Spitzer Space Telescope and the Herschel Space Observatory to characterize the infrared properties of our diverse set of composite {SEDs}. From these composite {SEDs} we analyze the rest-frame {UVJ} colors, as well as the ratio of {IR} to {UV} light ({IRX}) and the {UV} slope (\${\textbackslash}beta\$) in the {IRX}\$-{\textbackslash}beta\$ dust relation at 1{\textless}z{\textless}3. Blue star-forming composite {SEDs} show {IRX} and \${\textbackslash}beta\$ values consistent with local relations; dusty star-forming galaxies have considerable scatter, as found for local {IR} bright sources, but on average appear bluer than expected for their {IR} fluxes. We measure a tight linear relation between rest-frame {UVJ} colors and dust attenuation for star-forming composites, providing a direct method for estimating dust content from either (U-V) or (V-J) rest-frame colors for star-forming galaxies at intermediate redshifts.},
	author = {Forrest, Ben and Tran, Kim-Vy H. and Tomczak, Adam R. and Broussard, Adam and Labbé, Ivo and Papovich, Casey and Kriek, Mariska and Allen, Rebecca J. and Cowley, Michael and Dickinson, Mark and Glazebrook, Karl and van Houdt, Josha and Inami, Hanae and Kacprzak, Glenn G. and Kawinwanichakij, Lalitwadee and Kelson, Daniel and {McCarthy}, Patrick J. and Monson, Andrew and Morrison, Glenn and Nanayakkara, Themiya and Persson, S. Eric and Quadri, Ryan F. and Spitler, Lee R. and Straatman, Caroline and Tilvi, Vithal},
	date = {2016},
	eprinttype = {arxiv},
	eprint = {1602.01096},
	keywords = {observational: survey, galaxies: high-redshift, ultraviolet: galaxies, galaxies: star formation, infrared: galaxies, peak cosmic star formation},
}

@article{Shukla2016,
	title = {The Effects of Lyman-Limit Systems on the Evolution and Observability of the Epoch of Reionization},
	issn = {0035-8711},
	url = {http://arxiv.org/abs/1602.01144},
	doi = {10.1093/mnras/stw249},
	abstract = {We present the first large-scale, full radiative transfer simulations of the reionization of the intergalactic medium in the presence of Lyman-limit systems ({LLSs}). To illustrate the impact of {LLS} opacity, possibly missed by previous simulations, we add either a uniform or spatially-varying hydrogen bound-free opacity. This opacity, implemented as the mean free path (mfp) of the ionizing photons, extrapolates the observed, post-reionization redshift dependence into the epoch of reionization. In qualitative agreement with previous studies, we find that at late times the presence of {LLSs} slows down the ionization fronts, and alters the size distribution of H {II} regions. We quantitatively characterize the size distribution and morphological evolution of H {II} regions and examine the effects of the {LLSs} on the redshifted 21-cm signal from the patchy reionization. The presence of {LLSs} extends the ionization history by \${\textbackslash}Delta z {\textbackslash}sim 0.8\$. The {LLS} absorbers significantly impede the late-time growth of the H {II} regions. The position dependent {LLS} distribution slows reionization further and additionally limits the late growth of the ionized regions. However, there is no "freeze out" of the H {II} regions and the largest regions grow to the size of the simulation volume. The 21-cm power spectra show that at large scales the power drops by a factor of 2 for 50\% and 75\% ionization stages (at \$k = 0.1\$ \${\textbackslash}text\{h\} {\textbackslash}, {\textbackslash}text\{Mpc\}{\textasciicircum}\{-1\} \$) reflecting the limiting effect of the {LLSs} on the growth of ionized patches. The statistical observables such as the {RMS} of the brightness temperature fluctuations and the peak amplitudes of the 21-cm power spectra at large-scales (\$k = 0.05 - 0.1\$ \${\textbackslash}text\{h\} {\textbackslash}, {\textbackslash}text\{Mpc\}{\textasciicircum}\{-1\} \$) are diminished by the presence of {LLS}.},
	issue = {February},
	journaltitle = {{arXiv} eprint},
	author = {Shukla, Hemant and Mellema, Garrelt and Iliev, Ilian T. and Shapiro, Paul R.},
	date = {2016},
	eprinttype = {arxiv},
	eprint = {1602.01144},
	keywords = {H I, {LLS}, intergalactic medium, Cosmology, Methods: numerical, high redshift, theory: simulations, epoch of reionization, Epoch of Reionization:  theory, H {II} regions: Lyman-limit Systems: High-redshift, radiative transfer},
}

