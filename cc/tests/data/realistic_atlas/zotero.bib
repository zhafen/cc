@article{Somerville2017,
	title = {The relationship between galaxy and dark matter halo size from z ∼ 3 to the present},
	volume = {473},
	issn = {0035-8711},
	url = {http://arxiv.org/abs/1701.03526},
	doi = {10.1093/mnras/stx2040},
	abstract = {We explore empirical constraints on the statistical relationship between the radial size of galaxies and the radius of their host dark matter halos from \$z{\textbackslash}sim 0.1\$--3 using the {GAMA} and {CANDELS} surveys. We map dark matter halo mass to galaxy stellar mass using relationships from abundance matching, applied to the Bolshoi-Planck dissipationless N-body simulation. We define {SRHR}\${\textbackslash}equiv r\_e/R\_h\$ as the ratio of galaxy radius to halo virial radius, and {SRHR}\${\textbackslash}lambda {\textbackslash}equiv r\_e/({\textbackslash}lambda R\_h)\$ as the ratio of galaxy radius to halo spin parameter times halo radius. At \$z{\textbackslash}sim 0.1\$, we find an average value of {SRHR} \${\textbackslash}simeq 0.018\$ and {SRHR}\${\textbackslash}lambda {\textbackslash}simeq 0.5\$ with very little dependence on stellar mass. {SRHR} and {SRHR}\${\textbackslash}lambda\$ have a weak dependence on cosmic time since \$z{\textbackslash}sim 3\$. {SRHR} shows a mild decrease over cosmic time for low mass galaxies, but increases slightly or does not evolve for more massive galaxies. We find hints that at high redshift (\$z{\textbackslash}sim 2\$--3), {SRHR}\${\textbackslash}lambda\$ is lower for more massive galaxies, while it shows no significant dependence on stellar mass at \$z{\textbackslash}lesssim 0.5\$. We find that for both the {GAMA} and {CANDELS} samples, at all redshifts from \$z{\textbackslash}sim 0.1\$--3, the observed conditional size distribution in stellar mass bins is remarkably similar to the conditional distribution of \${\textbackslash}lambda R\_h\$. We discuss the physical interpretation and implications of these results.},
	pages = {2714--2736},
	number = {2},
	journaltitle = {Monthly Notices of the Royal Astronomical Society},
	author = {Somerville, Rachel S. and Behroozi, Peter and Pandya, Viraj and Dekel, Avishai and Faber, S. M. and Fontana, Adriano and Koekemoer, Anton M. and Koo, David C. and Pérez-González, P. G. and Primack, Joel R. and Santini, Paola and Taylor, Edward N. and van der Wel, Arjen},
	date = {2018-01-11},
	eprinttype = {arxiv},
	eprint = {1701.03526},
	keywords = {galaxies, structure, high redshift, - galaxies, evolution - galaxies, formation - galaxies},
}

@article{Mathes2017,
	title = {The Vulture Survey I: Analyzing the Evolution of \$\{{\textbackslash}{MgII}\}\$ Absorbers},
	url = {http://arxiv.org/abs/1701.05624},
	abstract = {We present detailed measurements of the redshift path density, equivalent width distribution, column density distribution, and redshift evolution of \$\{{\textbackslash}{MgII}\}\$ absorbers as measured in archival spectra from the {UVES} spectrograph at the Very Large Telescope ({VLT}/{UVES}) and the {HIRES} spectrograph at the Keck Telescope (Keck/{HIRES}) to equivalent width detection limits below \$0.01\${\textasciitilde}\{{\textbackslash}{AA}\}. This survey examines 432 {VLT}/{UVES} spectra from the {UVES} {SQUAD} collaboration and 170 Keck/{HIRES} spectra from the {KODIAQ} group, allowing for detections of intervening \$\{{\textbackslash}{MgII}\}\$ absorbers spanning redshifts \$0.1 {\textless} z {\textless} 2.6\$. We employ an accurate, automated approach to line detection which consistently detects redshifted absorption lines. We measure the equivalent widths, apparent optical depth column densities, and velocity widths for each absorbing system. Using our complete sample of all detectable \$\{{\textbackslash}{MgII}\}\$ absorbers, we can accurately determine the redshift path density of absorbers across cosmic time. We measure evolution in the comoving \$\{{\textbackslash}{MgII}\}\$ line density, \${dN}{\textbackslash},/{dX}\$, finding more high equivalent width absorbers at \$z = 2\$ than at present. We also measure evolution in the equivalent width distribution, parameterized by a Schechter function fit, finding a shallower weak-end slope for absorbers at redshifts between \$1.53 {\textless} z {\textless} 2.64\$ as compared to lower redshifts. Finally, we calculate the cosmic mass fraction of \$\{{\textbackslash}{MgII}\}\$ using the column density distribution. We find that weak \$\{{\textbackslash}{MgII}\}\$ absorbers, those with equivalent widths less than \$0.3\${\textasciitilde}\{{\textbackslash}{AA}\}, are physically distinct and evolve separately from very strong \$\{{\textbackslash}{MgII}\}\$ absorbers, which have equivalent widths greater than \$1.0\${\textasciitilde}\{{\textbackslash}{AA}\}.},
	pages = {15},
	author = {Mathes, Nigel L. and Churchill, Christopher W. and Murphy, Michael T.},
	urldate = {2017-01-23},
	date = {2017-01-19},
	eprinttype = {arxiv},
	eprint = {1701.05624},
	note = {Genre: Galaxy Astrophysics},
	keywords = {Absorption, High Redshift, To Read: High Priority, Mg {II}},
}

