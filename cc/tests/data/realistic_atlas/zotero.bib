@article{Singh2020,
	title = {Constraints on precipitation-limited hot halos from massive galaxies to galaxy clusters},
	url = {http://arxiv.org/abs/2008.04917},
	abstract = {We present constraints on a simple analytical model for hot diffuse halo gas, derived from a fit spanning two orders of magnitude in halo mass (\$M\_\{500\} {\textbackslash}sim 10{\textasciicircum}\{12.5\}-10{\textasciicircum}\{14.5\} M\_\{{\textbackslash}odot\}\$). The model is motivated by the observed prevalence of a precipitation limit, and its main free parameter is the central ratio of gas cooling timescale to free-fall timescale (\$t\_\{{\textbackslash}rm cool\}/t\_\{{\textbackslash}rm ff\}\$). Using stacked X-ray and thermal Sunyaev-Zel'dovich observations of the environments around massive galaxies, galaxy groups and clusters, we obtain the best-fitting model parameters. We find \$t\_\{{\textbackslash}rm cool\}/t\_\{{\textbackslash}rm ff\} {\textbackslash}sim 50-110\$, depending on the model extrapolation beyond the halo virial radius and possibly on biases present in the data-sets used in the fitting analysis. The model adequately describes the entire mass range, except for intermediate mass halos (\$M\_\{500\} {\textbackslash}sim 10{\textasciicircum}\{13.5\} M\_\{{\textbackslash}odot\}\$) which systematically fall below the model predictions. However, the best fits for \$t\_\{{\textbackslash}rm cool\}/t\_\{{\textbackslash}rm ff\}\$ substantially exceed the values typically derived from X-ray observations of individual systems (\$t\_\{{\textbackslash}rm cool\}/t\_\{{\textbackslash}rm ff\} {\textbackslash}sim 10-30\$). We consider several explanations for those discrepancies, including X-ray selection biases and a potential anti-correlation between X-ray luminosity and the central galaxy's stellar mass.},
	author = {Singh, Priyanka and Voit, G. M. and Nath, Biman B.},
	urldate = {2020-08-13},
	date = {2020-08-11},
	eprinttype = {arxiv},
	eprint = {2008.04917},
}

