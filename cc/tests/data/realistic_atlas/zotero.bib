	eprint = {1709.03999},
	file = {PDF:/Users/zhafen/Zotero/storage/KVMUDQSJ/Muzahid et al. - 2017 - COS-Weak Probing the CGM using analogs of weak Mg II absorbers at z 0.3.pdf:application/pdf},
