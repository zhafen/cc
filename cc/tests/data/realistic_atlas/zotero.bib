@article{Prochaska2013a,
	title = {{QUASARS} {PROBING} {QUASARS}. {VI}. {EXCESS} H I {ABSORPTION} {WITHIN} {ONE} {PROPER} Mpc {OF} z ∼ 2 {QUASARS}},
	volume = {776},
	issn = {0004-637X},
	url = {https://iopscience.iop.org/article/10.1088/0004-637X/776/2/136},
	doi = {10.1088/0004-637X/776/2/136},
	abstract = {With close pairs of quasars at different redshifts, a background quasar sightline can be used to study a foreground quasar's environment in absorption. We use a sample of 650 projected quasar pairs to study the H I Lyα absorption transverse to luminous, z ∼ 2 quasars at proper separations of 30 kpc {\textless} R ⊥ {\textless} 1 Mpc. In contrast to measurements along the line-of-sight, regions transverse to quasars exhibit enhanced H I Lyα absorption and a larger variance than the ambient intergalactic medium, with increasing absorption and variance toward smaller scales. Analysis of composite spectra reveals excess absorption characterized by a Lyα equivalent width profile W = 2.3 Å (R ⊥/100 kpc)-0.46. We also observe a high (≃ 60\%) covering factor of strong, optically thick H I absorbers (H I column ) at separations R ⊥ {\textless} 200 kpc, which decreases to ∼20\% at R ⊥ ≃ 1 Mpc, but still represents a significant excess over the cosmic average. This excess of optically thick absorption can be described by a quasar-absorber cross-correlation function ξ{QA}(r) = (r/r 0)γ with a large correlation length (comoving) and . The H I absorption measured around quasars exceeds that of any previously studied population, consistent with quasars being hosted by massive dark matter halos M halo ≈ 1012.5 M⊙ at z ∼ 2.5. The environments of these massive halos are highly biased toward producing optically thick gas, and may even dominate the cosmic abundance of Lyman limit systems and hence the intergalactic opacity to ionizing photons at z ∼ 2.5. The anisotropic absorption around quasars implies the transverse direction is much less likely to be illuminated by ionizing radiation than the line-of-sight. © 2013. The American Astronomical Society. All rights reserved..},
	pages = {136},
	number = {2},
	journaltitle = {The Astrophysical Journal},
	author = {Prochaska, J. Xavier and Hennawi, Joseph F. and Lee, Khee-Gan and Cantalupo, Sebastiano and Bovy, Jo and Djorgovski, S. G. and Ellison, Sara L. and Lau, Marie Wingyee and Martin, Crystal L. and Myers, Adam and Rubin, Kate H. R. and Simcoe, Robert A.},
	date = {2013-10-07},
	keywords = {quasars: absorption lines, galaxies: halos},
}

@article{Font-Ribera2013,
	title = {The large-scale quasar-Lyman α forest cross-correlation from {BOSS}},
	volume = {2013},
	issn = {1475-7516},
	url = {http://arxiv.org/abs/1303.1937},
	doi = {10.1088/1475-7516/2013/05/018},
	abstract = {We measure the large-scale cross-correlation of quasars with the Lyman {\textbackslash}alpha{\textbackslash} forest absorption in redshift space, using {\textasciitilde} 60000 quasar spectra from Data Release 9 ({DR}9) of the Baryon Oscillation Spectroscopic Survey ({BOSS}). The cross-correlation is detected over a wide range of scales, up to comoving separations r of 80 Mpc/h. For r {\textgreater} 15 Mpc/h, we show that the cross-correlation is well fitted by the linear theory prediction for the mean overdensity around a quasar host halo in the standard {\textbackslash}Lambda {CDM} model, with the redshift distortions indicative of gravitational evolution detected at high confidence. Using previous determinations of the Lyman {\textbackslash}alpha{\textbackslash} forest bias factor obtained from the Lyman {\textbackslash}alpha{\textbackslash} autocorrelation, we infer the quasar bias factor to be b\_q = 3.64{\textasciicircum}+0.13\_-0.15 at a mean redshift z=2.38, in agreement with previous measurements from the quasar auto-correlation. We also obtain a new estimate of the Lyman {\textbackslash}alpha{\textbackslash} forest redshift distortion factor, {\textbackslash}beta\_F = 1.1 +/- 0.15, slightly larger than but consistent with the previous measurement from the Lyman {\textbackslash}alpha{\textbackslash} forest autocorrelation. The simple linear model we use fails at separations r {\textless} 15 Mpc/h, and we show that this may reasonably be due to the enhanced ionization due to radiation from the quasars. We also provide the expected correction that the mass overdensity around the quasar implies for measurements of the ionizing radiation background from the line-of-sight proximity effect.},
	pages = {018--018},
	number = {5},
	journaltitle = {Journal of Cosmology and Astroparticle Physics},
	author = {Font-Ribera, Andreu and Arnau, Eduard and Miralda-Escudé, Jordi and Rollinde, Emmanuel and Brinkmann, J. and Brownstein, Joel R. and Lee, Khee-Gan and Myers, Adam D. and Palanque-Delabrouille, Nathalie and Pâris, Isabelle and Petitjean, Patrick and Rich, James and Ross, Nicholas P. and Schneider, Donald P. and White, Martin},
	urldate = {2020-09-11},
	date = {2013-05-17},
	eprinttype = {arxiv},
	eprint = {1303.1937},
}

@article{Bouche2004a,
	title = {The Clustering of Galaxies around Three z ∼ 3 Damped Lyα Absorbers},
	volume = {609},
	issn = {0004-637X},
	url = {http://arxiv.org/abs/astro-ph/0403544},
	doi = {10.1086/421319},
	abstract = {[Abridged] We present out results on the cross-correlation of Lyman break galaxies ({LBGs}) around three damped {\textbackslash}Ly absorbers ({DLAs}) at redshift three from deep multi-band {MOSAIC} images obtained at the {KPNO} 4m. The large area of the {MOSAIC} images, 0.31 \${\textbackslash}deg{\textasciicircum}2\$ allows us to probe the clustering of {LBGs} on scales up to 20 Mpc co-moving to test whether {DLA} halos are more or less massive than {LBG} halos. . We present our survey that covers a total of 1 deg\${\textasciicircum}2\$ and contains \${\textbackslash}sim\$3,000 {LBGs} with photometric redshifts between 2.8 and 3.5. Within a redshift slice of width \$W\_z=0.15\$, we find that the {DLA}-{LBG} cross-correlation is \$1.6{\textbackslash}pm 1.3\$ times the {LBG}-{LBG} autocorrelation. This corresponds to a correlation length of \$r\_o=5{\textbackslash}pm4.5h{\textasciicircum}\{-1\}\$ (co-moving). The cross-correlation is most significant on scales 5-10 Mpc, and is significantly greater than zero at the \${\textgreater}95\$\% level from Monte Carlo simulations.},
	pages = {513--524},
	number = {2},
	journaltitle = {The Astrophysical Journal},
	author = {Bouche, Nicolas and Lowenthal, James D.},
	urldate = {2020-09-11},
	date = {2004-07-10},
	eprinttype = {arxiv},
	eprint = {astro-ph/0403544},
}

@article{Bouche2004,
	title = {The clustering of luminous red galaxies around Mg ii absorbers},
	volume = {354},
	issn = {00358711},
	url = {http://arxiv.org/abs/astro-ph/0408468},
	doi = {10.1111/j.1365-2966.2004.08348.x},
	abstract = {We study the cross-correlation between 212 {MgII} quasar absorption systems and {\textbackslash}{\textasciitilde}20,000 Luminous Red Galaxies ({LRGs}) selected from the Sloan Digital Sky Survey Data Release 1 in the redshift range 0.4{\textless}z{\textless}0.8. The {MgII} systems were selected to have 2796 \& 2803 rest-frame equivalent widths {\textgreater}=1.0 Angstrom and identifications confirmed by the {FeII} 2600 or {MgI} 2852 lines. Over comoving scales 0.05--13 h{\textasciicircum}-1 Mpc, the {MgII}--{LRG} cross-correlation has an amplitude 0.69+/-0.09 times that of the {LRG}--{LRG} auto-correlation. Since {LRGs} have halo-masses greater than 3.5 x 10{\textasciicircum}12 solar masses for M\_R{\textless}-21, this relative amplitude implies that the absorber host-galaxies have halo-masses greater than 2--8 x 10{\textasciicircum}11 Msun. For 10{\textasciicircum}13 Msun {LRGs}, the absorber host-galaxies have halo-masses 0.5--2.5 x 10{\textasciicircum}12 Msun. Our results appear consistent with those of Steidel et al. (1994) who found that {MgII} absorbers with W\_r{\textgreater}=0.3 Angstrom are associated with {\textasciitilde}0.7 L{\textasciicircum}*\_B galaxies.},
	pages = {L25--L29},
	number = {2},
	journaltitle = {Monthly Notices of the Royal Astronomical Society},
	author = {Bouché, Nicolas and Murphy, Michael T. and Péroux, Céline},
	urldate = {2020-09-11},
	date = {2004-10-25},
	eprinttype = {arxiv},
	eprint = {astro-ph/0408468},
}

